/*
** Cadence Tool Registry - vi
*/

Tool vi {
Read_Command = -exec cdsvitool -args "-title %Lib.%Cell:%View -- -R %FullPath";
Edit_Command = -exec cdsvitool -args "-title %Lib.%Cell:%View -- %FullPath";
File_ReadCommand = -exec cdsvitool -args "-R %FullPath";
File_EditCommand = -exec cdsvitool -args "%FullPath";
}
