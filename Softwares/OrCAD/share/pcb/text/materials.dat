DIELECTRIC=3.5
LOSS_TANGENT=0.009
THICKNESS=8.0mil

# polytetrafluoroethylene composite
MATERIAL_NAME PTFE
E_CONDUCTIVITY=0
DIELECTRIC=2.7
LOSS_TANGENT=0.003
THICKNESS=5.0mil


# flex circuit dielectric materials

# unreinforced polyimide film
MATERIAL_NAME POLYIMIDE_FILM
