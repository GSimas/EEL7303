FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 33 6
HEADER "Convert Corner to Arc"


TILE
TEXT "Maximum Radius:"
TLOC 1 1
ENDTEXT

TEXT "Minimum Radius:"
TLOC 1 3
ENDTEXT

TEXT "Number Executions:"
TLOC 1 5
ENDTEXT

FIELD MAXIMUM_RADIUS
FLOC 20 1
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD MINIMUM_RADIUS
FLOC 20 3
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD NUMBER_EXECUTIONS
FLOC 20 5
INTFILLIN 3 3
MIN 1
MAX 999
ENDFIELD

FIELD Done
FLOC 1 8
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD help
FLOC 23 8
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
