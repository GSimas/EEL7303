BFILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 70 16 
HEADER "Advanced Simulator Preferences"

ACTIVEFIELD "Close"

TILE

TEXT "Hspice Command"
TLOC 2 3 
FGROUP "Hspice Advanced Preferences"
ENDTEXT

FIELD HspiceCommand
FLOC 20 3 
STRFILLIN 48 500
FGROUP "Hspice Advanced Preferences"
ENDFIELD

TEXT ".TRAN options:"
TLOC 2 6
FGROUP "Hspice Advanced Preferences"
ENDTEXT

TEXT "START = "
TLOC 20 6
FGROUP "Hspice Advanced Preferences"
ENDTEXT

FIELD HspiceTranStart
FLOC 29 6
STRFILLIN 5 25
FGROUP "Hspice Advanced Preferences"
ENDFIELD

FIELD HspiceTranUIC
FLOC 40 9
CHECKLIST " Use Initial Condition"
FGROUP "Hspice Advanced Preferences"
ENDFIELD

TEXT "W Element Minimum Lenght"
TLOC 2 9
FGROUP "Hspice Advanced Preferences"
ENDTEXT

FIELD HspiceWminlen
FLOC 29 9
STRFILLIN 5 25
FGROUP "Hspice Advanced Preferences"
ENDFIELD

FIELD HspiceOptions
FLOC 20 12 
MENUBUTTON "Set Hspice Options..." 25 3
FGROUP "Hspice Advanced Preferences"
ENDFIELD

GROUP "Hspice Advanced Preferences"
GLOC 1 0
GSIZE 70 15
ENDGROUP

FIELD Close
FLOC 4 16
MENUBUTTON "Close" 11 3
ENDFIELD


FIELD help
FLOC 58 16
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
