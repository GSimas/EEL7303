FILE_TYPE=FORM_DEFN VERSION=2
FORM
VARIABLE
PORT 36 12
HEADER "Net Layer Rule"


POPUP <MODP>"Edit""1","Delete""2","Copy""3".


TILE StdTile
TPANEL 2
FIELD done
FLOC 1 1
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD help
FLOC 11 1
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD cancel
FLOC 24 1
MENUBUTTON "Cancel" 9 3
ENDFIELD

ENDTILE

TILE TEXT_TILE
TPANEL 0
TEXT "Net Name"
TLOC 6 0
ENDTEXT

TEXT "# Nets Using"
TLOC 21 0
ENDTEXT

ENDTILE

TILE RULE_TILE
TPANEL 1
TEXT
TLOC 6 0
INFO rule_name 18
ENDTEXT

TEXT
TLOC 22 0
INFO count 6
ENDTEXT

FIELD modify_rule
FLOC 1 0
MENUBUTTON "" 4 2
POP "MODP"
ENDFIELD

ENDTILE

ENDFORM
