FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 42 22
HEADER "Component Options"

POPUP <MultipleChoiceList> "HDL" "HDL","SCALD" "SCALD"
POPUP <BGAChoiceList> "" "".

TILE

TEXT "BGA:"
TLOC 1 1
ENDTEXT
FIELD output_symbol
FLOC 15 1
ENUMSET 23
POP "BGAChoiceList"
ENDFIELD


TEXT "Output format:"
TLOC 1 4
ENDTEXT
FIELD output_file_name
FLOC 15 4
ENUMSET 23
POP "MultipleChoiceList"
ENDFIELD

TEXT "Library name:"
TLOC 1 6
ENDTEXT
FIELD output_lib_name
FLOC 15 6
STRFILLIN 24 256
ENDFIELD

FIELD output_flattened
FLOC 3 8
CHECKLIST "Export flattened hierarchy"
ENDFIELD

TEXT "Delay report options:"
TLOC 1 11
ENDTEXT

FIELD delay_time
FLOC 3 13
CHECKLIST "Time delay report"
ENDFIELD

FIELD delay_length
FLOC 3 15
CHECKLIST "Length delay report"
ENDFIELD

FIELD ok
FLOC 1 18
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD cancel
FLOC 11 18
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 32 18
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
