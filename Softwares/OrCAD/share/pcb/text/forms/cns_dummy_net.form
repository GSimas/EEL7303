FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 60 20
HEADER "Assign Dummy Nets by Pin to NetClasses"

POPUP <Spacing> """".
POPUP <Physical> """".

TILE

GRID grid
FLOC 2 1
FSIZE 58 20
OPTIONS HLINES VLINES USERSIZE 

GHEAD TOP 
HEADSIZE 2
OPTIONS 3D MULTI
ENDGHEAD

ENDGRID

TEXT "New netclass:"
TLOC 4 22
ENDTEXT

FIELD netclass
FLOC 15 22
STRFILLIN 16 255
ENDFIELD

FIELD spacing_domain
FLOC 32 22
CHECKLIST "Spacing"
ENDFIELD

FIELD physical_domain
FLOC 41 22
CHECKLIST "Physical"
ENDFIELD

FIELD done
FLOC 2 25
MENUBUTTON "Ok" 10 3
ENDFIELD

FIELD cancel
FLOC 14 25
MENUBUTTON "Cancel" 10 3
ENDFIELD

FLEXMODE edgegravity
FLEX grid 0 0 1 1

ENDTILE

ENDFORM
