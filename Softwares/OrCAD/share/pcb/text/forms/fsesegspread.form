FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 28 20
HEADER "Edge Spread"

POPUP <class>"class""class".
POPUP <subclass>"subclass""subclass".

TILE

TEXT "Active Class and Subclass:"
TLOC 1 1
ENDTEXT

FIELD class
FLOC 5 4
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "class"
ENDFIELD

FIELD subcolor
FLOC 2 7
COLOR 2 1
OPTIONS colorchooser
ENDFIELD

FIELD subclass
FLOC 5 7
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

FIELD mousepick
FLOC 1 11
CHECKLIST "Spread by mouse" "OPModes"
ENDFIELD

FIELD accurateinput
FLOC 1 13
CHECKLIST "Spread by accurate offsets" "OPModes"
ENDFIELD

TEXT "Offset:"
TLOC 3 15
ENDTEXT

FIELD offset
FLOC 9 15
REALFILLIN 12 128
ENDFIELD

FIELD reference
FLOC 1 18
CHECKLIST "Use reference edge"
ENDFIELD

ENDTILE

ENDFORM

