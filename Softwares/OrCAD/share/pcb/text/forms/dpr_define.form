FILE_TYPE=FORM_DEFN VERSION=2
FORM
VARIABLE
PORT 60 15
HEADER "Define Property"

POPUP <data_type> "STRING""20","INTEGER""18","REAL""19","DESIGN_UNITS""17",\
"BOOLEAN""16","ALTITUDE""1","CAPACITANCE""2","DISTANCE""3",\
"ELEC_CONDUCTIVITY""4","FAILURE_RATE""5","LAYER_THICKNESS""6","IMPEDANCE""7",\
