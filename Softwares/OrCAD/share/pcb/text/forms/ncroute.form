FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 43 6
HEADER "NCRoute"

MESSAGE nerror 0 "A problem occurred - please check the logfile (using Viewlog)."
MESSAGE ndone 0 "NCRoute complete."
MESSAGE ndemo 0 "Cannot Run in Demo Mode."

ACTIVEFIELD "cancel"

TILE
TEXT "Output File Name (Optional):"
TLOC 2 3
ENDTEXT

FIELD filename
FLOC 2 5
STRFILLIN 18 128
ENDFIELD

FIELD execute
FLOC 32 1
MENUBUTTON "Generate" 9 3
ENDFIELD

FIELD cancel
FLOC 32 4
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 32 8
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD browse
FLOC 2 8
MENUBUTTON "Browse..." 9 3
ENDFIELD

FIELD viewlog
FLOC 16 8
MENUBUTTON "View Log" 9 3
ENDFIELD

ENDTILE

ENDFORM
