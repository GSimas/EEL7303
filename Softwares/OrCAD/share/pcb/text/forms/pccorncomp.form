FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Printed Circuit Corner"

POPUP <actlayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".
POPUP <frequnits>"Hz""1","KHz""1000","MHz""1000000","GHz""1000000000".

TILE PCCORNCOMP_MINI

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 22

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Line Width:"
TLOC 1 3
ENDTEXT

FIELD widthvar
FLOC 10 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 5
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 5
INFO swidthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 7
ENDTEXT

FIELD angle
FLOC 14 7
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

TEXT "Working Frequency:"
TLOC 1 9
ENDTEXT

FIELD frequency
FLOC 1 11
REALFILLIN 10 10
REALMIN 0.0
DECIMAL 10
ENDFIELD

FIELD frequnit
FLOC 13 11
ENUMSET 6
POP "frequnits"
ENDFIELD

TEXT "Electrical Length:"
TLOC 1 13
ENDTEXT

TEXT
FLOC 1 15
INFO electricallength 12
ENDTEXT

TEXT
TLOC 14 15
INFO electricallengthunits 6
ENDTEXT

FIELD newtrace
FLOC 1 17
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD
ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 24
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 24
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbpccorn.bmp
OPTIONS STRETCH
FLOC 32 1
FSIZE 44 26
ENDFIELD

ENDTILE

ENDFORM
