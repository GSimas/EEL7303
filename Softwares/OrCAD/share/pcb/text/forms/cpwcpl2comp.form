FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Coplanar Waveguide (2 Center Conductors)"

POPUP <actlayers>"""".
POPUP <groundnets>"""".
POPUP <reflayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE CPWCPL2

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 36

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground:"
TLOC 1 3
ENDTEXT

FIELD ground_subclass
FLOC 7 3
ENUMSET 16
POP "reflayers"
ENDFIELD

FIELD groundlayervar
FLOC 26 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Signal Line Width:"
TLOC 1 5
ENDTEXT

FIELD swidthvar
FLOC 14 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 7
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 7
INFO swidthunits
ENDTEXT

TEXT "CPW Spacing:"
TLOC 1 9
ENDTEXT

FIELD spacingvar
FLOC 12 9
MENUBUTTON "..." 3 3
ENDFIELD

FIELD spacing
FLOC 1 11
REALFILLIN 10 10
ENDFIELD

TEXT spacingunitslabel
TLOC 14 11
INFO spacingunits
ENDTEXT

TEXT "CPW Gap:"
TLOC 1 13
ENDTEXT

FIELD gapvar
FLOC 9 13
MENUBUTTON "..." 3 3
ENDFIELD

FIELD gap
FLOC 1 15
REALFILLIN 10 10
ENDFIELD

TEXT gapunitslabel
TLOC 14 15
INFO gapunits
ENDTEXT

TEXT "Ground Stripe Width:"
TLOC 1 17
ENDTEXT

FIELD gswidthvar
FLOC 16 17
MENUBUTTON "..." 3 3
ENDFIELD

FIELD gswidth
FLOC 1 19
REALFILLIN 10 10
ENDFIELD

TEXT gswidthunitslabel
TLOC 14 19
INFO gswidthunits
ENDTEXT

TEXT "CPW Length:"
TLOC 1 21
ENDTEXT

FIELD lengthvar
FLOC 11 21
MENUBUTTON "..." 3 3
ENDFIELD

FIELD length
FLOC 1 23
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 23
INFO lengthunits
ENDTEXT

TEXT "Ground Stripe Length:"
TLOC 1 25
ENDTEXT

FIELD glengthvar
FLOC 17 25
MENUBUTTON "..." 3 3
ENDFIELD

FIELD gslength
FLOC 1 27
REALFILLIN 10 10
ENDFIELD

TEXT gslengthunitslabel
TLOC 14 27
INFO gslengthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 29
ENDTEXT

FIELD angle
FLOC 14 29
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 31
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 3 Net:"
TLOC 1 5
ENDTEXT

TEXT
TLOC 10 5
INFO pin3netname 12
ENDTEXT

FIELD pin3netname_list
FLOC 24 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 4 Net:"
TLOC 1 7
ENDTEXT

TEXT
TLOC 10 7
INFO pin4netname 12
ENDTEXT

FIELD pin4netname_list
FLOC 24 7
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground Net"
TLOC 1 9
ENDTEXT

FIELD groundnet
FLOC 10 9
ENUMSET 15
POP "groundnets"
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 38
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 38
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbcpwcpl2.bmp
OPTIONS STRETCH
FLOC 32 4
FSIZE 48 32
ENDFIELD
ENDTILE

ENDFORM

