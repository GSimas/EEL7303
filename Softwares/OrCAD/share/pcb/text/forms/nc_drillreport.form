FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED    
PORT 136 24
HEADER "Library Drill Report"     

POPUP <right_mouse> "Dummy""Dummy".

TILE

TABSET "DrillReportTABS"
FLOC 1 1
TSSIZE 134 36

TAB "Drill/Slot Holes"

GRID DrillGrid
FLOC 1 1
FSIZE 132 32
OPTIONS HLINES VLINES USERSIZE
POP "right_mouse"

GHEAD TOP
HEADSIZE 3
OPTIONS 3D MULTI
ENDGHEAD

ENDGRID

ENDTAB

FIELD close
FLOC 1 38
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 11 38
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD writeFile
FLOC 31 38
MENUBUTTON "Write report file" 15 3
ENDFIELD

FIELD csvFile
FLOC 31 40
CHECKLIST "CSV" "file"
ENDFIELD

FIELD htmlFile
FLOC 38 40
CHECKLIST "HTML" "file"
ENDFIELD

ENDTABSET

FLEXMODE edgegravityone
FLEX DrillReportTABS 0 0 1 1
FLEX DrillGrid 0 0 1 1

ENDTILE

ENDFORM
