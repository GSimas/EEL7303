#
# acon_linespace.form - Add connect group route line spacing form
#
FILE_TYPE=FORM_DEFN VERSION=2
FORM autogreytext {fixed_font}
FIXED
PORT 32 10

HEADER "Route Spacing"

POPUP <space_vals> "0""0".

TILE ACON_ROUTESPACE

TEXT "Spacing Mode:"
TLOC 1 1
ENDTEXT

FIELD spacing_mode_current
FLOC 15 1
CHECKLIST "Current Space" space_mode
ENDFIELD
FIELD spacing_mode_mindrc
FLOC 15 3
CHECKLIST "Minimum DRC" space_mode
ENDFIELD
FIELD spacing_mode_user
FLOC 15 5
CHECKLIST "User-defined" space_mode
ENDFIELD


TEXT "Space:"
TLOC 1 8
ENDTEXT

FIELD spacing_val
FLOC 15 8
REALFILLIN 8 20
POP "space_vals"
DECIMAL 0
ENDFIELD

FIELD done
FLOC 1 12
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD cancel
FLOC 12 12
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 23 12
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM

