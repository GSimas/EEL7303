FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 56 8
HEADER "IPC2581 In"

MESSAGE nexist 0 "You must enter a file name first."
MESSAGE nerror 0 "Error occurred - Check logfile."
MESSAGE ndone 0 "Translation complete."
MESSAGE ndemo 0 "Cannot Run in Demo Mode."
MESSAGE nfile 0 "The input file can not be found."

TILE

TEXT "IPC2581 File:"
TLOC 2 1
ENDTEXT

FIELD file_name
FLOC 12 1
STRFILLIN 36 1024
ENDFIELD

FIELD browse
FLOC 50 1
MENUBUTTON "..." 3 3
ENDFIELD


TEXT "Limited to importing Cadence Allegro IPC2581 files."
TLOC 2 4
ENDTEXT 

FIELD purge
FLOC 45 4
MENUBUTTON "Purge Import" 10 3
ENDFIELD


FIELD execute
FLOC 2 7
MENUBUTTON "Import" 10 3
ENDFIELD

FIELD compare
FLOC 13 7
MENUBUTTON "Compare..." 10 3
ENDFIELD


FIELD cancel
FLOC 24 7
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD viewlog
FLOC 35 7
MENUBUTTON "Viewlog" 10 3
ENDFIELD

FIELD help
FLOC 46 7
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM

