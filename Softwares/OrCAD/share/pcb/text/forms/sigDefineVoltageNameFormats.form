FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 58 20
HEADER "Possible Voltage Net Rules"

TILE

GROUP "Voltage Net Name Formats"
GLOC 1 1
GSIZE 57 31
ENDGROUP

TEXT "The following list defines the formats of potential voltage net names."
TLOC 2 4
ENDTEXT

TEXT "These formats can contain any of the standard Unix wildcard characters,"
TLOC 2 6
ENDTEXT

TEXT "such as '*', '?', '[ ]' and '{ }'.
TLOC 2 8
ENDTEXT

FIELD VoltageFormatStrings
FLOC 12 11
LIST "" 32 8
ENDFIELD

FIELD AddFormatString
FLOC 12 26
MENUBUTTON "Add New Voltage Net Name Format" 33 3
ENDFIELD

FIELD DeleteFormatString
FLOC 12 29
MENUBUTTON "Delete Selected Voltage Net Name Format" 33 3
ENDFIELD

FIELD IncludeNetsWithPowerGroundPins
FLOC 2 33
CHECKLIST "Include nets that contain power or ground pins."
ENDFIELD

FIELD IncludeNetsWithLargePinCounts
FLOC 2 35
CHECKLIST "Include nets that contain more than"
ENDFIELD

FIELD LargeNetPinCount
FLOC 30 35
STRFILLIN 4 10
ENDFIELD

TEXT "pins."
TLOC 36 35
ENDTEXT

FIELD MyOK
FLOC 2 39
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD Cancel
FLOC 24 39
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 48 39
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
