FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 81 36
HEADER "Header Information."

TILE

TEXT "Select the header and footer information you would like included in the text file for your component."
TLOC 3 1
ENDTEXT

TEXT "The header, as it will look in the file, is previewed in the text window below."
TLOC 3 3
ENDTEXT

GROUP "Basics"
GLOC 3 5
GSIZE 18 13
ENDGROUP

FIELD file_line
FLOC 5 7
CHECKLIST "File name"
ENDFIELD

FIELD date_line
FLOC 5 9
CHECKLIST "Date/Time"
ENDFIELD

FIELD units_line
FLOC 5 11
CHECKLIST "Design units"
ENDFIELD

FIELD inst_as_def
FLOC 5 15
CHECKLIST "Instance Data"
ENDFIELD

GROUP "Package"
GLOC 23 5
GSIZE 56 13
ENDGROUP

FIELD packname_line
FLOC 25 7
CHECKLIST "Name"
ENDFIELD

FIELD refdes_line
FLOC 25 9
CHECKLIST "Reference designator"
ENDFIELD

FIELD defname_line
FLOC 25 11
CHECKLIST "DEF design name"
ENDFIELD

FIELD dietype_line
FLOC 25 13
CHECKLIST "Die type"
ENDFIELD

FIELD origin_line
FLOC 45 7
CHECKLIST "Origin"
ENDFIELD

FIELD rotate_line
FLOC 45 9
CHECKLIST "Rotation"
ENDFIELD

FIELD layer_line
FLOC 45 11
CHECKLIST "Pad layer"
ENDFIELD

FIELD dieorient_line
FLOC 45 13
CHECKLIST "Die orientation"
ENDFIELD

FIELD write_pads
FLOC 65 7
CHECKLIST "Padstack data"
ENDFIELD

FIELD write_grids
FLOC 65 9
CHECKLIST "Grid data"
ENDFIELD

FIELD bbox_line
FLOC 25 16
CHECKLIST "Extents" "sizes_options"
ENDFIELD

FIELD size_line
FLOC 45 16
CHECKLIST "Dimensions" "sizes_options"
ENDFIELD

FIELD nosize_line
FLOC 65 16
CHECKLIST "No size info" "sizes_options"
ENDFIELD

TEXT "File name:"
TLOC 3 19
INFO file_name 55
ENDGROUP

GRID grid
FLOC 3 21
FSIZE 76 15
ENDGRID

FIELD back
FLOC 3 36
MENUBUTTON "<Back" 9 3
ENDFIELD

FIELD next
FLOC 13 36
MENUBUTTON "Next>" 9 3
ENDFIELD

FIELD cancel
FLOC 24 36
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 70 36
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM

