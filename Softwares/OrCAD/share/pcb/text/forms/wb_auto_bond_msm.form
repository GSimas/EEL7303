FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 29 40
HEADER "Auto Bond"

POPUP <profilePopup>"""".

TILE

GROUP "Bond wires"
GLOC 1 1
GSIZE 27 5
ENDGROUP

TEXT "Profile:"
TLOC 3 3
ENDTEXT
FIELD wireProfile
FLOC 10 3
STRFILLIN 12 1023
POP "profilePopup"
ENDFIELD

GROUP "Power/Ground rings"
GLOC 1 6
GSIZE 27 11
ENDGROUP

FIELD checkRings
FLOC 2 9
CHECKLIST "Create rings" 
ENDFIELD

TEXT "Dist. to 1st ring:"
TLOC 2 11
ENDTEXT

FIELD distanceFirstRing
FLOC 17 11
STRFILLIN 9 100
ENDFIELD

TEXT "Ring width:"
TLOC 2 13
ENDTEXT

FIELD ringWidth
FLOC 17 13
STRFILLIN 9 100
ENDFIELD

TEXT "Dist. between rings:"
TLOC 2 15
ENDTEXT

FIELD distanceRings
FLOC 17 15
STRFILLIN 9 100
ENDFIELD

GROUP "Guide paths"
GLOC 1 18
GSIZE 27 11
ENDGROUP

FIELD checkLine
FLOC 2 20
CHECKLIST "Linear path"
ENDFIELD

FIELD RowBond
FLOC 17 20
CHECKLIST "Row bond"
ENDFIELD

TEXT "Paths per die side:"
TLOC 2 22
ENDTEXT

FIELD sidePaths
FLOC 17 22
LONGFILLIN 3 2
MIN 1
ENDFIELD

TEXT "Dist. to 1st path:"
TLOC 2 24
ENDTEXT

FIELD distanceFirstPath
FLOC 17 24
STRFILLIN 9 100
ENDFIELD

TEXT "Dist. between paths:"
TLOC 2 26
ENDTEXT

FIELD distancePaths
FLOC 17 26
STRFILLIN 9 100
ENDFIELD

FIELD autoBond
FLOC 2 30
MENUBUTTON "Auto bond" 10 3
ENDFIELD

FIELD close
FLOC 14 30
MENUBUTTON "Done auto bond" 10 3
ENDFIELD

ENDTILE
ENDFORM

