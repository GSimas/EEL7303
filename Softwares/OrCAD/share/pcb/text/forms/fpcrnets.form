FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 89 26
HEADER "Create List of Nets"


TILE
TEXT "List file name:"
TLOC 2 1
ENDTEXT

TEXT "Net Filter:"
TLOC 3 10
TGROUP "lower"
ENDTEXT

TEXT "Available Nets"
TLOC 3 12
TGROUP "lower"
ENDTEXT

TEXT "Selected Nets"
TLOC 50 12
TGROUP "lower"
ENDTEXT

FIELD fpnetsFile
FLOC 15 1
STRFILLIN 30 512
ENDFIELD

FIELD fpnetsOpen
FLOC 15 4
MENUBUTTON "Open" 8 3
ENDFIELD

FIELD fpnetsSave
FLOC 25 4
MENUBUTTON "Save" 8 3
ENDFIELD

FIELD fpnetsSaveAs
FLOC 35 4
MENUBUTTON "Save As" 9 3
ENDFIELD

FIELD fpnetsFilter
FLOC 11 10
STRFILLIN 12 255
ENDFIELD

FIELD fpnetsAllFrom
FLOC 39 20
FGROUP "lower"
MENUBUTTON "<-- All" 9 3
ENDFIELD

FIELD fpnetsAllTo
FLOC 39 23
FGROUP "lower"
MENUBUTTON "All -->" 9 3
ENDFIELD

FIELD fpnetsFrom
FLOC 3 14
FGROUP "lower"
LIST "" 35 12
ENDFIELD

FIELD fpnetsTo
FLOC 50 14
FGROUP "lower"
LIST "" 35 12
ENDFIELD

FIELD done
FLOC 2 40
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD help
FLOC 76 40
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "Edit List of Nets"
GLOC 2 7
GSIZE 84 32
ENDGROUP

ENDTILE

ENDFORM
