FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 20
HEADER "Create Device File"

POPUP <DTYPE>"IC""IC","IO""IO","DISCRETE""DISCRETE".

TILE 

TEXT "Device Type:" 
TLOC 1 1
ENDTEXT 
 
FIELD dtype
FLOC 11 1
ENUMSET 10
POP "DTYPE"
ENDFIELD

FIELD done
FLOC 26 1
MENUBUTTON "OK" 11 3
ENDFIELD
 
FIELD cancel
FLOC 26 4
MENUBUTTON "Cancel" 11 3
ENDFIELD
 
FIELD help
FLOC 26 10
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
