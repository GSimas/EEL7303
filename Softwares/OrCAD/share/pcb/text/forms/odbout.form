FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 50 38
HEADER "Valor ODB++ Translator"
POPUP <class>"class""class".



TILE

TEXT "Output directory:"
FGROUP "General"
TLOC 5 3
ENDTEXT

FIELD output_dir
FLOC 5 5
FGROUP "General"
STRFILLIN 24 1024
ENDFIELD

FIELD dir_browse
FLOC 31 5
FGROUP "General"
MENUBUTTON "..." 3 3
ENDFIELD


GROUP "General"
GLOC 3 1 
GSIZE 43 12
ENDGROUP 

FIELD compress
FLOC 5 7
FGROUP "General"
CHECKLIST "Compress"
ENDFIELD


TEXT "Outline size:"
FGROUP "General"
TLOC 5 10
ENDTEXT

FIELD outline_size
FLOC 15 10
FGROUP "General"
REALFILLIN 5 5
ENDFIELD

TEXT "Symbol tolerance:"
FGROUP "General"
TLOC 23 10
ENDTEXT

FIELD sym_tolerance
FLOC 37 10
FGROUP "General"
REALFILLIN 5 5 
ENDFIELD


GROUP "Padflash"
GLOC 3 14
GSIZE 19 7
ENDGROUP

FIELD padflash_ignore
FLOC 6 16
FGROUP "PadFlash"
CHECKLIST "Ignore" "padflash"
ENDFIELD

FIELD padflash_sub
FLOC 6 18
FGROUP "PadFlash"
CHECKLIST "Substitute" "padflash"
ENDFIELD


GROUP "Component outline"
GLOC 25 14
GSIZE 21 7
ENDGROUP 
 
FIELD comp_outline_placebound
FLOC 27 16
FGROUP "Component Outline"
CHECKLIST "Placebound" "comp_outline"
ENDFIELD
 
FIELD comp_outline_assem
FLOC 27 18
FGROUP "Component Outline"
CHECKLIST "Assembly" "comp_outline"
ENDFIELD

GROUP "Thermal"
GLOC 3 22
GSIZE 43 9
ENDGROUP 
 
TEXT "File:"
FGROUP "Thermal"
TLOC 5 25
ENDTEXT  
 
FIELD thermal_file
FLOC 10 25
STRFILLIN 20 1024
FGROUP "Thermal"
ENDFIELD 
 
FIELD therm_browse
FLOC 32 25
FGROUP "Thermal"
MENUBUTTON "..." 3 3
ENDFIELD 
 
 
TEXT "Name:"
TLOC 5 28
FGROUP "Thermal"
ENDTEXT  
 
FIELD thermal_name
FLOC 10 28
STRFILLIN 20 1024
FGROUP "Thermal"
ENDFIELD 

GROUP "Extract files"
GLOC 3 32
GSIZE 43 5
ENDGROUP

FIELD delete_extracts
FLOC 5 34
FGROUP "Extract Files"
CHECKLIST "Delete when done"
ENDFIELD

FIELD regen_extracts
FLOC 25 34
FGROUP "Extract Files"
CHECKLIST "Regenerate"
ENDFIELD

FIELD done
FLOC 2 38
MENUBUTTON "Export" 9 3
ENDFIELD


FIELD close
FLOC 14 38
MENUBUTTON "Close" 9 3
ENDFIELD
 
FIELD cancel
FLOC 26 38
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 40 38
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
