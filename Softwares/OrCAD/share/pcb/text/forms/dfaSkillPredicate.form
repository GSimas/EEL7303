FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 61 44
HEADER "DFA Skill Predicate"

POPUP <MY_POP>"design""design","component""component","pin""pin","via""via","net""net","drc""drc","value""value","polygon""polygon","shape""shape".
POPUP <MY_POP1>" "" ".

TILE
TEXT "Environment File: "
TLOC 2 1

ENDTEXT

FIELD EnvFileName
FLOC 20 1
STRFILLIN 27 100
FGROUP " " 
ENDFIELD

FIELD Browse1
FLOC 50 1
MENUBUTTON "Browse..." 5 3
FGROUP " " 
ENDFIELD

FIELD TreeList
FLOC 11 5
TREEVIEW  38 5
FGROUP " " 
ENDFIELD

FIELD DeletePred
FLOC 50 12
MENUBUTTON " Delete " 8 3
ENDFIELD

TEXT "Predicate Name:"
TLOC 2 18
FGROUP "Predicate Definition" 
ENDTEXT
 
FIELD PredicateName
FLOC 23 18
STRFILLIN 35 100
FGROUP "Predicate Definition"
ENDFIELD


TEXT "Skill Function Name :"
TLOC 2 20
FGROUP "Predicate Definition"
ENDTEXT

FIELD SkillFnName
FLOC 23 20
STRFILLIN 35 100
FGROUP "Predicate Definition"
ENDFIELD


TEXT "Arguments Type List :"
TLOC 2 22
ENDTEXT

FIELD ArgsPassed
FLOC 23 22
STRFILLIN 35 150 
FGROUP "Predicate Definition"
ENDFIELD

TEXT "Return Type :"
TLOC 2 24
ENDTEXT  
 
FIELD ReturnType
FLOC 23 24
ENUMSET 15
POP MY_POP
ENDFIELD 

FIELD Clear
FLOC 50 24
MENUBUTTON "Clear" 10 3
FGROUP "Predicate Definition"
ENDFIELD

FIELD done
FLOC 2 28 
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 16 28
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD apply
FLOC 32 28
MENUBUTTON "Apply" 10 3
ENDFIELD

FIELD help
FLOC 49 28
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "Predicates Defined"
GLOC 1 3
GSIZE 60 12
ENDGROUP

GROUP "Predicate Definition"
GLOC 1 15
GSIZE 60 12
ENDGROUP

ENDTILE

ENDFORM
