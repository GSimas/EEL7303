FILE_TYPE=FORM_DEFN VERSION=2
    FORM
    FIXED
    PORT 109 60
    HEADER "Virtual Excitation Management"

    ACTIVEFIELD ExcitationNameList
    DEFAULT Done

    TILE

    FIELD Done
        FLOC 2 31
        MENUBUTTON "OK" 10 3
    ENDFIELD

    FIELD Cancel
        FLOC 13 31
        MENUBUTTON "Cancel" 10 3
    ENDFIELD

    FIELD Import
        FLOC 77 31
        MENUBUTTON "Import" 10 3
    ENDFIELD

    FIELD Export
        FLOC 88 31
        MENUBUTTON "Export" 10 3
    ENDFIELD

    FIELD Help
        FLOC 99 31
        MENUBUTTON "Help" 10 3
    ENDFIELD

    GROUP ""
        GLOC 1 0
        GSIZE 108 30
    ENDGROUP

    TEXT "Excitation name:"
        TLOC 2 2
    ENDTEXT

    FIELD ExcitationNameList
        FLOC 2 4
        LIST "" 22 11
    ENDFIELD

    FIELD ExcitationAdd
        FLOC 2 26
        MENUBUTTON "Add" 10 3
    ENDFIELD

    FIELD ExcitationRem
        FLOC 14 26
        MENUBUTTON "Remove" 10 3
    ENDFIELD

    TEXT "Amplitude:""GaussianAmplitudeTitle"
        TLOC 27 4
    ENDTEXT

    FIELD GaussianAmplitude
        FLOC 42 4
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Delay time:""GaussianDelayTimeTitle"
        TLOC 27 7
    ENDTEXT

    FIELD GaussianDelayTime
        FLOC 42 7
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Width:""GaussianWidthTitle"
        TLOC 27 10
    ENDTEXT

    FIELD GaussianWidth
        FLOC 42 10
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Init value:""PulseInitValueTitle"
        TLOC 27 4
    ENDTEXT

    FIELD PulseInitValue
        FLOC 42 4
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Final value:""PulseFinalValueTitle"
        TLOC 27 7
    ENDTEXT

    FIELD PulseFinalValue
        FLOC 42 7
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Delay time:""PulseDelayTimeTitle"
        TLOC 27 10
    ENDTEXT

    FIELD PulseDelayTime
        FLOC 42 10
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Rise time:""PulseRiseTimeTitle"
        TLOC 27 13
    ENDTEXT

    FIELD PulseRiseTime
        FLOC 42 13
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Fall time:""PulseFallTimeTitle"
        TLOC 27 16
    ENDTEXT

    FIELD PulseFallTime
        FLOC 42 16
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Width:""PulseWidthTitle"
        TLOC 27 19
    ENDTEXT

    FIELD PulseWidth
        FLOC 42 19
        STRFILLIN 10 256
    ENDFIELD

    TEXT "Period time:""PulsePeriodTitle"
        TLOC 27 22
    ENDTEXT

    FIELD PulsePeriod
        FLOC 42 22
        STRFILLIN 10 256
    ENDFIELD

    FIELD ExcitationGraph
        THUMBNAIL
        FLOC 55 3
        FSIZE 53 22
    ENDFIELD

    TEXT "The excitation acts as sink current in design and the board-package-die co-design flow."
        TLOC 27 26
    ENDTEXT

    ENDTILE

ENDFORM
