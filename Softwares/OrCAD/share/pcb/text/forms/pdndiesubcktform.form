FILE_TYPE=FORM_DEFN VERSION=2

    FORM
    FIXED
    PORT 87 50
    HEADER "Die Subcircuit Connection"

    ACTIVEFIELD "Done"
    DEFAULT Done

    TILE

    FIELD Done
        FLOC 2 30
        MENUBUTTON "OK" 10 3
    ENDFIELD

    FIELD Cancel
        FLOC 13 30
        MENUBUTTON "Cancel" 10 3
    ENDFIELD

    FIELD Help
        FLOC 77 30
        MENUBUTTON "Help" 10 3
    ENDFIELD

    GROUP ""
        GLOC 1 0
        GSIZE 86 29
    ENDGROUP

    TEXT "Equivalent Subcircuit For Die:"
        TLOC 2 2
        OPTIONS BOLD
    ENDTEXT

    TEXT
        TLOC 29 2
        INFO DieName 10
        OPTIONS BOLD
    ENDTEXT

    TEXT "Power Net:"
        TLOC 45 2
        OPTIONS BOLD
    ENDTEXT

    TEXT
        TLOC 57 2
        INFO PowerNet 10
        OPTIONS BOLD
    ENDTEXT

    TEXT "SubCkt:"
        TLOC 2 5
    ENDTEXT

    POPUP <SubCktNameList>"""".
    FIELD SubCktName
        FLOC 11 5
        ENUMSET 31
        POP "SubCktNameList"
    ENDFIELD

    POPUP <SubCktBrowserList>"Model Browse""SubCktModelBrowser","External""SubCktExternalBrowser".
    FIELD SubCktBrowser
        FLOC 48 5
        MENUBUTTON "Browse ..." 15 3
        POP "SubCktBrowserList"
    ENDFIELD

    FIELD SubCktTextEdit
      FLOC 67 5
      MENUBUTTON "Text Edit" 15 3
    ENDFIELD

    TEXT "Die <--> Chip Mapping:"
        TLOC 2 8
    ENDTEXT

    POPUP <SubCktPadNameList>"""".
    POPUP <SubCktPortNameList>"""".
    GRID SubCktPinGrid
    FLOC 2 10
    FSIZE 84 18
    OPTIONS HLINES VLINES USERSIZE

    GHEAD TOP
        HEADSIZE 2
        OPTIONS 3D
    ENDGHEAD
    GHEAD SIDE
        HEADSIZE 0
    ENDGHEAD
    ENDGRID

    ENDTILE
ENDFORM
