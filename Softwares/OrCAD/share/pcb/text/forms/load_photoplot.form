FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 55 14
HEADER "Load Cadence Artwork"

    #ifdef APD || ALLEGRO_MCM || SIP
POPUP <CLASSNAMES>"CONDUCTOR""ETCH","SUBSTRATE GEOMETRY""BOARD GEOMETRY","DRAWING FORMAT""DRAWING FORMAT","MANUFACTURING""MANUFACTURING".
    #else
POPUP <CLASSNAMES>"ETCH""ETCH","BOARD GEOMETRY""BOARD GEOMETRY","DRAWING FORMAT""DRAWING FORMAT","MANUFACTURING""MANUFACTURING".
    #endif

POPUP <SUBCLASSNAMES>"TOP""TOP","BOTTOM""BOTTOM".
POPUP <FORMATTYPE>"Gerber 4x00""1","Gerber 6x00""2","Gerber RS274X""3","Barco DPF""4","MDA""5","Automatic""0".
POPUP <ORIGIN>"Data Origin""dataOrigin","Absolute Origin""absOrigin".

ACTIVEFIELD "fileName"


TILE

TEXT "Filename:"
TLOC 1 1
ENDTEXT

FIELD fileName
FLOC 10 1
STRFILLIN 38 1000
ENDFIELD

FIELD browse
FLOC 51 1
MENUBUTTON "..." 3 3
ENDFIELD
 
TEXT "Format:"
TLOC 1 4
ENDTEXT

TEXT
TLOC 10 4
INFO formatType 20
ENDTEXT

FIELD forceFormatType
FLOC 33 4
MENUBUTTON "Manual" 11 3
POP "FORMATTYPE"
ENDFIELD

TEXT "Class:"
TLOC 1 6
ENDTEXT

FIELD className
FLOC 10 6
ENUMSET 21
OPTIONS prettyprint 
POP "CLASSNAMES"
ENDFIELD

TEXT "Subclass:"
TLOC 1 8
ENDTEXT

FIELD subClassName
FLOC 10 8
ENUMSET 21
OPTIONS prettyprint ownerdrawn
POP "SUBCLASSNAMES"
ENDFIELD

TEXT "Limited to importing Cadence Allegro Gerber files."
TLOC 2 11
ENDTEXT

#ifdef NOTYET
FIELD autolayer
FLOC 10 10
CHECKLIST "Create Drawing Format subclass using film name"
ENDFIELD
#endif

GROUP "Options"
GLOC 1 13
GSIZE 52 10
ENDGROUP

TEXT
TLOC 9 18
INFO artworkFilePresent 30
ENDTEXT

FIELD reuseValues
FLOC 2 18
CHECKLIST "Re-use last mirror/rotation/location"
ENDFIELD

FIELD displayTargets
FLOC 2 16
CHECKLIST "Display pad targets"
ENDFIELD
 
TEXT
TLOC 5 16
INFO originTxt 8
ENDTEXT

FIELD origin
FLOC 15 16
ENUMSET 16
POP "ORIGIN"
ENDFIELD

FIELD offsetAdd
FLOC 2 20
CHECKLIST "Add offset"
ENDFIELD

FIELD loadFile
FLOC 20 24
MENUBUTTON "Load file" 15 3
ENDFIELD

FIELD done
FLOC 1 24
MENUBUTTON "Done" 11 3
ENDFIELD

FIELD help
FLOC 42 24
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
