FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 55 19
HEADER  "LVS Check" 

POPUP <LEFLIBPOPUP14>"""".
POPUP <ICPOPUP14>"""".
POPUP <LAYERPOPUP14>"""".

TILE

TEXT "Reference Designator:"
TLOC 2 2
ENDTEXT

FIELD refdes
FLOC 20 2
ENUMSET 21
POP "ICPOPUP14"
ENDFIELD

GROUP " Import data "
FLOC 2 5
FSIZE 50 13

TEXT "File Type:"
TLOC 5 8
ENDTEXT

FIELD filetype
FLOC 13 8
ENUMSET 7
POP "ICPOPUP14"
ENDFIELD


TEXT "File:"
TLOC 5 11
ENDTEXT

FIELD filename
FLOC 13 11
STRFILLIN 23 100
ENDFIELD

FIELD filePath
FLOC 40 11
MENUBUTTON "Browse..." 10 3
ENDFIELD


TEXT "Lef Library: "
TLOC 5 14
ENDTEXT

TEXT ""
TLOC 14 14
INFO leflibrary 25
ENDTEXT

FIELD lefmanager
FLOC 29 14
MENUBUTTON "&Library Manager..." 21 3
ENDFIELD


ENDGROUP


GROUP " Golden data "
FLOC 2 19
FSIZE 18 10


FIELD golden_file
FLOC 6 22
CHECKLIST "File" "golden"
ENDFIELD

FIELD golden_design
FLOC 6 24
CHECKLIST "Package" "golden"
ENDFIELD

ENDGROUP

GROUP " Data checks (optional) "
FLOC 23 19
FSIZE 29 10

FIELD netnames
FLOC 25 22
CHECKLIST "Net names"
ENDFIELD

FIELD diffpair
FLOC 25 24
CHECKLIST "Diff Pair property" 
ENDFIELD

FIELD linewidth
FLOC 25 26
CHECKLIST "Line width constraint" 
ENDFIELD

ENDGROUP

FIELD ok
FLOC 10 31
MENUBUTTON "&OK" 10 3
ENDFIELD
 
FIELD cancel
FLOC 22 31
MENUBUTTON "&Cancel" 10 3
ENDFIELD

FIELD Help
FLOC 34 31
MENUBUTTON "&Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
