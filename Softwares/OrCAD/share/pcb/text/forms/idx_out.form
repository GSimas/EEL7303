FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 75 7
HEADER "IDX Out"

POPUP <formatList>"IDX""IDX","PTC""PTC","SDRC""SDRC".

MESSAGE nerror 0 "Error occurred - Check logfile."
MESSAGE ndone 0 "Translation complete."
MESSAGE ndemo 0 "Cannot Run in Demo Mode."
MESSAGE nbaseline1 0 "Baseline field is empty."
MESSAGE nbaseline2 0 "Baseline file not found."
MESSAGE nexist 0 "Using Board name for Output File Name."

TILE

TEXT "Output file name:"
TLOC 2 1
ENDTEXT

FIELD outnameField
FLOC 18 1
STRFILLIN 50 1024
ENDFIELD

FIELD browse
FLOC 70 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Design version:"
TLOC 2 4
ENDTEXT

FIELD versionField
FLOC 18 4
LONGFILLIN 5 5
MIN 0
ENDFIELD

TEXT "Source identification:"
TLOC 2 7
ENDTEXT

FIELD srcField
FLOC 18 7
STRFILLIN 30 256
ENDFIELD

TEXT "Export filter:"
TLOC 2 10
ENDTEXT

FIELD baseFilter
FLOC 18 10
MENUBUTTON "Filter Options" 10 3
ENDFIELD

TEXT "Re-baseline:"
TLOC 2 13
ENDTEXT

FIELD re_baseline
FLOC 18 13
MENUBUTTON "Re-Baseline" 10 3
ENDFIELD

FIELD compare_file
FLOC 53 4
MENUBUTTON "Export Compare File" 20 3
ENDFIELD

FIELD Customized_shapes
FLOC 53 7
MENUBUTTON "Export  User  Layers" 20 3
ENDFIELD

FIELD execute
FLOC 2 16
MENUBUTTON "Export" 10 3
ENDFIELD

FIELD viewlog
FLOC 15 16
MENUBUTTON "Viewlog" 10 3
ENDFIELD

FIELD cancel
FLOC 28 16
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD help
FLOC 41 16
MENUBUTTON "Help" 10 3
ENDFIELD


ENDTILE

ENDFORM

