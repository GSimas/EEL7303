FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 60 34
HEADER "Die Abstract Write"

TILE

POPUP <diePop> "" "".

TEXT "Die to write abstract for:"
TLOC 1 1
ENDTEXT
FIELD die_comp
FLOC 3 3
ENUMSET 53
POP "diePop"
ENDFIELD

FIELD write_dia
FLOC 1 6
CHECKLIST "Die abstract (.dia or .xda) file:"
ENDFIELD

FIELD dia_file
FLOC 3 8
STRFILLIN 50 1024
ENDFIELD

FIELD dia_browse
FLOC 55 8
MENUBUTTON "..." 4 3
ENDFIELD

FIELD write_txt
FLOC 1 10
CHECKLIST "Encounter I/O update (.txt) file:"
ENDFIELD

FIELD txt_file
FLOC 3 12
STRFILLIN 50 1024
ENDFIELD

FIELD txt_browse
FLOC 55 12
MENUBUTTON "..." 4 3
ENDFIELD

FIELD write
FLOC 1 16
MENUBUTTON "Write" 9 3
ENDFIELD

FIELD close
FLOC 13 16
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 50 16
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM

