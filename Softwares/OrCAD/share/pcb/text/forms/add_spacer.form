FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 38 30
HEADER "Add Spacer"

POPUP <layerPopup>" ""0".
POPUP <rotationPopup>"0 degrees""0 degrees","90 degrees""90 degrees","180 degrees""180 degrees","270 degrees""270 degrees".
POPUP <diePopup>"""".

TILE

TEXT "Ref ID:"
TLOC 2 2
ENDTEXT
FIELD refId
FLOC 15 2
STRFILLIN 16 28
ENDFIELD

TEXT "Symbol name:"
TLOC 2 4
ENDTEXT
FIELD symbolName
FLOC 15 4
STRFILLIN 16 40
ENDFIELD
FIELD symbolBrowse
FLOC 34 4
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Part number:"
TLOC 2 6
ENDTEXT
FIELD partNumber
FLOC 15 6
STRFILLIN 16 40
ENDFIELD

GROUP "Material"
GLOC 1 8 
GSIZE 37 7
ENDGROUP

TEXT "Name:"
TLOC 2 10
ENDTEXT
FIELD dielMatl
FLOC 15 10
STRFILLIN 16 40
ENDFIELD
FIELD dielMatlBrowse
FLOC 34 10
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Thickness:"
TLOC 2 12
ENDTEXT
FIELD dielThk
FLOC 15 12
STRFILLIN 16 40
ENDFIELD

GROUP "Dimensions"
GLOC 1 15
GSIZE 37 11
ENDGROUP

TEXT "Length:"
TLOC 2 17
ENDTEXT
FIELD length
FLOC 15 17
STRFILLIN 16 40
ENDFIELD

TEXT "Width:"
TLOC 2 19
ENDTEXT
FIELD width
FLOC 15 19
STRFILLIN 16 40
ENDFIELD

FIELD dim_derive
FLOC 2 21
CHECKLIST "Derive from:"
ENDFIELD
FIELD dim_die
FLOC 15 21
ENUMSET 15
POP "diePopup"
ENDFIELD
TEXT "Shrink factor:"
TLOC 4 23
ENDTEXT
FIELD dim_shrink
FLOC 15 23
REALFILLIN 16 16
MIN 0.01
ENDFIELD

GROUP "Placement"
GLOC 1 26
GSIZE 37 7
ENDGROUP

TEXT "Layer:"
TLOC 2 28
ENDTEXT
FIELD layer
FLOC 15 28
ENUMSET 15
POP "layerPopup"
ENDFIELD

TEXT "Rotation:"
TLOC 2 30
ENDTEXT
FIELD rotation
FLOC 15 30
ENUMSET 15
POP "rotationPopup"
ENDFIELD

FIELD done
FLOC 1 35
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD place
FLOC 10 35
MENUBUTTON "Place" 9 3
ENDFIELD

FIELD cancel
FLOC 19 35
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 30 35
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM
