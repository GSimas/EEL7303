FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 20
HEADER "Ministatus"

TILE 

TEXT "Max iterations:"
TLOC 1 2
ENDTEXT

FIELD max_iterations
FLOC 14 2
INTFILLIN 3 3
MIN 1
MAX 999
ENDFIELD

FIELD stretch_cline
FLOC 1 5
CHECKLIST ""
ENDFIELD

TEXT "Stretch trace end vertex to pin/via"
FLOC 4 5
FSIZE 20 3
ENDFIELD

FIELD allow_touching_shapes
FLOC 1 8
CHECKLIST ""
ENDFIELD

TEXT "Assign shapes that touch but do not overlap"
FLOC 4 8
FSIZE 20 3
ENDTEXT

GROUP "Status"
GLOC 1 11
GSIZE 27 11
ENDGROUP

TEXT "DRC selected:"
TGROUP "status"
TLOC 3 13
ENDTEXT

TEXT
FLOC 20 13
INFO drc_selected 5
FGROUP "status"
ENDTEXT

TEXT "Iteration:"
TGROUP "status"
TLOC 3 15
ENDTEXT

TEXT
FLOC 20 15
INFO iteration 5
FGROUP "status"
ENDTEXT

TEXT "Connections resolved:"
TGROUP "status"
TLOC 3 17
ENDTEXT

TEXT
FLOC 20 17
INFO connected 5
FGROUP "status"
ENDTEXT

TEXT "Total DRC errors:"
TGROUP "status"
TLOC 3 19
ENDTEXT

TEXT
FLOC 20 19
INFO drc_total 5
FGROUP "status"
ENDTEXT

ENDTILE

ENDFORM
