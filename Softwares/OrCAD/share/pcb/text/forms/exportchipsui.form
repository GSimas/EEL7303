ENDFIELD

TEXT "Output Dir Path:"
TLOC 2 24
ENDTEXT

FIELD Directory
FLOC 20 24
STRFILLIN 80 1024
ENDFIELD

FIELD Browse
FLOC 103 24
MENUBUTTON "..." 3 3
ENDFIELD

FIELD Ok
FLOC 2 28
MENUBUTTON "Ok" 10 3
ENDFIELD
 
FIELD Cancel
FLOC 14 28
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 26 28
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
