FILE_TYPE=FORM_DEFN VERSION=2

FORM fixed_font
	FIXED
	PORT 37 18
	HEADER "RFSiP Explore Route"

        POPUP <library>"""".
	POPUP <cell>"""".


	TILE

		GROUP ""
			GLOC 1 2
			GSIZE 36 12 
		ENDGROUP

		TEXT "Virtuoso Library and Cell "
			TLOC 1 1 
		ENDTEXT

		TEXT "Library"
			TLOC 2 4 
		ENDTEXT

		FIELD library
			FLOC 10 4
                        STRFILLIN 12 12
                        POP "library"
		ENDFIELD

		TEXT "Cell"
			TLOC 2 7 
		ENDTEXT

		FIELD cell
			FLOC 10 7
                        STRFILLIN 12 12
                        POP "cell"
		ENDFIELD

                
		TEXT "View"
			TLOC 2 10
		ENDTEXT

		FIELD  view
			FLOC 10 10
			STRFILLIN 12 12
		ENDFIELD


        	FIELD ok
			FLOC 1 15
			MENUBUTTON "OK" 8 3
		ENDFIELD

		FIELD cancel
			FLOC 10 15
			MENUBUTTON "Cancel" 8 3
		ENDFIELD


	ENDTILE

ENDFORM
