FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 62 12
HEADER "Connect by Component"
POPUP <FromDesignName> """".
POPUP <FromComponentName> """".
POPUP <ToDesignName> """".
POPUP <ToComponentName> """".


ACTIVEFIELD "FromDesign"

TILE

TEXT "Design"
TLOC 2 2
FGROUP "From"
ENDTEXT

TEXT "Component"
TLOC 16 2
FGROUP "From"
ENDTEXT

TEXT "Design"
TLOC 33 2
FGROUP "From"
ENDTEXT

TEXT "Component"
TLOC 48 2
FGROUP "From"
ENDTEXT

FIELD FromDesign
FLOC 2 4
FGROUP "From"
STRFILLIN 8 40
POP "FromDesignName"
ENDFIELD

FIELD FromComponent
FLOC 16 4
FGROUP "From"
STRFILLIN 8 40
POP "FromComponentName"
ENDFIELD

FIELD ToDesign
FLOC 33 4
FGROUP "To"
STRFILLIN 8 40
POP "ToDesignName"
ENDFIELD

FIELD ToComponent
FLOC 48 4
FGROUP "To"
STRFILLIN 8 40
POP "ToComponentName"
ENDFIELD

FIELD OK
FLOC 2 8
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD Cancel
FLOC 16 8
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 28 8
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "From"
GLOC 1 0
GSIZE 30 7
ENDGROUP

GROUP "To"
GLOC 31 0
GSIZE 31 7
ENDGROUP

ENDTILE

ENDFORM
