BFILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 70 16 
HEADER "Advanced Spectre Simulator Preferences"

ACTIVEFIELD "Close"

TILE

TEXT "Spectre Command"
TLOC 2 3 
FGROUP "Spectre Advanced Preferences"
ENDTEXT

FIELD SpectreCommand
FLOC 20 3 
STRFILLIN 48 80
FGROUP "Spectre Advanced Preferences"
ENDFIELD

TEXT ".TRAN options:"
TLOC 2 6
FGROUP "Spectre Advanced Preferences"
ENDTEXT

FIELD SpectreTranUIC
FLOC 20 9
CHECKLIST " Use Initial Condition"
FGROUP "Spectre Advanced Preferences"
ENDFIELD

TEXT "START = "
TLOC 20 6
FGROUP "Spectre Advanced Preferences"
ENDTEXT

FIELD SpectreTranStart
FLOC 29 6
STRFILLIN 5 25
FGROUP "Spectre Advanced Preferences"
ENDFIELD

FIELD SpectreOptions
FLOC 20 12 
MENUBUTTON "Set Spectre Options..." 25 3
FGROUP "Spectre Advanced Preferences"
ENDFIELD

GROUP "Spectre Advanced Preferences"
GLOC 1 0
GSIZE 70 15
ENDGROUP

FIELD Close
FLOC 4 16
MENUBUTTON "Close" 11 3
ENDFIELD


FIELD help
FLOC 58 16
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
