FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 44 35

HEADER "Missing Fillets Report"

TILE

TEXT "File name:"
TLOC 1 2
ENDTEXT
FIELD report_name
FLOC 11 2
STRFILLIN 25 128
ENDFIELD
FIELD report_browse
FLOC 38 2
MENUBUTTON "..." 4 3
ENDFIELD

GROUP "Options"
GLOC 1 5
GSIZE 42 26
ENDGROUP 

TEXT "Scan pads of the following items:"
TLOC 3 7
ENDTEXT

FIELD check_pins
FLOC 5 9
CHECKLIST "Pins"
ENDFIELD

FIELD check_vias
FLOC 5 11
CHECKLIST "Vias"
ENDFIELD

FIELD check_fingers
FLOC 5 13
CHECKLIST "Fingers"
ENDFIELD

TEXT "Minimum size difference to require filleting:"
TLOC 3 15
ENDTEXT
FIELD use_size_tolerance
FLOC 5 17
CHECKLIST "Length:" "tolerance"
ENDFIELD
FIELD size_tolerance
FLOC 14 17
STRFILLIN 25 128
ENDFIELD
FIELD use_percent_tolerance
FLOC 5 19
CHECKLIST "Percent:" "tolerance"
ENDFIELD
FIELD percent_tolerance
FLOC 14 19
STRFILLIN 25 128
ENDFIELD

FIELD check_acid_traps
FLOC 3 22
CHECKLIST "Check for acid traps (gap between fillet and pad)"
ENDFIELD

FIELD create_drcs
FLOC 3 25
CHECKLIST "Create DRCs for missing fillets"
ENDFIELD

FIELD clear_drcs
FLOC 28 25
MENUBUTTON "Clear DRCs" 12 3
ENDFIELD


FIELD write_report
FLOC 3 27
CHECKLIST "Write report"
ENDFIELD

FIELD view_report
FLOC 3 29
CHECKLIST "View report"
ENDFIELD

FIELD report
FLOC 1 33
MENUBUTTON "Report" 9 3
ENDFIELD

FIELD close
FLOC 12 33
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 34 33
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM
