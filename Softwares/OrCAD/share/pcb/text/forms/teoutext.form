FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 73 18
HEADER "3D Output"

POPUP <xSection-pop>"XY""1","XZ""2","YZ""3".

POPUP <subclass_pop>"SUBSTRATE GEOMETRY/PACKAGE""SUBSTRATE GEOMETRY/PACKAGE".

TILE
TEXT "Z zooming factor"
TLOC 3 4
TGROUP "Geometry"
ENDTEXT

TEXT "X"
TLOC 3 11
TGROUP "Cross Section"
ENDTEXT

TEXT "Y"
TLOC 25 11
TGROUP "Cross Section"
ENDTEXT

TEXT "Z"
TLOC 47 11
TGROUP "Cross Section"
ENDTEXT

TEXT "Select Z by Class/Subclass(Optional)"
TLOC 3 14
TGROUP "Cross Section"
ENDTEXT

TEXT "Side"
TLOC 58 14
TGROUP "Cross Section"
ENDTEXT

TEXT "Time"
TLOC 3 25
TGROUP "Transient Data"
ENDTEXT

FIELD zFactor
FLOC 26 4
FGROUP "Geometry"
REALFILLIN 15 20
DECIMAL 2
ENDFIELD

FIELD x
FLOC 6 11
FGROUP "Cross Section"
STRFILLIN 13 50
ENDFIELD

FIELD y
FLOC 28 11
FGROUP "Cross Section"
STRFILLIN 13 50
ENDFIELD

FIELD z
FLOC 50 11
FGROUP "Cross Section"
STRFILLIN 13 50
ENDFIELD

FIELD subclass
FLOC 3 16
FGROUP "Cross Section"
ENUMSET 48
POP "subclass_pop"
ENDFIELD

FIELD top     
FLOC 58 16
FGROUP "Cross Section"
CHECKLIST "Top" "side"
ENDFIELD

FIELD bottom  
FLOC 58 18
FGROUP "Cross Section"
CHECKLIST "Bottom" "side"
ENDFIELD

FIELD time
FLOC 26 25
STRFILLIN 15 20
FGROUP "Transient Data"
ENDFIELD

FIELD geometry
FLOC 1 30
MENUBUTTON "Geometry" 16 3
ENDFIELD

FIELD xSection
FLOC 21 30
MENUBUTTON "XSection Map" 16 3
POP "xSection-pop"
ENDFIELD

FIELD history
FLOC 41 30
MENUBUTTON "History Map" 16 3
ENDFIELD

FIELD done
FLOC 1 33
MENUBUTTON "OK" 16 3
ENDFIELD

FIELD model
FLOC 21 33
MENUBUTTON "Model Map" 16 3
ENDFIELD

FIELD formHelp
FLOC 41 33
MENUBUTTON "Help" 16 3
ENDFIELD

GROUP "Geometry"
GLOC 2 1
GSIZE 70 6
ENDGROUP

GROUP "Cross Section"
GLOC 2 8
GSIZE 70 13
ENDGROUP

GROUP "Transient Data"
GLOC 2 22
GSIZE 70 6
ENDGROUP

ENDTILE

ENDFORM
