FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 27 24
HEADER "RF DISPLAY NEW COMPONENT"

MESSAGE warning 2 "%s"

TILE

TEXT "Operation mode:"
TLOC 1 1
ENDTEXT

FIELD highlight
FLOC 3 3
CHECKLIST "Highlight" "hl"
ENDFIELD

TEXT "Highlight color:"
TLOC 5 5
ENDTEXT

FIELD customcolor
FLOC 16 5
COLOR 2 1
ENDFIELD

FIELD dehighlight
FLOC 3 8
CHECKLIST "Dehighlight" "hl"
ENDFIELD

TEXT "Selection mode:"
TLOC 5 10
ENDTEXT

FIELD all
FLOC 7 12
CHECKLIST "All new RF components" "rg"
ENDFIELD

FIELD userselect
FLOC 7 14
CHECKLIST "By user selection" "rg"
ENDFIELD

ENDTILE

ENDFORM

