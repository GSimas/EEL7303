FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 50 8
HEADER "Interconnect Circuit Extract"

POPUP <iceFormatPop>"tlsim""0","spice""1","apv""2".

ACTIVEFIELD "Close"

TILE
TEXT "Coupling Window:"
TLOC 3 1
ENDTEXT

TEXT "Circuit Format:"
TLOC 3 3
ENDTEXT

TEXT "Output File:"
TLOC 3 9
ENDTEXT

FIELD iceWindow
FLOC 22 1
STRFILLIN 20 40
ENDFIELD

FIELD iceFormat
FLOC 22 3
STRFILLIN 20 30
POP "iceFormatPop"
ENDFIELD

FIELD iceInfileCheck
FLOC 3 5
CHECKLIST "Use Input File:"
ENDFIELD

FIELD iceInFile
FLOC 22 5
STRFILLIN 20 50
ENDFIELD

FIELD iceUseFind
FLOC 3 7
CHECKLIST "Use Find Filter."
ENDFIELD

FIELD iceOutFile
FLOC 22 9
STRFILLIN 20 50
ENDFIELD

FIELD Close
FLOC 3 12
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD iceExtract
FLOC 23 12
MENUBUTTON "Extract" 9 3
ENDFIELD

FIELD help
FLOC 36 12
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
