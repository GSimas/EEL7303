FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 38 30
HEADER "Add Interposer"

POPUP <layerPopup>"WB1""WB1","WB2""WB2".
POPUP <rotationPopup>"0 degrees""0 degrees","90 degrees""90 degrees","180 degrees""180 degrees","270 degrees""270 degrees".

TILE

TEXT "Ref ID:"
TLOC 2 2
ENDTEXT
FIELD refId
FLOC 15 2
STRFILLIN 16 28
ENDFIELD

TEXT "Symbol Name:"
TLOC 2 4
ENDTEXT
FIELD symbolName
FLOC 15 4
STRFILLIN 16 40
ENDFIELD
FIELD symbolBrowse
FLOC 34 4
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Part Number:"
TLOC 2 6
ENDTEXT
FIELD partNumber
FLOC 15 6
STRFILLIN 16 40
ENDFIELD

GROUP "Conductor Material"
GLOC 1 8 
GSIZE 37 7
ENDGROUP

TEXT "Name:"
TLOC 2 10
ENDTEXT
FIELD condMatl
FLOC 15 10
STRFILLIN 16 40
ENDFIELD
FIELD condMatlBrowse
FLOC 34 10
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Thickness:"
TLOC 2 12
ENDTEXT
FIELD condThk
FLOC 15 12
STRFILLIN 16 40
ENDFIELD

GROUP "Dielectric Material"
GLOC 1 15 
GSIZE 37 7
ENDGROUP

TEXT "Name:"
TLOC 2 17
ENDTEXT
FIELD dielMatl
FLOC 15 17
STRFILLIN 16 40
ENDFIELD
FIELD dielMatlBrowse
FLOC 34 17
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Thickness:"
TLOC 2 19
ENDTEXT
FIELD dielThk
FLOC 15 19
STRFILLIN 16 40
ENDFIELD

GROUP "Placement"
GLOC 1 22 
GSIZE 37 7
ENDGROUP

TEXT "Layer:"
TLOC 2 24
ENDTEXT
FIELD layer
FLOC 15 24
ENUMSET 15
POP "LayerPopup"
ENDFIELD

TEXT "Rotation:"
TLOC 2 26
ENDTEXT
FIELD rotation
FLOC 15 26
ENUMSET 15
POP "RotationPopup"
ENDFIELD

FIELD done
FLOC 1 31
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD place
FLOC 10 31
MENUBUTTON "Place" 9 3
ENDFIELD

FIELD cancel
FLOC 19 31
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 30 31
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM
