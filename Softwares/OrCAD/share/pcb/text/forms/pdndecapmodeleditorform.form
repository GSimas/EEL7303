FILE_TYPE=FORM_DEFN VERSION=2
    FORM
    FIXED
    PORT 62 28
    HEADER "Decoupling Capacitor Model Editor"

    ACTIVEFIELD "Done"

    TILE
    FIELD Done
      FLOC 2 26
      MENUBUTTON "Done" 10 3
    ENDFIELD

    FIELD Cancel
      FLOC 14 26
      MENUBUTTON "Cancel" 10 3
    ENDFIELD

    FIELD Help
      FLOC 51 26
      MENUBUTTON "Help" 10 3
    ENDFIELD

    FIELD ModelBitmap
        THUMBNAIL pqcapschem.bmp
        FLOC 1 1
        FSIZE 60 4
    ENDFIELD

    GROUP ""
        GLOC 1 5
        GSIZE 61 20
    ENDGROUP

    TEXT "Model Name:"
        TLOC 2 7
    ENDTEXT

    TEXT
        TLOC 21 7
        INFO ModelName 27
    ENDTEXT

    TEXT "Capacitance:"
        TLOC 2 10
    ENDTEXT

    FIELD Capacitance
      FLOC 21 10
      STRFILLIN 27 50
    ENDFIELD

    TEXT "Intrinsic inductance (ESL):"
      TLOC 2 13
    ENDTEXT

    FIELD ESL
      FLOC 21 13
      STRFILLIN 27 50
    ENDFIELD

    FIELD ESLCalc
      FLOC 50 13
      MENUBUTTON "Estimate" 10 3
    ENDFIELD

    TEXT "Intrinsic Resistance (ESR):"
      TLOC 2 16
    ENDTEXT

    FIELD ESR
      FLOC 21 16
      STRFILLIN 27 50
    ENDFIELD

    TEXT "Resonant Frequency:"
    TLOC 2 19
    ENDTEXT

    FIELD ResonantFreq
      FLOC 21 19
      STRFILLIN 27 50
    ENDFIELD

    FIELD TextEdit
      FLOC 5 22
      MENUBUTTON "Text Edit" 15 3
    ENDFIELD

    FIELD GraphResponse
      FLOC 43 22
      MENUBUTTON "Graph Response" 15 3
    ENDFIELD

    ENDTILE

ENDFORM
