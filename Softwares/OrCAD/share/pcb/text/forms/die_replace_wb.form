FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 35 40
HEADER "Wire Bond Die Replace"

TILE

TEXT "Die to replace:"
TLOC 1 1
ENDTEXT
FIELD old_die
FLOC 13 1
STRFILLIN 20 128
ENDFIELD 

GROUP "Replacement Options"
GLOC 1 3
GSIZE 33 11
ENDGROUP

FIELD match_numbers
FLOC 3 5
CHECKLIST "Reconnect wires by pin number" "wire_options"
ENDFIELD

FIELD match_nets
FLOC 3 7
CHECKLIST "Reconnect wires by net name" "wire_options"
ENDFIELD

FIELD match_location
FLOC 3 9
CHECKLIST "Reconnect wires by pin location" "wire_options"
ENDFIELD

FIELD ripup
FLOC 3 11
CHECKLIST "Rip up existing wires" "wire_options"
ENDFIELD

GROUP "Adjustment Options"
GLOC 1 14
GSIZE 33 5
ENDGROUP

FIELD realign_fingers
FLOC 3 16
CHECKLIST "Realign fingers to wires"
ENDFIELD

GROUP "Ripup Options"
GLOC 1 19
GSIZE 33 7
ENDGROUP

FIELD delete_dangling
FLOC 3 21
CHECKLIST "Delete dangling wires"
ENDFIELD 

FIELD delete_fingers
FLOC 3 23
CHECKLIST "Delete disconnected fingers"
ENDFIELD 

FIELD done
FLOC 1 27
MENUBUTTON "Ok" 9 3
ENDFIELD

FIELD help
FLOC 26 27
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM

