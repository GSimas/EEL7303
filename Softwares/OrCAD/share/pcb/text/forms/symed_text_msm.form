FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 6
HEADER "Symbol_Text_Settings"

POPUP <TEXT_BLOCK_POPUP> "" "".

TILE

GROUP "Pin Text"
GLOC 1 1
GSIZE 28 11
ENDGROUP

FIELD text_pins
TLOC 3 3
CHECKLIST "Add text labels on pins"
ENDFIELD

TEXT "Offset X:"
TLOC 3 5
ENDTEXT
FIELD text_pins_offset_x
FLOC 11 5
STRFILLIN 14 128
ENDFIELD

TEXT "Offset Y:"
TLOC 3 7
ENDTEXT
FIELD text_pins_offset_y
FLOC 11 7
STRFILLIN 14 128
ENDFIELD

TEXT "Text Size:"
TLOC 3 9
ENDTEXT
FIELD text_pins_block
FLOC 11 9
ENUMSET 13
POP "TEXT_BLOCK_POPUP"
ENDFIELD

GROUP "Border Text"
GLOC 1 13
GSIZE 28 15
ENDGROUP

FIELD text_border
TLOC 3 15
CHECKLIST "Enable border numbers."
ENDFIELD

FIELD text_left
TLOC 5 17
CHECKLIST "Left"
ENDFIELD

FIELD text_right
TLOC 15 17
CHECKLIST "Right"
ENDFIELD

FIELD text_top
TLOC 5 19
CHECKLIST "Top"
ENDFIELD

FIELD text_bottom
TLOC 15 19
CHECKLIST "Bottom"
ENDFIELD

TEXT "Offset X:"
TLOC 3 21
ENDTEXT
FIELD text_border_offset_x
FLOC 11 21
STRFILLIN 14 128
ENDFIELD

TEXT "Offset Y:"
TLOC 3 23
ENDTEXT
FIELD text_border_offset_y
FLOC 11 23
STRFILLIN 14 128
ENDFIELD

TEXT "Text Size:"
TLOC 3 25
ENDTEXT
FIELD text_border_block
FLOC 11 25
ENUMSET 13
POP "TEXT_BLOCK_POPUP"
ENDFIELD

FIELD update_text
FLOC 1 29
MENUBUTTON "Apply changes" 28 3
ENDFIELD

ENDTILE

ENDFORM

