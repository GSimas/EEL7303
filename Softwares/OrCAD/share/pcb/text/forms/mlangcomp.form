FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Microstrip Lange Coupler"

POPUP <actlayers>"""".
POPUP <gndlayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".
POPUP <via_list>"""".

TILE MLANG

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 34

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground:"
TLOC 1 3
ENDTEXT

FIELD ground_subclass
FLOC 7 3
ENUMSET 16
POP "gndlayers"
ENDFIELD

FIELD groundlayervar
FLOC 26 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Finger Width:"
TLOC 1 5
ENDTEXT

FIELD fwidthvar
FLOC 11 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD fingerwidth
FLOC 1 7
REALFILLIN 10 10
ENDFIELD

TEXT fingerwidthlabel
TLOC 14 7
INFO fingerwidthunits
ENDTEXT

TEXT "Conductor Spacing:"
TLOC 1 9
ENDTEXT

FIELD spacingvar
FLOC 15 9
MENUBUTTON "..." 3 3
ENDFIELD

FIELD conductorspacing
FLOC 1 11
REALFILLIN 10 10
ENDFIELD

TEXT conductorspacinglabel
TLOC 14 11
INFO conductorspacingunits
ENDTEXT

TEXT "Conductor Length:"
TLOC 1 13
ENDTEXT

FIELD lengthvar
FLOC 15 13
MENUBUTTON "..." 3 3
ENDFIELD

FIELD conductorlength
FLOC 1 15
REALFILLIN 10 10
ENDFIELD

TEXT conductorlengthlabel
TLOC 14 15
INFO conductorlengthunits
ENDTEXT

TEXT "Connect Line Width:"
TLOC 1 17
ENDTEXT

FIELD widthvar
FLOC 16 17
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 19
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 19
INFO swidthunits
ENDTEXT

FIELD padadded
FLOC 1 21
CHECKLIST "Add Pad"
ENDFIELD

TEXT "Via Available"
TLOC 1 23
ENDTEXT

FIELD padstack_list
FLOC 1 25
ENUMSET 17
POP "via_list"
ENDFIELD

TEXT "Rotation Lock:"
TLOC 1 27
ENDTEXT

FIELD angle
FLOC 14 27
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 29
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 3 Net:"
TLOC 1 5
ENDTEXT

TEXT
TLOC 10 5
INFO pin3netname 12
ENDTEXT

FIELD pin3netname_list
FLOC 24 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 4 Net:"
TLOC 1 7
ENDTEXT

TEXT
TLOC 10 7
INFO pin4netname 12
ENDTEXT

FIELD pin4netname_list
FLOC 24 7
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 36
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 36
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbmlang.bmp
OPTIONS STRETCH
FLOC 32 3
FSIZE 40 32
ENDFIELD

ENDTILE

ENDFORM

