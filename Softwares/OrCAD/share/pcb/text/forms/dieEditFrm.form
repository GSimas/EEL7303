FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 75 55
HEADER  "                Add I/O Driver " 

POPUP <LEFLIBPOPUP7>"""".

MESSAGE nedit 2 "Not editable while in interactive command."


TILE

TEXT "Current Library:"
TLOC 2 1
ENDTEXT

FIELD lefLibrary
FLOC 2 3
ENUMSET 21
POP "LEFLIBPOPUP7"
ENDFIELD

TEXT "Available items:" 
TLOC 2 5
ENDTEXT

FIELD tree
FLOC 2 7
TREEVIEW  23 12
ENDFIELD

GROUP "Item's data:"
FLOC 29 1
FSIZE 29 9
ENDGROUP

TEXT "Name:" 
TLOC 30 3
ENDTEXT

TEXT "" 
TLOC 45 3
INFO itemName 11
ENDTEXT

TEXT "Height:" 
TLOC 30 5
ENDTEXT

TEXT "" 
TLOC 45 5
INFO macroHeightValue 10
ENDTEXT

TEXT "Width:" 
TLOC 30 7
ENDTEXT

TEXT "" 
TLOC 45 7
INFO macroWidthValue 10
ENDTEXT

GROUP "Quick view"
FLOC 29 10
FSIZE 29 21
ENDGROUP

FIELD QuickView
THUMBNAIL
FLOC 30 12
FSIZE 27 17
ENDFIELD

FIELD LibManagerBtn
FLOC 2 32
MENUBUTTON "&Library Manager" 15 3
ENDFIELD

FIELD OkBtn
FLOC 19 32
MENUBUTTON "&OK" 9 3
ENDFIELD
 
FIELD CancelBtn
FLOC 29 32
MENUBUTTON "&Cancel" 9 3
ENDFIELD

FIELD HelpBtn
FLOC 39 32
MENUBUTTON "&Help" 9 3
ENDFIELD


FLEXMODE edgegravity
FLEX tree 0 0 1 1


ENDTILE

ENDFORM



