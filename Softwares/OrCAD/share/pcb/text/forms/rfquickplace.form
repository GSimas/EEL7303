FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 30 46
HEADER "RF Quickplace"

TILE

GROUP "Placement Position"
FLOC 1 0
FSIZE 29 21
ENDGROUP

FIELD placebyuser
FLOC 2 2
CHECKLIST "By user pick" "placeposition"
ENDFIELD

FIELD setorigin
FLOC 16 2
MENUBUTTON "Select origin" 10 3
ENDFIELD

TEXT "<xxxx.xxx xxxx.xxx>"
TLOC 4 4
INFO origin
ENDTEXT

FIELD placearoundkeepin
FLOC 2 6
CHECKLIST "Around package keepin" "placeposition"
ENDFIELD

GROUP "Edge"
FLOC 2 8
FSIZE 27 7
ENDGROUP

FIELD topedge
FLOC 12 10
CHECKLIST "Top"
ENDFIELD

FIELD leftedge
FLOC 4 11
CHECKLIST "Left"
ENDFIELD

FIELD rightedge
FLOC 21 11
CHECKLIST "Right"
ENDFIELD

FIELD bottomedge
FLOC 12 12
CHECKLIST "Bottom"
ENDFIELD

GROUP "Board Side for Non RF Components"
FLOC 2 15
FSIZE 27 5
ENDGROUP

FIELD topside
FLOC 5 17
CHECKLIST "Top" "sidemode"
ENDFIELD

FIELD bottomside
FLOC 18 17
CHECKLIST "Bottom" "sidemode"
ENDFIELD

GROUP "Placement Filter"
FLOC 1 21
FSIZE 29 25
ENDGROUP

FIELD placeall
FLOC 2 23
CHECKLIST "Place all RF-related components" "placemode"
ENDFIELD

FIELD placebyrefdes
FLOC 2 25
CHECKLIST "Place by refdes" "placemode"
ENDFIELD

FIELD placeic
FLOC 5 27
CHECKLIST "IC"
ENDFIELD

FIELD placeio
FLOC 10 27
CHECKLIST "IO"
ENDFIELD

FIELD placediscrete
FLOC 15 27
CHECKLIST "Discrete"
ENDFIELD

FIELD placerf
FLOC 24 27
CHECKLIST "RF"
ENDFIELD

FIELD placecomplist
FLOC 2 29
TREEVIEW 27 8
ENDFIELD

GROUP "Progress"
FLOC 1 46
FSIZE 29 7
ENDGROUP

TEXT "Placing RFU1..." placemsg
TLOC 2 48
FGROUP "Progress"
ENDTEXT

FIELD action
FLOC 2 50
MENUBUTTON "Start" 9 3
ENDFIELD

ENDTILE

ENDFORM
