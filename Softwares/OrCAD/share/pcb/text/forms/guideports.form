# Guideport command mini status form
#
FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 39 6
HEADER "Guideports"

TILE

GROUP "Guideport Commands"
GLOC 1 0
GSIZE 27 29
ENDGROUP

FIELD gpt_create
FLOC 2 3
MENUBUTTON "Create" 8 3
ENDFIELD

FIELD gpt_replace_all
FLOC 12 3
CHECKLIST "Replace All"
ENDFIELD

FIELD gpt_delete_all
FLOC 2 5
MENUBUTTON "Delete All" 8 3
ENDFIELD

TEXT "Select Action:"
TLOC 2 20
ENDTEXT

FIELD gpt_move
FLOC 12 20
CHECKLIST "Move" "gpt_action_group"
ENDFIELD

FIELD gpt_delete
FLOC 12 22
CHECKLIST "Delete" "gpt_action_group"
ENDFIELD

FIELD gpt_collapse_or_spread
FLOC 12 24
CHECKLIST "Collapse/Spread" "gpt_action_group"
ENDFIELD

FIELD gpt_auto_select_dp
FLOC 2 26
CHECKLIST "Auto-Select Diff Pair Mate"
ENDFIELD

TEXT "Spacing Criteria:"
TLOC 2 8
ENDTEXT

FIELD gpt_spacing_default
FLOC 2 10
CHECKLIST "Default Grid" "gpt_spacing_group"
ENDFIELD

FIELD gpt_default_value
FLOC 20 10
REALFILLIN 5 5
MIN 0
DECIMAL 0
ENDFIELD

FIELD gpt_spacing_net_lin_lin
FLOC 2 12
CHECKLIST "Min Line/Line by Net" "gpt_spacing_group"
ENDFIELD

FIELD gpt_net_value
FLOC 20 12
REALFILLIN 5 5
MIN 0
DECIMAL 0
ENDFIELD

FIELD gpt_spacing_user
FLOC 2 14
CHECKLIST "User Defined" "gpt_spacing_group"
ENDFIELD

FIELD gpt_user_value
FLOC 20 14
REALFILLIN 5 5
MIN 0
DECIMAL 0
ENDFIELD

FIELD gpt_spacing_none
FLOC 2 16
CHECKLIST "Ignore spacing rules" "gpt_spacing_group"
ENDFIELD

TEXT "Suggested Guideports:" gpt_suggest
TLOC 2 29
ENDTEXT

TEXT "Total Guideports:" gpt_actual
TLOC 2 31
ENDTEXT

ENDTILE

ENDFORM

