FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 54 12
HEADER "Drill Legend"

POPUP <UNITS_POP>"Mils""mils","Inch""inches","Microns""microns",\
"Millimeter""millimeters","Centimeter""centimeters".

TILE
TEXT "Template File:"
TLOC 1 2
ENDTEXT

TEXT "Legend Title:"
TLOC 1 5
ENDTEXT

TEXT "Output Unit:"
TLOC 1 8
ENDTEXT

TEXT "Plated Hole Tolerance:"
TLOC 1 11
ENDTEXT

TEXT "Non-Plated Hole Tolerance:"
TLOC 24 11
ENDTEXT

TEXT "BY Hole Size"
TLOC 3 17
TGROUP "HOLE SORTING METHOD"
ENDTEXT

TEXT "BY Plating Status"
TLOC 30 17
TGROUP "HOLE SORTING METHOD"
ENDTEXT

FIELD filename
FLOC 18 2
STRFILLIN 18 100
ENDFIELD

FIELD browse
FLOC 40 2
MENUBUTTON "Browse..." 11 3
ENDFIELD

FIELD title
FLOC 18 5
STRFILLIN 31 60
ENDFIELD

FIELD output_unit
FLOC 18 8
ENUMSET 12 
POP "UNITS_POP"
ENDFIELD

FIELD plate_tol
FLOC 18 11
REALFILLIN 4 6
DECIMAL 4
VALUE 0
ENDFIELD

FIELD non_plate_tol
FLOC 45 11
REALFILLIN 4 6
DECIMAL 4
VALUE 0
ENDFIELD

FIELD ascending
FLOC 4 19
FGROUP "HOLE SORTING METHOD"
CHECKLIST "Ascending" "size_order"
ENDFIELD

FIELD platedf
FLOC 30 19
FGROUP "HOLE SORTING METHOD"
CHECKLIST "Plated First" "plate_order"
ENDFIELD

FIELD descending
FLOC 4 21
FGROUP "HOLE SORTING METHOD"
CHECKLIST "Descending" "size_order"
ENDFIELD

FIELD nplatedf
FLOC 30 21
FGROUP "HOLE SORTING METHOD"
CHECKLIST "Non Plated First" "plate_order"
ENDFIELD

FIELD done
FLOC 2 26
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 14 26
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 41 26
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "HOLE SORTING METHOD"
GLOC 2 14
GSIZE 49 10
ENDGROUP

ENDTILE

ENDFORM
