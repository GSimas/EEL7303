FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 45 32
HEADER "EMS3D Preferences"

POPUP <TriangleMesh>"""".
POPUP <PrismLayer>"""".
POPUP <PortWidth>"""".
POPUP <TopABCDistance>"""".
POPUP <BotABCDistance>"""".
POPUP <NumABCLayers>"""".
POPUP <FreqSweep>"""".
POPUP <NumEdgesOnTrace>"""".

ACTIVEFIELD "Close"

TILE

TEXT "Port Width Factor"
TLOC 2 2
ENDTEXT

FIELD Ems3d_PortWidth
FLOC 32 2
STRFILLIN 7 20
POP "PortWidth"
ENDFIELD

TEXT "Top ABC Distance Ratio"
TLOC 2 4
ENDTEXT

FIELD Ems3d_TopABCDistance
FLOC 32 4
STRFILLIN 7 20
POP "TopABCDistance"
ENDFIELD

TEXT "Bottom ABC Distance Ratio"
TLOC 2 6
ENDTEXT

FIELD Ems3d_BotABCDistance
FLOC 32 6
STRFILLIN 7 20
POP "BotABCDistance"
ENDFIELD

TEXT "Number of ABC Layers"
TLOC 2 8
ENDTEXT

FIELD Ems3d_NumABCLayers
FLOC 32 8
STRFILLIN 7 20
POP "NumABCLayers"
ENDFIELD

TEXT "Triangle Mesh Level"
TLOC 2 10
ENDTEXT

FIELD Ems3d_TriangleMesh
FLOC 32 10
STRFILLIN 7 20
POP "TriangleMesh"
ENDFIELD

TEXT "Number of Prism Layers"
TLOC 2 12
ENDTEXT

FIELD Ems3d_PrismLayer
FLOC 32 12
STRFILLIN 7 20
POP "PrismLayer"
ENDFIELD

TEXT "Frequency Sweep"
TLOC 2 14
ENDTEXT

FIELD Ems3d_FreqSweep
FLOC 32 14
STRFILLIN 7 20
POP "FreqSweep"
ENDFIELD

TEXT "Number of Edges on Trace"
TLOC 2 16
ENDTEXT

FIELD Ems3d_NumEdgesOnTrace
FLOC 32 16
STRFILLIN 7 20
POP "NumEdgesOnTrace"
ENDFIELD

FIELD Ems3d_OutputSParam
FLOC 2 19
CHECKLIST "Output SParameter (.snp)"
ENDFIELD


TEXT "Extra Command Line Options:"
TLOC 2 22
ENDTEXT

FIELD Ems3d_ExtraOptions
FLOC 2 24
STRFILLIN 40 200
ENDFIELD

FIELD Ok
FLOC 2 28
MENUBUTTON "OK" 11 3
ENDFIELD

FIELD Cancel
FLOC 17 28
MENUBUTTON "Cancel" 11 3
ENDFIELD

FIELD help
FLOC 32 28
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
