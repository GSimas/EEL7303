ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 3 Net:"
TLOC 1 5
ENDTEXT

TEXT
TLOC 10 5
INFO pin3netname 12
ENDTEXT

FIELD pin3netname_list
FLOC 24 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 4 Net:"
TLOC 1 7
ENDTEXT

TEXT
TLOC 10 7
INFO pin4netname 12
ENDTEXT

FIELD pin4netname_list
FLOC 24 7
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 32
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 32
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbsclin.bmp
OPTIONS STRETCH
FLOC 32 4
FSIZE 48 26
ENDFIELD
ENDTILE

ENDFORM

