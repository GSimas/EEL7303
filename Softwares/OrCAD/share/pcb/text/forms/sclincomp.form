FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Edge-Coupled Lines"

POPUP <CouplingModes>"Right Coupling""0","Left Coupling""1"
POPUP <actlayers>"""".
POPUP <gndlayers1>"""".
POPUP <gndlayers2>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE SCLIN

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 30

TAB "Parameters"
TEXT "Ground 1:"
TLOC 1 1
ENDTEXT

FIELD ground1_subclass
FLOC 9 1
ENUMSET 14
POP "gndlayers1"
ENDFIELD

FIELD ground1layervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Layer:"
TLOC 1 3
ENDTEXT

FIELD active_subclass
FLOC 9 3
ENUMSET 14
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground 2:"
TLOC 1 5
ENDTEXT

FIELD ground2_subclass
FLOC 9 5
ENUMSET 14
POP "gndlayers2"
ENDFIELD

FIELD ground2layervar
FLOC 26 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Line Width:"
TLOC 1 7
ENDTEXT

FIELD widthvar
FLOC 10 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 9
INFO swidthunits
ENDTEXT

TEXT "Line Length:"
TLOC 1 11
ENDTEXT

FIELD lengthvar
FLOC 10 11
MENUBUTTON "..." 3 3
ENDFIELD

FIELD length
FLOC 1 13
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 13
INFO lengthunits
ENDTEXT

TEXT "Line Spacing:"
TLOC 1 15
ENDTEXT

FIELD spacingvar
FLOC 11 15
MENUBUTTON "..." 3 3
ENDFIELD

FIELD spacing
FLOC 1 17
REALFILLIN 10 10
ENDFIELD

TEXT spacingunitslabel
TLOC 14 17
INFO spacingunits
ENDTEXT

TEXT "Coupling Mode:"
TLOC 1 19
ENDTEXT

FIELD couplingmode
FLOC 1 21
ENUMSET 14
POP "CouplingModes"
ENDFIELD

TEXT "Rotation Lock:"
TLOC 1 23
ENDTEXT

FIELD angle
FLOC 14 23
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 25
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "More"
TEXT "Width at Pin1:"
TLOC 1 1
ENDTEXT

FIELD widthp1var
FLOC 12 1
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp1
FLOC 1 3
REALFILLIN 10 10
ENDFIELD

TEXT widthp1unitslabel
TLOC 14 3
INFO widthp1units
ENDTEXT

TEXT "Width at Pin2:"
TLOC 1 5
ENDTEXT

FIELD widthp2var
FLOC 12 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp2
FLOC 1 7
REALFILLIN 10 10
ENDFIELD

TEXT widthp2unitslabel
TLOC 14 7
INFO widthp2units
ENDTEXT

TEXT "Width at Pin3:"
TLOC 1 9
ENDTEXT

FIELD widthp3var
FLOC 12 9
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp3
FLOC 1 11
REALFILLIN 10 10
ENDFIELD

TEXT widthp3unitslabel
TLOC 14 11
INFO widthp3units
ENDTEXT

TEXT "Width at Pin4:"
TLOC 1 13
ENDTEXT

FIELD widthp4var
FLOC 12 13
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp4
FLOC 1 15
REALFILLIN 10 10
ENDFIELD

TEXT widthp4unitslabel
TLOC 14 15
INFO widthp4units
ENDTEXT
ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
