FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 45 40
HEADER "Add Routing Channel"

TILE

FIELD checkChannel
FLOC 1 2
CHECKLIST "Number of Routing Channels:" "choice"
ENDFIELD

FIELD checkSpacing
FLOC 1 5
CHECKLIST "Exact Spacing:" "choice"
ENDFIELD

FIELD numberChannel
FLOC 25 2
STRFILLIN 5 4
ENDFIELD

FIELD numberSpacing
FLOC 25 5
STRFILLIN 10 15
ENDFIELD

FIELD add
FLOC 8 8
MENUBUTTON "&Add" 10 3
ENDFIELD
 
FIELD cancel
FLOC 20 8
MENUBUTTON "&Cancel" 10 3
ENDFIELD

FIELD help
FLOC 32 8
MENUBUTTON "&Help" 10 3
ENDFIELD

ENDTILE
ENDFORM

