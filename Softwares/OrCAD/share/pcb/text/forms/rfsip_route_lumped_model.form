FILE_TYPE=FORM_DEFN VERSION=2

FORM fixed_font
	FIXED
	PORT 61 37
	HEADER "RFSiP Route Lumped Models"

	TILE

		TEXT "Output Model File:"
			TLOC 1 1
		ENDTEXT

		FIELD output_file
			FLOC 1 3
			STRFILLIN 54 1024
		ENDFIELD

		FIELD browse
			FLOC 58 3
			MENUBUTTON "..." 4 3
		ENDFIELD

		TEXT "Analysis Frequency"
			TLOC 1 6
		ENDTEXT

		FIELD analysisFrequency
			FLOC 26 6
			STRFILLIN 12 1024
		ENDFIELD

		GROUP ""
			GLOC 1 9
			GSIZE 60 5
		ENDGROUP

		TEXT "Virtuoso Lumped Model"
			TLOC 2 9
		ENDTEXT

		TEXT "Lib"
			TLOC 2 11
		ENDTEXT

		FIELD library
			FLOC 6 11
			STRFILLIN 12 1024
		ENDFIELD

		TEXT "Cell"
			TLOC 21 11
		ENDTEXT

		FIELD cell
			FLOC 26 11
			STRFILLIN 12 1024
		ENDFIELD

		TEXT "View"
			TLOC 41 11
		ENDTEXT

		FIELD view
			FLOC 46 11
			STRFILLIN 12 1024
		ENDFIELD

		GROUP ""
			GLOC 1 15
			GSIZE 60 8
		ENDGROUP

		FIELD testbenchSchematic
			FLOC 2 15
			CHECKLIST "Testbench Schematic"
		ENDFIELD

		TEXT "Lib"
			TLOC 2 17
		ENDTEXT

		FIELD testbenchLib
			FLOC 6 17
			STRFILLIN 12 1024
		ENDFIELD

		TEXT "Cell"
			TLOC 21 17
		ENDTEXT

		FIELD testbenchCell
			FLOC 26 17
			STRFILLIN 12 1024
		ENDFIELD

		TEXT "View"
			TLOC 41 17
		ENDTEXT

		FIELD testbenchView
			FLOC 46 17
			STRFILLIN 12 1024
		ENDFIELD

		FIELD automaticSimulation
			FLOC 2 20
			CHECKLIST "Automatically simulate testbench"
		ENDFIELD

		FIELD simulationOptions
			FLOC 40 20
			MENUBUTTON "Simulation Options" 19 3
		ENDFIELD

		GROUP ""
			GLOC 1 24
			GSIZE 60 7
		ENDGROUP

		TEXT "Create Model for"
			TLOC 2 24
		ENDTEXT

		FIELD individual_shape
			FLOC 2 26
			CHECKLIST "Selected RF shape only" "model_extent"
		ENDFIELD

		FIELD complete_route
			FLOC 2 28
			CHECKLIST "Entire RF route" "model_extent"
		ENDFIELD
	
		FIELD ok
			FLOC 1 33
			MENUBUTTON "OK" 8 3
		ENDFIELD
	
		FIELD cancel
			FLOC 11 33
			MENUBUTTON "Cancel" 8 3
		ENDFIELD

	ENDTILE

ENDFORM
