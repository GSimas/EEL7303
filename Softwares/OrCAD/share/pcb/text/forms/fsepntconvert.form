FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 27 20
HEADER "Vertex Convert"

POPUP <class>"class""class".
POPUP <subclass>"subclass""subclass".

TILE

TEXT "Active Class and Subclass:"
TLOC 1 1
ENDTEXT

FIELD class
FLOC 5 4
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "class"
ENDFIELD

FIELD subcolor
FLOC 2 7
COLOR 2 1
OPTIONS colorchooser
ENDFIELD

FIELD subclass
FLOC 5 7
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

FIELD toarc
FLOC 1 11
CHECKLIST "Convert to arc" "OPModes"
ENDFIELD

TEXT "Arc radius:"
TLOC 3 13
ENDTEXT

FIELD radius
FLOC 16 13
REALFILLIN 10 128
ENDFIELD

FIELD tomiter
FLOC 1 16 
CHECKLIST "Convert to miter" "OPModes"
ENDFIELD

TEXT "Left miter length:"
TLOC 3 18
ENDTEXT

FIELD leftmiter
FLOC 16 18
REALFILLIN 10 128
ENDFIELD

TEXT "Right miter length:"
TLOC 3 20
ENDTEXT

FIELD rightmiter
FLOC 16 20
REALFILLIN 10 128
ENDFIELD

ENDTILE

ENDFORM

