FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 73 17
HEADER "Soft Net Partition Assignment"

POPUP <PRNT_NAME_POPUP>"""".
POPUP <PIN_BOUNDARY_POPUP>"""".

MESSAGE softnet_ready 0 "Ready."
MESSAGE softnet_none_prnt 0 "No valid partitions selected."
MESSAGE softnet_apply 0 "Use the Apply button to save changes."


TILE
TEXT "Soft Nets' Owner:"
TLOC 1 1
ENDTEXT

TEXT "Name filter:"
TLOC 2 5
ENDTEXT

FIELD prtn_name
FLOC 15 1
ENUMSET 15
POP "PRNT_NAME_POPUP"
ENDFIELD

TEXT ""
TLOC 34 1
INFO use_findfilter 38
ENDTEXT

FIELD namefilter
FLOC 11 5
STRFILLIN 19 255
ENDFIELD

TEXT "Net filter:"
TLOC 2 7
ENDTEXT

FIELD pin_boundary_filter
FLOC 11 7
ENUMSET 18
POP "PIN_BOUNDARY_POPUP"
ENDFIELD

FIELD objlist
FLOC 2 9
LIST "" 30 6
OPTIONS alphanumsort prettyprint
ENDFIELD

FIELD destlist
FLOC 43 5
LIST "" 29 8
#OPTIONS ALPHANUMSORT
ENDFIELD

FIELD all_right
FLOC 34 12
MENUBUTTON "All -->" 7 3
ENDFIELD

FIELD all_left
FLOC 34 16
MENUBUTTON "<-- All" 7 3
ENDFIELD

FIELD done
FLOC 1 21
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 12 21
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD select
FLOC 42 21
MENUBUTTON "Apply" 10 3
ENDFIELD

FIELD help
FLOC 63 21
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "Available Nets" g1
FLOC 1 3
FSIZE 32 17
ENDGROUP

GROUP "Selected Nets" g2
FLOC 42 3
FSIZE 31 17
ENDGROUP

FLEXMODE edgegravityone
FLEX g1 0 0 0.5 1
FLEX g2 0.5 0 0.5 1
FLEX namefilter 0 0 1 0
FLEX valuefilter 0 0 1 0
FLEX objlist 0 0 1 1
FLEX destlist 0 0 1 0.93
FLEX all_right 0.5 0 0 0
FLEX all_left 0.5 0 0 0

ENDTILE

ENDFORM
