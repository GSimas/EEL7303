FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 64 33
HEADER "Decoupling Capacitor Editor"

POPUP <ruleTypePop>"""".

POPUP <PRINTP>"to File""0","to Printer""1","to Script""2".

POPUP <mountTypePop>"Surface""0","Leaded""1".

ACTIVEFIELD "graph"

MESSAGE badentry 0 "%s"

TILE

FIELD cap_bitmap
THUMBNAIL pqcapschem.bmp
FLOC 2 1
FSIZE 60 4
ENDFIELD

TEXT "Model:"
TLOC 2 8
TGROUP
ENDTEXT

TEXT
FLOC 24 8
INFO model_info 40
TGROUP
ENDTEXT

TEXT "Device:"
TLOC 2 10
TGROUP
ENDTEXT

FIELD device
FLOC 24 10
STRFILLIN 27 28
FGROUP 
ENDFIELD

TEXT
FLOC 24 10
INFO device_info 40
TGROUP
ENDTEXT

TEXT "Mount type:"
TLOC 2 12
TGROUP
ENDTEXT

FIELD mount_type
FLOC 24 12
STRFILLIN 24 50
POP "mountTypePop"
FGROUP 
ENDFIELD

TEXT "Capacitance:"
TLOC 2 14
TGROUP
ENDTEXT

FIELD capacitance
FLOC 24 14
STRFILLIN 27 50
FGROUP 
ENDFIELD



TEXT "Intrinsic inductance:"
TLOC 2 16
TGROUP
ENDTEXT

FIELD intrinsicInductance
FLOC 24 16
STRFILLIN 27 50
FGROUP 
ENDFIELD

FIELD intrinsicCalc
FLOC 53 16
MENUBUTTON "Estimate" 10 2
FGROUP
ENDFIELD

TEXT "Mounted inductance:"
TLOC 2 18
TGROUP
ENDTEXT

FIELD nominalInductance
FLOC 24 18
STRFILLIN 25 50
FGROUP 
ENDFIELD

TEXT "*"
TLOC 51 18
TGROUP
ENDTEXT

FIELD nominalCalc
FLOC 53 18
MENUBUTTON "Estimate" 10 2
FGROUP
ENDFIELD

TEXT "Resistance (ESR):"
TLOC 2 20
TGROUP
ENDTEXT

FIELD resistance
FLOC 24 20
STRFILLIN 27 50
FGROUP 
ENDFIELD

TEXT "Part number:"
TLOC 2 22
TGROUP
ENDTEXT

FIELD part
FLOC 24 22
STRFILLIN 27 50
FGROUP 
ENDFIELD

TEXT "Package:"
TLOC 2 24
TGROUP
ENDTEXT

FIELD pkg
FLOC 24 24
STRFILLIN 21 32
FGROUP 
ENDFIELD

FIELD pkg_browse
FLOC 47 24
MENUBUTTON "..." 5 2
FGROUP
ENDFIELD

FIELD newpkg
FLOC 53 24
MENUBUTTON "Create" 10 2
FGROUP
ENDFIELD

TEXT "Resonant frequency:"
TLOC 2 26
TGROUP
ENDTEXT


TEXT
TLOC 24 26
INFO freq 10
TGROUP
ENDTEXT

TEXT "* Value not saved"
TLOC 45 26
TGROUP
ENDTEXT

TEXT "Params for a single Y Cap" X2Y_ParamText
TLOC 2 28
TGROUP
ENDTEXT

FIELD graph
FLOC 47 28
MENUBUTTON "Graph Response" 10 2
ENDFIELD

FIELD done
FLOC 3 32
MENUBUTTON "Done" 10 3
ENDFIELD

FIELD cancel
FLOC 19 32
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 51 32
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
