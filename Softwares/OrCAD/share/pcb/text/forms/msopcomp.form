ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 38
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 38
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbmsop.bmp
