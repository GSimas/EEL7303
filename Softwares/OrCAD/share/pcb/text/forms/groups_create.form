FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 41 31
HEADER "Add to Group"

TILE

TEXT "Enter new group name:"
TLOC 2 1
ENDTEXT

FIELD name
FLOC 2 3
STRFILLIN 37 100
ENDFIELD

TEXT "Or pick group to add elements to:"
TLOC 2 6
ENDTEXT

FIELD grouplist
FLOC 2 8
LIST "" 38 9
OPTIONS alphanumsort
ENDFIELD

FIELD overwrite
FLOC 2 26
CHECKLIST "Overwrite existing group"
ENDFIELD

FIELD done
FLOC 2 29
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 14 29
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 26 29
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
