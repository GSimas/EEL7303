FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Multi-layer 190-degree Corner, Changing Width"

POPUP <actlayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE MLACRNR1

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 18

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Conductor Line Width1:"
TLOC 1 3
ENDTEXT

FIELD width1var
FLOC 18 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD width1
FLOC 1 5
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 5
INFO lengthunits
ENDTEXT

TEXT "Conductor Line Width2:"
TLOC 1 7
ENDTEXT

FIELD width2var
FLOC 18 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD width2
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 9
INFO swidthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 11
ENDTEXT

FIELD angle
FLOC 14 11
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 13
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD
ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 20
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 20
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbmlacrnr1.bmp
OPTIONS STRETCH
FLOC 32 1
FSIZE 40 20
ENDFIELD

ENDTILE

ENDFORM

