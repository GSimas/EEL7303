FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 46 7
HEADER "DIE In"

POPUP <LAYERPOPUP>"""".

ACTIVEFIELD "filename"
TILE
TEXT "DIE file name [.die]:"
TLOC 1 1
ENDTEXT

FIELD diefile
FLOC 18 1
STRFILLIN 19 1024
ENDFIELD

FIELD browse
FLOC 40 1
MENUBUTTON "..." 5 3
ENDFIELD

TEXT "Reference designator:"
TLOC 1 4
INFO refdes_label 30
ENDTEXT

FIELD refdes
FLOC 18 4
STRFILLIN 19 19
ENDFIELD

GROUP "Placement Information "
GLOC 1 7
GSIZE 43 9
ENDGROUP

TEXT "Die pad layer:"
TLOC 3 9
INFO refdes_label 30
ENDTEXT

FIELD pad_layer
FLOC 18 9
ENUMSET 18
POP "LAYERPOPUP"
ENDFIELD

FIELD flip_chip
FLOC 3 11 
CHECKLIST "Flip placed die"
ENDFIELD

FIELD die_center
FLOC 3 13
CHECKLIST "Use die center as origin"
ENDFIELD

FIELD done
FLOC 1 18
MENUBUTTON "OK" 11 3
ENDFIELD

FIELD cancel
FLOC 14 18
MENUBUTTON "Cancel" 11 3
ENDFIELD

FIELD help
FLOC 35 18
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
