FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 49 7
HEADER "BATCH DRC"

MESSAGE dnexist 0 "That directory does not exist."
MESSAGE nexist 0 "That file does not exist."
MESSAGE ndone 0 "Batch DRC completed."
MESSAGE nerror 0 "Batch DRC Not completed successfully."
MESSAGE inprogress 0 "Batch DRC in progress - please wait."
MESSAGE nfile 0 "You must enter a file name first."

ACTIVEFIELD "cancel"

TILE
TEXT "Input Design:"
TLOC 1 0
ENDTEXT

TEXT "Output Design:"
TLOC 1 5
ENDTEXT

FIELD execute
FLOC 37 1
MENUBUTTON "Run" 11 3
ENDFIELD

FIELD filename
FLOC 2 2
STRFILLIN 30 128
ENDFIELD

FIELD cancel
FLOC 37 4
MENUBUTTON "Close" 11 3
ENDFIELD

FIELD outputfile
FLOC 2 7
STRFILLIN 30 128
ENDFIELD

FIELD browse
FLOC 37 7
MENUBUTTON "Browse..." 11 3
ENDFIELD

FIELD help
FLOC 37 11
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
