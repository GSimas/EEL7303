FILE_TYPE=FORM_DEFN VERSION=2

    FORM
    FIXED
    PORT 29 22
    HEADER "Current display"

    TILE

    FIELD GeneralTab
        FLOC 1 0
        CHECKLIST "General" "DisplayTab"
    ENDFIELD

    FIELD ColorLegendTab
        FLOC 11 0
        CHECKLIST "Color Legend" "DisplayTab"
    ENDFIELD

    TEXT "Analysis:" "SimModeTitle"
        TLOC 1 3
    ENDTEXT
    TEXT
        TLOC 11 3
        INFO SimMode 28
    ENDTEXT

    TEXT "Mesh mode:" "MeshModeTitle"
        TLOC 1 5
    ENDTEXT

    TEXT
        TLOC 11 5
        INFO MeshRectMode 28
    ENDTEXT

    TEXT "X size:" "MeshRectSizeXTitle"
        TLOC 1 7
    ENDTEXT

    TEXT
        TLOC 11 7
        INFO MeshRectSizeX 28
    ENDTEXT

    TEXT "Y yize:" "MeshRectSizeYTitle"
        TLOC 1 9
    ENDTEXT

    TEXT
        TLOC 11 9
        INFO MeshRectSizeY 28
    ENDTEXT

    TEXT "Shape:" "ShapeTitle"
        TLOC 1 11
    ENDTEXT

    TEXT
        TLOC 11 11
        INFO MeshShapeAutoVoid 28
    ENDTEXT

    TEXT "Review:" "ReviewTitle"
        TLOC 1 16
    ENDTEXT

    POPUP <ReviewLayerPopCurrent>"""".
    FIELD ReviewLayer
        FLOC 11 16
        ENUMSET 15
        OPTIONS prettyprint ownerdrawn
        POP "ReviewLayerPopCurrent"
    ENDFIELD

    FIELD VisEtch
        FLOC 11 19
        CHECKLIST "Etch"
    ENDFIELD

    FIELD VisVia
        FLOC 17 19
        CHECKLIST "Via"
    ENDFIELD

    FIELD VisPin
        FLOC 23 19
        CHECKLIST "Pin"
    ENDFIELD

    TEXT "Surface:" "SurfaceTitle"
        TLOC 1 21
    ENDTEXT

    FIELD SurfaceTop
        FLOC 11 21
        CHECKLIST "Top" "Surface"
    ENDFIELD

    FIELD SurfaceBottom
        FLOC 18 21
        CHECKLIST "Bottom" "Surface"
    ENDFIELD

    TEXT "Net:" "NetInfoTitle"
        TLOC 1 25
    ENDTEXT

    TEXT
        TLOC 11 25
        INFO NetInfo 28
    ENDTEXT

    TEXT "Current:" "CurrentTitle"
        TLOC 1 27
    ENDTEXT

    TEXT
        TLOC 11 27
        INFO Current 256
    ENDTEXT

    TEXT "Time:" "TimeTitle"
        TLOC 1 29
    ENDTEXT

    TEXT
        TLOC 11 29
        INFO Time 256
    ENDTEXT

    FIELD Custom
        FLOC 1 3
        MENUBUTTON "Custom" 10 3
    ENDFIELD

    GRID LegendGrid
    FLOC 1 5
    FSIZE 25 26
    OPTIONS HLINES VLINES

    GHEAD TOP
        HEADSIZE 0
    ENDGHEAD
    GHEAD SIDE
        HEADSIZE 0
    ENDGHEAD
    ENDGRID
    ENDTILE
ENDFORM
