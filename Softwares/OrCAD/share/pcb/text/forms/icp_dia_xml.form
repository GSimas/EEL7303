FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 60 34
HEADER "Convert Dia to Xml"

TILE

TEXT "Die abstract (.dia) file:"
TLOC 3 2
ENDTEXT

FIELD diaFile
FLOC 3 4
STRFILLIN 50 1024
ENDFIELD

FIELD diaBrowse
FLOC 55 4
MENUBUTTON "..." 4 3
ENDFIELD

TEXT "Text (.xml) file:"
TLOC 3 7
ENDTEXT

FIELD xmlFile
FLOC 3 9
STRFILLIN 50 1024
ENDFIELD

FIELD xmlBrowse
FLOC 55 9
MENUBUTTON "..." 4 3
ENDFIELD

FIELD write
FLOC 1 13
MENUBUTTON "Write" 9 3
ENDFIELD

FIELD close
FLOC 13 13
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 50 13
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM


