FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 62 34
HEADER "Finger Padstack"

POPUP <PADPOPUP>"""".
POPUP <LAYERPOPUP>"""".

TILE

GROUP "Method"
GLOC 3 1
GSIZE 58 7
ENDGROUP

FIELD pad_create
FLOC 5 3
CHECKLIST "New" "pad_method"
ENDFIELD

FIELD pad_library
FLOC 5 5
CHECKLIST "Load from disk:" "pad_method"
ENDFIELD

FIELD pad_file_name
FLOC 22 5
STRFILLIN 20 128
ENDFIELD

FIELD pad_file_browse
FLOC 45 5
MENUBUTTON "Browse" 9 3
ENDFIELD

GROUP "Specifications"
GLOC 3 9
GSIZE 58 17
ENDGROUP

TEXT "Name:"
TLOC 5 11
ENDTEXT

FIELD pad_name
FLOC 22 11
STRFILLIN 20 128
ENDFIELD

TEXT "Layer:"
TLOC 5 13
ENDTEXT

FIELD pad_layer
FLOC 22 13
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "LAYERPOPUP"
ENDFIELD

GROUP "Padstack"
GLOC 5 15
GSIZE 54 10
ENDGROUP

GROUP "Shape"
GLOC 7 17
GSIZE 16 7
ENDGROUP

FIELD pad_oblong
FLOC 9 19
CHECKLIST "Oblong" "pad_type"
ENDFIELD

FIELD pad_rectangle
FLOC 9 21
CHECKLIST "Rectangle" "pad_type"
ENDFIELD

GROUP "Dimensions"
GLOC 26 17
GSIZE 32 7
ENDGROUP

TEXT "Width:"
TLOC 28 19
ENDTEXT
FIELD pad_width
FLOC 34 19
STRFILLIN 20 128
ENDFIELD

TEXT "Height:"
TLOC 28 21
ENDTEXT
FIELD pad_height
FLOC 34 21
STRFILLIN 20 128
ENDFIELD

FIELD done
FLOC 3 28
MENUBUTTON "Ok" 9 3
ENDFIELD

FIELD cancel
FLOC 15 28
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 52 28
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM

