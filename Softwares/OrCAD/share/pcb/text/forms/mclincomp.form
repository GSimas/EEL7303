FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 80 20
HEADER "Microstrip Coupled Line"

POPUP <CouplingModes>"Right Coupling""0","Left Coupling""1"
POPUP <actlayers>"""".
POPUP <gndlayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE MCLIN

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 28

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground:"
TLOC 1 3
ENDTEXT

FIELD ground_subclass
FLOC 7 3
ENUMSET 16
POP "gndlayers"
ENDFIELD

FIELD groundlayervar
FLOC 26 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Line Width:"
TLOC 1 5
ENDTEXT

FIELD widthvar
FLOC 10 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 7
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 7
INFO swidthunits
ENDTEXT

TEXT "Line Length:"
TLOC 1 9
ENDTEXT

FIELD lengthvar
FLOC 10 9
MENUBUTTON "..." 3 3
ENDFIELD

FIELD length
FLOC 1 11
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 11
INFO lengthunits
ENDTEXT

TEXT "Line Spacing:"
TLOC 1 13
ENDTEXT

FIELD spacingvar
FLOC 11 13
MENUBUTTON "..." 3 3
ENDFIELD

FIELD spacing
FLOC 1 15
REALFILLIN 10 10
ENDFIELD

TEXT spacingunitslabel
TLOC 14 15
INFO spacingunits
ENDTEXT

TEXT "Coupling Mode:"
TLOC 1 17
ENDTEXT

FIELD couplingmode
FLOC 1 19
ENUMSET 14
POP "CouplingModes"
ENDFIELD

TEXT "Rotation Lock:"
TLOC 1 21
ENDTEXT

FIELD angle
FLOC 14 21
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 23
CHECKLIST "New trace after insertion"
ENDFIELD
ENDTAB

TAB "More"
TEXT "Width at Pin1:"
TLOC 1 1
ENDTEXT

FIELD widthp1var
FLOC 12 1
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp1
FLOC 1 3
REALFILLIN 10 10
ENDFIELD

TEXT widthp1unitslabel
TLOC 14 3
INFO widthp1units
ENDTEXT

TEXT "Width at Pin2:"
TLOC 1 5
ENDTEXT

FIELD widthp2var
FLOC 12 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp2
FLOC 1 7
REALFILLIN 10 10
ENDFIELD

TEXT widthp2unitslabel
TLOC 14 7
INFO widthp2units
ENDTEXT

TEXT "Width at Pin3:"
TLOC 1 9
ENDTEXT

FIELD widthp3var
FLOC 12 9
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp3
FLOC 1 11
REALFILLIN 10 10
ENDFIELD

TEXT widthp3unitslabel
TLOC 14 11
INFO widthp3units
ENDTEXT

TEXT "Width at Pin4:"
TLOC 1 13
ENDTEXT

FIELD widthp4var
FLOC 12 13
MENUBUTTON "..." 3 3
ENDFIELD

FIELD widthp4
FLOC 1 15
REALFILLIN 10 10
ENDFIELD

TEXT widthp4unitslabel
TLOC 14 15
INFO widthp4units
ENDTEXT
ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 3 Net:"
TLOC 1 5
ENDTEXT

TEXT
TLOC 10 5
INFO pin3netname 12
ENDTEXT

FIELD pin3netname_list
FLOC 24 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 4 Net:"
TLOC 1 7
ENDTEXT

TEXT
TLOC 10 7
INFO pin4netname 12
ENDTEXT

FIELD pin4netname_list
FLOC 24 7
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 30
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 30
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbmclin.bmp
OPTIONS STRETCH
FLOC 32 6
FSIZE 40 18
ENDFIELD

ENDTILE

ENDFORM

