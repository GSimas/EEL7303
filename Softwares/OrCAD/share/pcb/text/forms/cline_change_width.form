FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 52 6
HEADER "Cline Change Width"

TILE

FIELD single_trace_mode
FLOC 1 1
CHECKLIST "Single trace mode (two-pick)" "mode"
ENDFIELD

FIELD window_mode
FLOC 1 3
CHECKLIST "Multi trace mode (window)" "mode"
ENDFIELD

FIELD filter_width
FLOC 1 6
CHECKLIST "Filter by cline width"
ENDFIELD 
TEXT "Minimum:"
TLOC 3 8
ENDTEXT
FIELD min_width
FLOC 10 8
STRFILLIN 15 128
ENDFIELD
TEXT "Maximum:"
TLOC 3 10
ENDTEXT
FIELD max_width
FLOC 10 10
STRFILLIN 15 128
ENDFIELD

TEXT "New width:"
TLOC 1 13
ENDTEXT
FIELD cline_width
FLOC 10 13
STRFILLIN 15 128
ENDFIELD

FIELD maximize_width
FLOC 1 15
CHECKLIST "Maximize line width"
ENDFIELD

FIELD remove
FLOC 1 17
CHECKLIST "Remove selected part of cline(s)"
ENDFIELD

ENDTILE

ENDFORM
