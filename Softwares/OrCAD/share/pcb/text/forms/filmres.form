FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 67 6
HEADER "Thick/Thin Film Resistor Generator Controls"


ACTIVEFIELD "cntrl_file"

TILE
TEXT "Resistor Control File:"
TLOC 1 1
ENDTEXT

TEXT "Text Block for Symbol text:"
TLOC 1 4
ENDTEXT

FIELD cntrl_file
FLOC 29 1
STRFILLIN 20 128
ENDFIELD

FIELD browse
FLOC 54 1
MENUBUTTON "Browse..." 11 3
ENDFIELD

FIELD text_block
FLOC 29 4
INTSLIDEBAR 3 3
MIN 1
MAX 64
ENDFIELD

FIELD execute
FLOC 2 8
MENUBUTTON "OK" 11 3
ENDFIELD

FIELD close
FLOC 17 8
MENUBUTTON "Close" 11 3
ENDFIELD

FIELD help
FLOC 32 8
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
