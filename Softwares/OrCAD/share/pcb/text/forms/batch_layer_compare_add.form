FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 15

HEADER "Batch Compare Layers"

POPUP <master_class_popup>     "" "".
POPUP <master_subclass_popup>  "" "".
POPUP <comp_class_popup>       "" "".
POPUP <comp_subclass_popup>    "" "".
POPUP <dest_class_popup>       "" "".
POPUP <dest_subclass_popup>    "" "".

TILE

TEXT "Class:"
TLOC 1 4
ENDTEXT
TEXT "Subclass:"
TLOC 1 6
ENDTEXT

TEXT "Master"
TLOC 10 1
ENDTEXT
FIELD master_class
FLOC 10 4
ENUMSET 20
POP "master_class_popup"
ENDFIELD
FIELD master_subclass
FLOC 10 6
ENUMSET 20
POP "master_subclass_popup"
ENDFIELD 

TEXT "Compare"
TLOC 35 1
ENDTEXT
FIELD comp_class
FLOC 35 4
ENUMSET 20
POP "comp_class_popup"
ENDFIELD
FIELD comp_subclass
FLOC 35 6
ENUMSET 20
POP "comp_subclass_popup"
ENDFIELD 

TEXT "Destination"
TLOC 60 1
ENDTEXT
FIELD dest_class
FLOC 60 4
ENUMSET 20
POP "dest_class_popup"
ENDFIELD
FIELD dest_subclass
FLOC 60 6
STRFILLIN 18 1024
POP "dest_subclass_popup"
ENDFIELD

FIELD done
FLOC 1 10
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD cancel
FLOC 11 10
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 74 10
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM
