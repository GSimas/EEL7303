FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 68 60
HEADER "SMT Library Translator - Components"

POPUP <PART_TYPE> "Resistor""Resistor", "Capacitor""Capacitor", "Inductor""Inductor", "Diode""Diode", "Triode""Triode", "SMT IC""SMT IC".

TILE

	GROUP "Package List"
		FLOC 2 0
		FSIZE 33 36
	ENDGROUP
	
	FIELD ComponentTreeList
        FLOC 5 3
        TREEVIEW  28 16
    ENDFIELD

	GROUP "Footprint"
		FLOC 38 0
		FSIZE 28 18
	ENDGROUP
	
	FIELD MAPPING_MODE
		FLOC 40 3
		CHECKLIST "Mapping mode"
	ENDFIELD
	
	TEXT "JEDEC type:"
		FLOC 40 6
	ENDTEXT
	
	FIELD JEDEC_TYPE
        FLOC 40 8
        STRFILLIN 16 256
    ENDFIELD
    
    FIELD JEDEC_TYPE_BROWSER
    	FLOC 59 8
    	MENUBUTTON "..." 5 3
    ENDFIELD
    
    TEXT "Padstack:"
    	FLOC 40 11
	ENDTEXT
	
	FIELD PADSTACK
		FLOC 40 13
		STRFILLIN 22 256
	ENDFIELD
	
	TEXT "Current component:"
		FLOC 38 20
	ENDTEXT
	
	FIELD CurComp
		FLOC 38 22
		STRFILLIN 26 256
	ENDFIELD
	
	FIELD BACK
		FLOC 3 37
		MENUBUTTON "< Back" 9 3
	ENDFIELD
	
	FIELD NEXT
		FLOC 18 37
		MENUBUTTON "Translate" 9 3
	ENDFIELD
	
	FIELD Cancel
		FLOC 35 37
		MENUBUTTON "Cancel" 9 3
	ENDFIELD
	
	FIELD HELP
		FLOC 50 37
		MENUBUTTON "Help" 9 3
	ENDFIELD

ENDTILE

ENDFORM