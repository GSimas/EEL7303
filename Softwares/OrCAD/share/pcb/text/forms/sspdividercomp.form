FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 44
HEADER "Single-Section Power Divider"

POPUP <actlayers>"subclass""subclass".
POPUP <gndlayers1>"""".
POPUP <gndlayers2>"""".
POPUP <via_list>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE SSPDIVIDER

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 48

TAB "Parameters"
TEXT "Ground 1:"
TLOC 1 1
ENDTEXT

FIELD ground1_subclass
FLOC 10 1
ENUMSET 15
POP "gndlayers1"
ENDFIELD

TEXT "Layer:"
TLOC 1 3
ENDTEXT

FIELD active_subclass
FLOC 10 3
ENUMSET 15
POP "actlayers"
ENDFIELD

TEXT "Ground 2:"
TLOC 1 5
ENDTEXT

FIELD ground2_subclass
FLOC 10 5
ENUMSET 15
POP "gndlayers2"
ENDFIELD

FIELD padadded
FLOC 1 7
CHECKLIST "Add Pad"
ENDFIELD

TEXT "Via Available"
TLOC 1 9
ENDTEXT

FIELD padstack_list
FLOC 1 11
ENUMSET 17
POP "via_list"
ENDFIELD

TEXT "Line Width:"
TLOC 1 13
ENDTEXT

FIELD swidth
FLOC 1 15
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 15
INFO swidthunits
ENDTEXT

TEXT "Line Length:"
TLOC 1 17
ENDTEXT

FIELD length
FLOC 1 19
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 19
INFO lengthunits
ENDTEXT

TEXT "Line Width 1:"
TLOC 1 21
ENDTEXT

FIELD swidth1
FLOC 1 23
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel1
TLOC 14 23
INFO swidthunits1
ENDTEXT

TEXT "Line Length 1:"
TLOC 1 25
ENDTEXT

FIELD length1
FLOC 1 27
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel1
TLOC 14 27
INFO lengthunits1
ENDTEXT

TEXT "Line Length 2:"
TLOC 1 29
ENDTEXT

FIELD length2
FLOC 1 31
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel2
TLOC 14 31
INFO lengthunits2
ENDTEXT

TEXT "Angle:"
TLOC 1 33
ENDTEXT

FIELD angle1
FLOC 1 35
REALFILLIN 10 10
ENDFIELD

TEXT "Angle 1:"
TLOC 1 37
ENDTEXT

FIELD angle2
FLOC 1 39
REALFILLIN 10 10
ENDFIELD

TEXT "Rotation Lock:"
TLOC 1 41
ENDTEXT

FIELD angle
FLOC 14 41
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 43
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 3 Net:"
TLOC 1 5
ENDTEXT

TEXT
TLOC 10 5
INFO pin3netname 12
ENDTEXT

FIELD pin3netname_list
FLOC 24 5
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 50
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 50
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbsspdivider.bmp
OPTIONS STRETCH
FLOC 32 8
FSIZE 54 30
ENDFIELD
ENDTILE

ENDFORM

 