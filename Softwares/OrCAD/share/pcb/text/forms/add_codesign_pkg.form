FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 48 6
HEADER "Add Package on Package"

TILE

FIELD create
FLOC 1 1
CHECKLIST "Start new package on package" "mode"
ENDFIELD

FIELD new_name
FLOC 5 3
STRFILLIN 28 1024
ENDFIELD

FIELD new_browse
FLOC 36 3
MENUBUTTON "Browse..." 10 3
ENDFIELD

FIELD import
FLOC 1 5
CHECKLIST "Add existing package on package" "mode"
ENDFIELD

FIELD old_name
FLOC 5 7
STRFILLIN 28 1024
ENDFIELD

FIELD old_browse
FLOC 36 7
MENUBUTTON "Browse..." 10 3
ENDFIELD

FIELD done
FLOC 1 11
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 12 11
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 36 11
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
