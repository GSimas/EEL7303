FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Arbitrary Angle Mitered Bend"

POPUP <actlayers>"""".
POPUP <gndlayers1>"""".
POPUP <gndlayers2>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".
POPUP <frequnits>"Hz""1","KHz""1000","MHz""1000000","GHz""1000000000".

TILE SBEND2

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 32

TAB "Parameters"
TEXT "Ground 1:"
TLOC 1 1
ENDTEXT

FIELD ground1_subclass
FLOC 9 1
ENUMSET 14
POP "gndlayers1"
ENDFIELD

FIELD ground1layervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Layer:"
TLOC 1 3
ENDTEXT

FIELD active_subclass
FLOC 9 3
ENUMSET 14
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground 2:"
TLOC 1 5
ENDTEXT

FIELD ground2_subclass
FLOC 9 5
ENUMSET 14
POP "gndlayers2"
ENDFIELD

FIELD ground2layervar
FLOC 26 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Miter Fraction:"
TLOC 1 7
ENDTEXT

FIELD mitervar
FLOC 12 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD miterfraction
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT "Angle:"
TLOC 1 11
ENDTEXT

FIELD sangle
FLOC 9 11
REALFILLIN 10 10
ENDFIELD

FIELD anglevar
FLOC 21 11
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Line Width:"
TLOC 1 13
ENDTEXT

FIELD widthvar
FLOC 10 13
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 15
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 15
INFO swidthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 17
ENDTEXT

FIELD angle
FLOC 14 17
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

TEXT "Working Frequency:"
TLOC 1 19
ENDTEXT

FIELD frequency
FLOC 1 21
REALFILLIN 10 10
REALMIN 0.0
DECIMAL 10
ENDFIELD

FIELD frequnit
FLOC 13 21
ENUMSET 6
POP "frequnits"
ENDFIELD

TEXT "Electrical Length:"
TLOC 1 23
ENDTEXT

TEXT
FLOC 1 25
INFO electricallength 12
ENDTEXT

TEXT
TLOC 14 25
INFO electricallengthunits 6
ENDTEXT

FIELD newtrace
FLOC 1 27
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD
ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 34
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 34
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbsbend2.bmp
OPTIONS STRETCH
FLOC 32 3
FSIZE 42 28
ENDFIELD
ENDTILE

ENDFORM

