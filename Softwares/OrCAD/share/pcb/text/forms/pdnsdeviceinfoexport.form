FILE_TYPE=FORM_DEFN VERSION=2

    FORM
    FIXED
    PORT 50 8
    HEADER "Device Info Export"

    ACTIVEFIELD FilePath

    TILE
        TEXT "Export to:"
            TLOC 1 1
        ENDTEXT
        
        FIELD FilePath
            FLOC 8 1
            STRFILLIN 35 512
        ENDFIELD
        
        FIELD FileBrowser
            FLOC 45 1
            MENUBUTTON "..." 5 3
        ENDFIELD
        
        FIELD Export
            FLOC 40 4
            MENUBUTTON "Export" 10 3
        ENDFIELD
        
        FIELD Done
            FLOC 29 4
            MENUBUTTON "Close" 10 3
        ENDFIELD
        
    ENDTILE
    
ENDFORM
