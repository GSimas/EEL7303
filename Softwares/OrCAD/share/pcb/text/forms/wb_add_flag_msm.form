FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 29 40
HEADER "Add Die Flag"

POPUP <LAYERPOPUP>"TOP_COND""TOP_COND".
POPUP <NETPOPUP>"GROUND""GROUND".
POPUP <STYLEPOPUP>"Solid""SOLID", "Wheel" "WHEEL".
POPUP <CENTERPOPUP>"Circle""CIRCLE", "Octagon" "OCTAGON", "Square" "SQUARE".

TILE

TEXT "Layer:"
TLOC 1 1
ENDTEXT
FIELD flag_layer
FLOC 10 1
ENUMSET 19
OPTIONS prettyprint
POP "LAYERPOPUP"
ENDFIELD

TEXT "Net:"
TLOC 1 3
ENDTEXT
FIELD flag_net
FLOC 10 3
ENUMSET 19
OPTIONS prettyprint
POP "NETPOPUP"
ENDFIELD

FIELD group_with_die
FLOC 1 5
CHECKLIST "Group flag with die symbol"
ENDFIELD

FIELD soldermask_create
FLOC 1 7
CHECKLIST "Solder opening:"
ENDFIELD
FIELD soldermask_dist
FLOC 15 7
STRFILLIN 15 1024
ENDFIELD

TEXT "Style:"
TLOC 1 10
ENDTEXT
FIELD flag_style
FLOC 10 10
ENUMSET 19
OPTIONS prettyprint
POP "STYLEPOPUP"
ENDFIELD


TEXT "Edge dist:"
TLOC 1 12
ENDTEXT
FIELD edge_dist
FLOC 10 12
STRFILLIN 20 1024
ENDFIELD
FIELD edge_round
FLOC 1 14
CHECKLIST "Round corners"
ENDFIELD

TEXT "Ring width:"
TLOC 1 17
ENDTEXT
FIELD ring_width
FLOC 10 17
STRFILLIN 20 1024
ENDFIELD

TEXT "Center area:"
TLOC 1 19
ENDTEXT
TEXT "Shape:"
TLOC 3 21
ENDTEXT
FIELD center_shape
FLOC 10 21
ENUMSET 19
OPTIONS prettyprint
POP "CENTERPOPUP"
ENDFIELD
TEXT "Width:"
TLOC 3 23
ENDTEXT
FIELD center_width
FLOC 10 23
STRFILLIN 20 1024
ENDFIELD
TEXT "Height:"
TLOC 3 25
ENDTEXT
FIELD center_height
FLOC 10 25
STRFILLIN 20 1024
ENDFIELD

TEXT "Spokes:"
TLOC 1 27
ENDTEXT
TEXT "Width:"
TLOC 3 29
ENDTEXT
FIELD spoke_width
FLOC 10 29
STRFILLIN 20 1024
ENDFIELD
TEXT "Count:"
TLOC 3 31
ENDTEXT
FIELD spoke_count
FLOC 10 31
INTFILLIN 20 1024
MIN 2
MAX 50
ENDFIELD

FIELD create
FLOC 1 36
MENUBUTTON "Create Flag" 30 3
ENDFIELD

ENDTILE
ENDFORM

