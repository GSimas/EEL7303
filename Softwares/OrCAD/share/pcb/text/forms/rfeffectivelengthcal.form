FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 30 40
HEADER "Wavelength Convertion"

POPUP <actlayers>"""".
POPUP <gndlayers1>"""".
POPUP <gndlayers2>"""".
POPUP <frequnits>"Hz""1","KHz""1000","MHz""1000000","GHz""1000000000".

TILE

TEXT "Ground 1:"
TLOC 1 1
ENDTEXT

FIELD ground1_subclass
FLOC 12 1
ENUMSET 15
POP "gndlayers1"
ENDFIELD

TEXT "Layer:"
TLOC 1 3
ENDTEXT

FIELD active_subclass
FLOC 12 3
ENUMSET 15
POP "actlayers"
ENDFIELD

TEXT "Ground 2:"
TLOC 1 5
ENDTEXT

FIELD ground2_subclass
FLOC 12 5
ENUMSET 15
POP "gndlayers2"
ENDFIELD

TEXT "Frequency:"
TLOC 1 7
ENDTEXT

FIELD frequency
FLOC 1 9
REALFILLIN 20 20
DECIMAL 5
ENDFIELD

FIELD frequnit
FLOC 23 9
ENUMSET 4
POP "frequnits"
ENDFIELD

TEXT "Wavelength:"
TLOC 1 11
ENDTEXT

FIELD wavelength
FLOC 1 13
REALFILLIN 20 20
DECIMAL 5
ENDTEXT

TEXT "Meters"
TLOC 23 13
ENDTEXT

TEXT "Ratio:"
TLOC 1 15
ENDTEXT

FIELD ratio
FLOC 1 17
REALFILLIN 10 10
DECIMAL 5
ENDFIELD

TEXT "Conductor Width:"
TLOC 1 19
ENDTEXT

FIELD width
FLOC 1 21
REALFILLIN 20 20
DECIMAL 5
ENDTEXT

TEXT
TLOC 23 21
INFO widthunit 5
ENDTEXT

TEXT "Effective Length:"
TLOC 1 23
ENDTEXT

FIELD length
FLOC 1 25
REALFILLIN 20 20
DECIMAL 5
ENDTEXT

TEXT
TLOC 23 25
INFO lengthunit 5
ENDTEXT

FIELD Done
FLOC 1 28
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD help
FLOC 21 28
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
