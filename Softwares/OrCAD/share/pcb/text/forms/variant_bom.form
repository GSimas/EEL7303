FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 40 9
HEADER "Create Variant BOM"

POPUP <VAR_LIST>"""".
 
TILE

TEXT "Variant:"
TLOC 1 1
ENDTEXT 

FIELD variant_list
FLOC 10 1
ENUMSET 20
POP "var_list"
ENDFIELD 
 
FIELD dni_option
CHECKLIST "Include Components Not Installed"
FLOC 1 4
ENDFIELD

FIELD done
FLOC 1 7
MENUBUTTON "OK" 10 3
ENDFIELD 
 
FIELD cancel
FLOC 13 7
MENUBUTTON "Cancel" 10 3
ENDFIELD 
 
FIELD help
FLOC 29 7
MENUBUTTON "Help" 10 3
ENDFIELD 
 
ENDTILE

ENDFORM




