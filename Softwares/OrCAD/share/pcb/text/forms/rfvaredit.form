FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 50 20
HEADER "Variable Editing"

TILE VAREDIT_MINI

TEXT "Variable Definition File:"
TLOC 1 1
ENDTEXT

FIELD varfile
FLOC 1 3
STRFILLIN 47 256
ENDFIELD

TEXT "Variable List"
TLOC 1 5
ENDTEXT

FIELD varlist
FLOC 1 7
TREEVIEW 48 8
ENDFIELD

TEXT "Variable:"
TLOC 1 23
ENDTEXT

TEXT "X"
TLOC 8 23
INFO varname 32
ENDTEXT

TEXT "Value:"
TLOC 1 25
ENDTEXT

TEXT "RefCount:"
TLOC 34 23
ENDTEXT

TEXT "1"
TLOC 42 23
INFO varcount 8
ENDTEXT

FIELD varvalue
FLOC 8 25
STRFILLIN 36 256
ENDFIELD

FIELD eqbtn
FLOC 46 25
MENUBUTTON "..." 3 3
ENDFIELD

FIELD Done
FLOC 1 28
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD Cancel
FLOC 12 28
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 40 28
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
