FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 90 30
HEADER "Rat-Race Matched Hybrid"

POPUP <actlayers>"subclass""subclass".
POPUP <gndlayers1>"""".
POPUP <gndlayers2>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE RHYBRID

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 34

TAB "Parameters"
TEXT "Ground 1:"
TLOC 1 1
ENDTEXT

FIELD ground1_subclass
FLOC 10 1
ENUMSET 15
POP "gndlayers1"
ENDFIELD

TEXT "Layer:"
TLOC 1 3
ENDTEXT

FIELD active_subclass
FLOC 10 3
ENUMSET 15
POP "actlayers"
ENDFIELD

TEXT "Ground 2:"
TLOC 1 5
ENDTEXT

FIELD ground2_subclass
FLOC 10 5
ENUMSET 15
POP "gndlayers2"
ENDFIELD

TEXT "Radius:"
TLOC 1 7
ENDTEXT

FIELD radius
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT radiusunitslabel
TLOC 14 9
INFO radiusunits
ENDTEXT

TEXT "Stub Width:"
TLOC 1 11
ENDTEXT

FIELD swidth
FLOC 1 13
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 13
INFO swidthunits
ENDTEXT

TEXT "Stub Length:"
TLOC 1 15
ENDTEXT

FIELD length
FLOC 1 17
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 17
INFO lengthunits
ENDTEXT

TEXT "Quarter-circle 1 Width:"
TLOC 1 19
ENDTEXT

FIELD swidth1
FLOC 1 21
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel1
TLOC 14 21
INFO swidthunits1
ENDTEXT

TEXT "Quarter-circle 2 Width:"
TLOC 1 23
ENDTEXT

FIELD swidth2
FLOC 1 25
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel2
TLOC 14 25
INFO swidthunits2
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 27
ENDTEXT

FIELD angle
FLOC 14 27
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 29
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 3 Net:"
TLOC 1 5
ENDTEXT

TEXT
TLOC 10 5
INFO pin3netname 12
ENDTEXT

FIELD pin3netname_list
FLOC 24 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 4 Net:"
TLOC 1 7
ENDTEXT

TEXT
TLOC 10 7
INFO pin4netname 12
ENDTEXT

FIELD pin4netname_list
FLOC 24 7
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 36
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 36
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbrhybrid.bmp
OPTIONS STRETCH
FLOC 32 2
FSIZE 52 30
ENDFIELD
ENDTILE

ENDFORM

 