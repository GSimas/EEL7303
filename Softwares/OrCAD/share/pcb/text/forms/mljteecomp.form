FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 30
HEADER "Multi-layer Tee Junction"

POPUP <ConnectPoints>"Opposite Junction""0","Right Connect""1"
POPUP <actlayers>"subclass""subclass".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE MSLINEOPENENDEFFECT

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 26

TAB "Parameters"

TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Line Width1:"
TLOC 1 3
ENDTEXT

FIELD width1var
FLOC 10 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD width1
FLOC 1 5
REALFILLIN 10 10
ENDFIELD

TEXT width1unitslabel
TLOC 14 5
INFO widthunits1
ENDTEXT

TEXT "Line Width2:"
TLOC 1 7
ENDTEXT

FIELD width2var
FLOC 10 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD width2
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT widthunitslabel2
TLOC 14 9
INFO widthunits2
ENDTEXT

TEXT "Line Width3:"
TLOC 1 11
ENDTEXT

FIELD width3var
FLOC 10 11
MENUBUTTON "..." 3 3
ENDFIELD

FIELD width3
FLOC 1 13
REALFILLIN 10 10
ENDFIELD

TEXT width3unitslabel
TLOC 14 13
INFO widthunits3
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 15
ENDTEXT

FIELD angle
FLOC 14 15
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

TEXT "Next Connect:" nextlabel
TLOC 1 17
ENDTEXT

FIELD nextconnect
FLOC 1 19
ENUMSET 24
POP "ConnectPoints"
ENDFIELD

FIELD newtrace
FLOC 1 21
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 3 Net:"
TLOC 1 5
ENDTEXT

TEXT
TLOC 10 5
INFO pin3netname 12
ENDTEXT

FIELD pin3netname_list
FLOC 24 5
MENUBUTTON "..." 3 3
ENDFIELD
ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 28
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 28
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbmljtee.bmp
OPTIONS STRETCH
FLOC 32 2
FSIZE 42 22
ENDFIELD
ENDTILE

ENDFORM

