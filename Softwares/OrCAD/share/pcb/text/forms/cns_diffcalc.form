FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 40 17
HEADER "Differential Calculator"

POPUP <layerNamesPopup>"""".

TILE

GROUP ""
GLOC 1 0
GSIZE 39 17
ENDGROUP

TEXT "Layer name:"
FLOC 3 2
ENDTEXT

FIELD layerNames
FLOC 14 2
ENUMSET 18
POP "layerNamesPopup"
ENDFIELD

TEXT "Differential impedance:"
FLOC 3 5
ENDTEXT

FIELD diffImp
FLOC 21 5
STRFILLIN 12 32
ENDFIELD

FIELD calcDiffImp
FLOC 36 5
CHECKLIST "" "target"
ENDFIELD

TEXT "Single-line impedance:"
FLOC 3 7
ENDTEXT

FIELD imp
FLOC 21 7
STRFILLIN 12 32
ENDFIELD

TEXT "Line width:"
FLOC 3 9
ENDTEXT

FIELD width
FLOC 21 9
STRFILLIN 12 32
ENDFIELD

FIELD calcWidth
FLOC 36 9
CHECKLIST "" "target"
ENDFIELD

TEXT "Primary gap:"
FLOC 3 11
ENDTEXT

FIELD gap
FLOC 21 11
STRFILLIN 12 32
ENDFIELD

FIELD calcGap
FLOC 36 11
CHECKLIST "" "target"
ENDFIELD

TEXT "Select a button to choose a recalculation target."
FLOC 3 14
ENDTEXT

FIELD close
FLOC 2 18
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD help
FLOC 29 18
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
