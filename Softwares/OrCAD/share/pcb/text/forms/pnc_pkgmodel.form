FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 52 40
HEADER "Package Model"


TILE

TEXT "Select method to create Package Model:"
TLOC 2 3
ENDTEXT

{
GROUP "2-D Models from SigNoise"
GLOC 4 3
GSIZE 45 7
ENDGROUP

FIELD 2d_single_mode
FLOC 7 6
CHECKLIST "Single Line Package Model for entire package" "mode"
ENDFIELD
}


GROUP "3-D Models from PNC's Parasitic Parameters"
GLOC 4 6
GSIZE 45 14
ENDGROUP

FIELD 3d_single_mode
FLOC 7 9
CHECKLIST "Single Line Package Model for entire package" "mode"
ENDFIELD

FIELD 3d_coupled_mode
FLOC 7 12 
CHECKLIST "Coupled Line Package Model for entire package" "mode" 
ENDFIELD 
 
FIELD 3d_by_net_mode
FLOC 7 15
CHECKLIST "Package Model by nets" "mode" 
ENDFIELD

 
FIELD apply
FLOC 2 24
MENUBUTTON "OK" 10 3
ENDFIELD
 
FIELD close
FLOC 13 24
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD help  
FLOC 38 24
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM

