FILE_TYPE=FORM_DEFN VERSION=2

    FORM
    FIXED
    PORT 65 31
    HEADER "Cadence-Standard VRM Editor"

    ACTIVEFIELD "Done"
    DEFAULT Done

    TILE

    FIELD Done
        FLOC 2 31
        MENUBUTTON "OK" 10 3
    ENDFIELD

    FIELD Help
        FLOC 54 31
        MENUBUTTON "Help" 10 3
    ENDFIELD

    GROUP ""
        GLOC 1 0
        GSIZE 64 30
    ENDGROUP

    FIELD ModelBitmap
        THUMBNAIL pqmfvrmedit.bmp
        FLOC 2 2
        FSIZE 62 7
    ENDFIELD

    TEXT "Model Name:"
        TLOC 2 12
    ENDTEXT

    TEXT
        TLOC 22 12
        INFO ModelName 23
    ENDTEXT

    TEXT "Slew Inductance (Lslew):"
        TLOC 2 15
    ENDTEXT

    FIELD InputInduct
        FLOC 22 15
        STRFILLIN 23 256
    ENDFIELD

    FIELD InputInductCalc
        FLOC 47 15
        MENUBUTTON "Calculate" 15 3
    ENDFIELD

    TEXT "Flat Resistance (Rflat):"
        TLOC 2 18
    ENDTEXT

    FIELD InputResist
      FLOC 22 18
      STRFILLIN 23 256
    ENDFIELD

    TEXT "Output Inductance (Lout):"
      TLOC 2 21
    ENDTEXT

    FIELD OutputInduct
      FLOC 22 21
      STRFILLIN 23 256
    ENDFIELD

    TEXT "Output resistance (R0):"
      TLOC 2 24
    ENDTEXT

    FIELD OutputResist
      FLOC 22 24
      STRFILLIN 23 256
    ENDFIELD

    FIELD TextEdit
      FLOC 2 27
      MENUBUTTON "Text Edit" 15 3
    ENDFIELD

    FIELD GraphResponse
      FLOC 25 27
      MENUBUTTON "Graph Response" 15 3
    ENDFIELD

    ENDTILE
ENDFORM
