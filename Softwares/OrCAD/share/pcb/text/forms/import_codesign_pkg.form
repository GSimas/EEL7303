FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 48 6
HEADER "Place PoP Package"

POPUP <ROTATION_POPUP> "" "".
POPUP <SUBCLASS_POPUP> "" "".

TILE

GROUP "Logic"
GLOC 2 1
GSIZE 44 7
ENDGROUP

TEXT "Ref Des:"
TLOC 5 3
ENDTEXT

FIELD refdes
FLOC 22 3
STRFILLIN 21 128
ENDFIELD

FIELD import_nets
FLOC 5 5
CHECKLIST "Import net assignments"
ENDFIELD

GROUP "Placement"
GLOC 2 8
GSIZE 44 11
ENDGROUP

TEXT "Pin Layer:"
TLOC 5 10
ENDTEXT

FIELD subclass
FLOC 22 10
ENUMSET 20
OPTIONS prettyprint ownerdrawn
POP "SUBCLASS_POPUP"
ENDFIELD

TEXT "Origin X:"
TLOC 5 12
ENDTEXT

FIELD origin_x
FLOC 22 12
STRFILLIN 21 128
ENDFIELD

TEXT "Origin Y:"
TLOC 5 14
ENDTEXT

FIELD origin_y
FLOC 22 14
STRFILLIN 21 128
ENDFIELD

TEXT "Rotation:"
TLOC 5 16
ENDTEXT

FIELD rotation
FLOC 22 16
ENUMSET 20
POP "ROTATION_POPUP"
ENDFIELD

FIELD done
FLOC 3 21
MENUBUTTON "Ok" 10 3
ENDFIELD

FIELD cancel
FLOC 14 21
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD place
FLOC 25 21
MENUBUTTON "Place" 10 3
ENDFIELD

FIELD help
FLOC 36 21
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE
ENDFORM

