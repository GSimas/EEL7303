FILE_TYPE=FORM_DEFN VERSION=2
FORM
VARIABLE
PORT 19 34
HEADER "Find Filter"

POPUP <FIND_NAME_TYPE>"Net""0","Comp""1", "Symbol""2", \
			"Func""3", "Devtype""4","Symtype""5".
POPUP <FIND_LIST>"Name""0","List""1".

POPUP <FIND_LINE_SEG>"Line Segs""0","Lines""1".
POPUP <FIND_CLINE_SEG>"Cline Segs""0","Clines""1".
POPUP <FIND_SHAPE_SEG>"Shape Segs""0","Shapes""1".
POPUP <FIND_VOID_SEG>"Void Segs""0","Voids""1".


TILE StdTile
TPANEL 2
FIELD done
FLOC 1 1
MENUBUTTON "Done" 8 3
ENDFIELD

FIELD help
FLOC 10 1
MENUBUTTON "Help" 8 3
ENDFIELD

ENDTILE

TILE AllOnOffTile
TPANEL 0
TEXT "All:"
TLOC 1 0
ENDTEXT

FIELD all_on
FLOC 6 0
MENUBUTTON "On" 5 2
ENDFIELD

FIELD all_off
FLOC 12 0
MENUBUTTON "Off" 5 2
ENDFIELD

ENDTILE

TILE FindNameTile
TPANEL 2
FIELD name_type
FLOC 1 4
FGROUP "Find By Name/Prop"
ENUMSET 7
POP "FIND_NAME_TYPE"
ENDFIELD
 
FIELD find_list
FLOC 11 4
FGROUP "Find By Name/Prop"
ENUMSET 4
POP "FIND_LIST"
ENDFIELD
 
TEXT ">"
TLOC 1 6
ENDTEXT
 
FIELD find_name
FLOC 3 6
FGROUP "Find By Name/Prop"
STRFILLIN 12 255
ENDFIELD
 
FIELD find_by_name
FLOC 1 9
MENUBUTTON "More Find..." 15 3
ENDFIELD

GROUP "Find By Name/Prop"
GLOC 0 1
GSIZE 19 11
ENDGROUP
 
ENDTILE

TILE FILTER_TILE
TPANEL 1
TEXT
TLOC 4 0
INFO name 13
ENDTEXT

FIELD on
FLOC 1 0
CHECKLIST ""
ENDFIELD

ENDTILE

TILE LINE_SEG_TILE
TPANEL 1
FIELD on
FLOC 1 0
CHECKLIST ""
ENDFIELD

FIELD line_or_seg
FLOC 4 0
ENUMSET 13
POP "FIND_LINE_SEG"
ENDFIELD
ENDTILE

TILE CLINE_SEG_TILE
TPANEL 1
FIELD on
FLOC 1 0
CHECKLIST ""
ENDFIELD

FIELD cline_or_seg
FLOC 4 0
ENUMSET 13
POP "FIND_CLINE_SEG"
ENDFIELD

ENDTILE

TILE SHAPE_SEG_TILE
TPANEL 1
FIELD on
FLOC 1 0
CHECKLIST ""
ENDFIELD

FIELD shape_or_seg
FLOC 4 0
ENUMSET 13
POP "FIND_SHAPE_SEG"
ENDFIELD

ENDTILE

TILE VOID_SEG_TILE
TPANEL 1
FIELD on
FLOC 1 0
CHECKLIST ""
ENDFIELD

FIELD void_or_seg
FLOC 4 0
ENUMSET 13
POP "FIND_VOID_SEG"
ENDFIELD

ENDTILE

ENDFORM
