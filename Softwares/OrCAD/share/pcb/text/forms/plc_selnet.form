FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 70 25
HEADER "Create Net Exception List"

POPUP <filter_pop>"*""*".

TILE

TEXT "Name filter:"
TLOC 2 4
ENDTEXT

FIELD NAMEFILTER
FLOC 11 4
STRFILLIN 13 255
POP "filter_pop"
ENDFIELD

FIELD UNSELECTEDLIST
FLOC 2 7
LIST "" 27 10
ENDFIELD

FIELD SELECTEDLIST
FLOC 41 4
LIST "" 27 10
ENDFIELD

TEXT "File name:"
TLOC 41 24
ENDTEXT

FIELD SAVEFILE
FLOC 49 24
STRFILLIN 9 256
ENDFIELD

FIELD BROWSE
FLOC 60 24
MENUBUTTON "Browse..." 8 3
ENDFIELD

FIELD ALLTOSELECTED
FLOC 32 12
MENUBUTTON "All->" 7 3
ENDFIELD

FIELD ALLFROMSELECTED
FLOC 32 15
MENUBUTTON "<-All" 7 3
ENDFIELD

FIELD DONE
FLOC 4 28
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD CANCEL
FLOC 17 28
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD HELP
FLOC 55 28
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "Unselected nets" G1
FLOC 1 1
FSIZE 30 26
ENDGROUP

GROUP "Selected nets" G2
FLOC 40 1
FSIZE 30 26
ENDGROUP

ENDTILE

ENDFORM
