FILE_TYPE=FORM_DEFN VERSION=2
FORM AUTOGREYTEXT
FIXED
PORT 42 14
HEADER "IDF Out Filter Setup"

MESSAGE nedit 2 "Not editable while in interactive command."

TILE

TEXT "The selected objects will be excluded from IDF Output"
FLOC 2 1
ENDTEXT

FIELD filter_tree
FLOC 2 3
TREEVIEW  28 9
ENDFIELD

FIELD filter_ok
FLOC 31 3
MENUBUTTON "OK" 10 3
ENDFIELD
 
FIELD filter_cancel
FLOC 31 7
MENUBUTTON "Cancel" 10 3
ENDFIELD

  
FIELD filter_reset
FLOC 31 11
MENUBUTTON "Reset" 10 3
ENDFIELD

FIELD help
FLOC 31 16
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
