FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 70 48
HEADER "View Die Abstract"

TILE

TEXT "Die abstract file to load: "
TLOC 2 1
ENDTEXT

FIELD xmlname_text
FLOC 20 1
STRFILLIN 32 1024
ENDFIELD

FIELD xmlfile_browse
FLOC 57 1
MENUBUTTON "Browse..." 10 3
ENDFIELD

TEXT "Design Name: "
TLOC 2 3
ENDTEXT

FIELD design_name_text
FLOC 20 3
STRFILLIN 32 1024
ENDFIELD

TEXT "Must be a Die Abstract file (.dia or .xda) which includes CELLLIBRARY section."
TLOC 2 5
ENDTEXT

FIELD optical_shrink
FLOC 2 8
CHECKLIST "Apply IC fabrication optical shrink by:"
ENDFIELD

FIELD shrinkage_value
FLOC 30 8
REALFILLIN 7 16
DECIMAL 2
REALMIN 0.0
REALMAX 100.0
ENDFIELD

FIELD scribe_lines
FLOC 2 10
CHECKLIST "Add scribe width"
ENDFIELD

FIELD north_scribe
FLOC 13 12
STRFILLIN 10 64
ENDFIELD
TEXT "North:"
TLOC 8 12
ENDTEXT

FIELD south_scribe
FLOC 32 12
STRFILLIN 10 64
ENDFIELD
TEXT "South:"
TLOC 27 12
ENDTEXT

FIELD east_scribe
FLOC 13 14
STRFILLIN 10 64
ENDFIELD
TEXT "East:"
TLOC 8 14
ENDTEXT

FIELD west_scribe
FLOC 32 14
STRFILLIN 10 64
ENDFIELD
TEXT "West:"
TLOC 27 14
ENDTEXT

TEXT "Use Library Manager to set Die Pin and IC Routing Pin options before proceeding."
TLOC 2 17
ENDTEXT

FIELD leflibmgr
FLOC 2 19
MENUBUTTON "Library Manager" 16 3
ENDFIELD

FIELD done
FLOC 2 23
MENUBUTTON "OK" 16 3
ENDFIELD

FIELD cancel
FLOC 27 23
MENUBUTTON "Cancel" 16 3
ENDFIELD

FIELD help
FLOC 52 23
MENUBUTTON "Help" 16 3
ENDFIELD

ENDTILE

ENDFORM
