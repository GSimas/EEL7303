FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 27 20
HEADER "Multi-Line Route"

POPUP <quantity_vals>"10""10".
POPUP <line_width_dbrep_vals>"5""5".
POPUP <line_to_line_space_dbrep_vals>"5""5".
POPUP <ctrl_trace_orientation>"Centered""0","on Left""-1","on Right""1".

TILE

TEXT "Quantity"
FLOC 1 1
ENDTEXT

FIELD quantity
FLOC 15 1
STRFILLIN 5 6
POP "quantity_vals"
ENDFIELD

TEXT "Line Width"
FLOC 1 3
ENDTEXT

FIELD line_width
FLOC 15 3
STRFILLIN 5 6
POP "line_width_dbrep_vals"
ENDFIELD

TEXT "Line to Line Space"
FLOC 1 5
ENDTEXT

FIELD line_to_line_space
FLOC 15 5
STRFILLIN 5 6
POP "line_to_line_space_dbrep_vals"
ENDFIELD

TEXT "Control Trace"
FLOC 1 7
INFO control_trace_label 13
ENDTEXT

FIELD control_trace_orientation
FLOC 15 7
ENUMSET 8
POP "ctrl_trace_orientation"
ENDFIELD

FIELD ok
FLOC 1 10
MENUBUTTON "Ok" 6 3
ENDFIELD

FIELD cancel
FLOC 10 10
MENUBUTTON "Cancel" 6 3
ENDFIELD

FIELD help
FLOC 20 10
MENUBUTTON "Help" 6 3
ENDFIELD

ENDTILE
ENDFORM
