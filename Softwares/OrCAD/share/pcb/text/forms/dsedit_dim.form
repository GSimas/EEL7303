FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 35 12
HEADER "Resize Spacer"

POPUP <diePopup>"""".

TILE

TEXT "A new symbol will be created for this change."
TLOC 2 1
ENDTEXT

GROUP ""
GLOC 1 2
GSIZE 34 13
ENDGROUP

TEXT "Symbol Name:"
TLOC 2 4
ENDTEXT
FIELD spacerSymbolName
FLOC 14 4
STRFILLIN 18 40
ENDTEXT

TEXT "Length:"
TLOC 2 6
ENDTEXT
FIELD spacerLength
FLOC 14 6
STRFILLIN 18 40
ENDFIELD

TEXT "Width:"
TLOC 2 8
ENDTEXT
FIELD spacerWidth
FLOC 14 8
STRFILLIN 18 40
ENDFIELD

FIELD spacerDerive
FLOC 2 10
CHECKLIST "Derive from:"
ENDFIELD
FIELD spacerDie
FLOC 15 10
ENUMSET 15
POP "diePopup"
ENDFIELD
TEXT "Shrink factor:"
TLOC 4 12
ENDTEXT
FIELD spacerShrink
FLOC 15 12
REALFILLIN 16 16
MIN 0.01
ENDFIELD

FIELD done
FLOC 6 16
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD cancel
FLOC 21 16
MENUBUTTON "Cancel" 9 3
ENDFIELD

ENDTILE
ENDFORM
