FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 33 29
HEADER "BGA Properties"

POPUP <PINLABELPOPUP>"""".
POPUP <PINSTARTPOPUP>"""".
POPUP <TEXTBLOCKPOPUP>"""".

TILE

GROUP "Pin Labeling"
GLOC 1 0
GSIZE 32 15
ENDGROUP

TEXT "Ordering:"
TLOC 2 2
ENDTEXT

FIELD pin_labelling
FLOC 10 2
ENUMSET 18
POP "PINLABELPOPUP"
ENDFIELD

TEXT "Start at:"
TLOC 2 4
ENDTEXT

FIELD pin_start
FLOC 10 4
ENUMSET 18
POP "PINSTARTPOPUP"
ENDFIELD

FIELD pin_letter_first
FLOC 2 6
CHECKLIST "Label with letters before numbers"
ENDFIELD

FIELD pin_jedec
FLOC 2 8
CHECKLIST "JEDEC standard."
ENDFIELD

FIELD pin_pad_letters
FLOC 2 10
CHECKLIST "Pad letters with A's."
ENDFIELD

FIELD pin_pad_numbers
FLOC 2 12
CHECKLIST "Pad numbers with Zeroes."
ENDFIELD

GROUP "Pin Arrangement"
GLOC 1 15
GSIZE 32 9
ENDGROUP

TEXT "Horizontal pitch:"
TLOC 2 17
ENDTEXT

FIELD hor_pitch
FLOC 15 17
STRFILLIN 14 16
ENDFIELD

TEXT "Vertical pitch:  "
TLOC 2 19
ENDTEXT

FIELD ver_pitch
FLOC 15 19
STRFILLIN 14 16
ENDFIELD

FIELD pin_staggered
FLOC 2 21
CHECKLIST "Staggered pin configuration."
ENDFIELD

GROUP "Display Settings"
GLOC 1 24
GSIZE 32 14
ENDGROUP

FIELD pin_number_display
TLOC 2 26
CHECKLIST "Display pin numbers."
ENDFIELD

FIELD pin_number_left
TLOC 5 28
CHECKLIST "Left"
ENDFIELD

FIELD pin_number_right
TLOC 5 30
CHECKLIST "Right"
ENDFIELD

FIELD pin_number_top
TLOC 15 28
CHECKLIST "Top"
ENDFIELD

FIELD pin_number_bottom
TLOC 15 30
CHECKLIST "Bottom"
ENDFIELD

TEXT "Text Size:"
TLOC 2 32
ENDTEXT

FIELD pin_text_block
FLOC 15 32
ENUMSET 13
POP "TEXTBLOCKPOPUP"
ENDFIELD

TEXT "Distance from package edge:"
TLOC 2 34
FSIZE 12 4
ENDTEXT

FIELD pin_text_edge_spacing
FLOC 15 34
STRFILLIN 14 128
ENDFIELD


FIELD help
FLOC 23 39
MENUBUTTON "Help" 10 3
ENDFIELD

FIELD back
FLOC 1 42
MENUBUTTON "<Back" 10 3
ENDFIELD

FIELD next
FLOC 12 42
MENUBUTTON "Next>" 10 3
ENDFIELD

FIELD Cancel
FLOC 23 42
MENUBUTTON "Cancel" 10 3
ENDFIELD

ENDTILE

ENDFORM

