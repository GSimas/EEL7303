FILE_TYPE=FORM_DEFN VERSION=1
FORM autogreytext
FIXED
PORT 29 13

HEADER "Die Escape Generator Options"

TILE DE_MINI

FIELD de_include_voltage_nets
FLOC 1 2
CHECKLIST "Allow Selection of Voltage Pins"
ENDFIELD

FIELD de_include_unassigned_pins
FLOC 1 4
CHECKLIST "Allow Selection of Unassigned Pins"
ENDFIELD

TEXT "Escape Distance From Die Outline"
FLOC 1 7
ENDFIELD

FIELD de_escape_distance
FLOC 3 9
STRFILLIN 8 8
ENDFIELD

TEXT "mm"
FLOC 13 9
INFO de_escape_distance_units 8
ENDFIELD

TEXT "Min Bend Distance From Escaping"
FLOC 1 12
ENDFIELD

TEXT "Pin/Via Pad"
FLOC 1 13
ENDFIELD

FIELD de_min_bend_distance
FLOC 3 15
STRFILLIN 8 8
ENDFIELD

TEXT "mm"
FLOC 13 15
INFO de_min_bend_distance_units 8
ENDFIELD

ENDTILE

ENDFORM
