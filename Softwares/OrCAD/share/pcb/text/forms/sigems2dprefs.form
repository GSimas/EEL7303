FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 76 28
HEADER "EMS2D Preferences"

POPUP <NoOfFreqPoints>"""".
POPUP <MeshOrder>"""".
POPUP <RoughnessModels>"""".

ACTIVEFIELD "Close"

TILE

GROUP "Frequency Settings"
GLOC 1 1
GSIZE 75 15

FIELD Ems2dFreqSettings_DefaultFreqs
FLOC 4 4
CHECKLIST "Default Frequencies" "Ems2dFreqSettings"
FGROUP "Frequency Settings"
ENDFIELD

FIELD Ems2dFreqSettings_FreqParams
FLOC 4 6
CHECKLIST "Frequency Parameters" "Ems2dFreqSettings"
FGROUP "Frequency Settings"
ENDFIELD

TEXT "Start Frequency"
TLOC 8 8
ENDTEXT

FIELD Ems2d_StartFrequency
FLOC 29 8
STRFILLIN 10 20
ENDFIELD

TEXT "End Frequency"
TLOC 47 8
ENDTEXT

FIELD Ems2d_EndFrequency
FLOC 63 8
STRFILLIN 10 20
ENDFIELD

TEXT "# of Freq. Points"
TLOC 8 10
ENDTEXT

FIELD Ems2d_NoOfFreqPoints
FLOC 29 10
STRFILLIN 7 20
POP "NoOfFreqPoints"
ENDFIELD

FIELD Ems2d_FreqSweep
FLOC 47 10 
CHECKLIST "Log Scale"
ENDFIELD

FIELD Ems2dFreqSettings_FreqPtFile
FLOC 4 13
CHECKLIST "Frequency Point File" "Ems2dFreqSettings"
FGROUP "Frequency Settings"
ENDFIELD

FIELD Ems2d_FreqPtFile
FLOC 29 13
STRFILLIN 31 80
ENDFIELD

FIELD BrowseFiles
FLOC 63 13
MENUBUTTON "Browse..." 11 3
ENDFIELD
ENDGROUP

GROUP "Surface Roughness"
GLOC 1 17
GSIZE 75 6

TEXT "Model"
TLOC  4 20
ENDTEXT

FIELD EmsRoughnessModel
FLOC 10 20
STRFILLIN 20 20
POP "RoughnessModels"
ENDFIELD

TEXT "RMS Value"
TLOC 41 20
ENDTEXT

FIELD EmsSurfaceRoughnessRMS
FLOC 51 20
STRFILLIN 20 20
ENDFIELD
ENDGROUP

TEXT "Mesh Order"
TLOC 4 24 
ENDTEXT

FIELD Ems2d_MeshOrder
FLOC 16 24
STRFILLIN 3 10 
POP "MeshOrder"
ENDFIELD

FIELD Ems2d_FastSolution
FLOC 4 26 
CHECKLIST "Fast Frequency Sweep"
ENDFIELD

FIELD Ems2d_OutputSParam
FLOC 4 28 
CHECKLIST "Output SParameter (.snp)"
ENDFIELD

FIELD Ok
FLOC 2 31
MENUBUTTON "OK" 11 3
ENDFIELD

FIELD Cancel
FLOC 16 31
MENUBUTTON "Cancel" 11 3
ENDFIELD

FIELD help
FLOC 63 31
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
