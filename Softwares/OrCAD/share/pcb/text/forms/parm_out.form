FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 52 40
HEADER "Export Allegro Parameters"

MESSAGE nerror 3 "A problem occurred - please check the logfile."
MESSAGE nwarn 2 "Parameter file generated with warnings."
MESSAGE ndone 0 "Parameter file write completed."
MESSAGE norec 2 "No parameter group selected."
MESSAGE nofile 2 "No output file name entered."

TILE 
ACTIVEFIELD "fileName"
TEXT "Output File Name:"
TLOC 1 3
ENDTEXT

FIELD fileName
FLOC 14 3
STRFILLIN 30 256
ENDFIELD

FIELD browse
FLOC 46 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD parmTree
FLOC 3 10
TREEVIEW 46 8
ENDFIELD

FIELD selectAll
FLOC 3 26
MENUBUTTON "Select All" 9 3
ENDFIELD

FIELD clearAll
FLOC 13 26
MENUBUTTON "Clear All" 9 3
ENDFIELD

GROUP "Available Parameters" "grp_af"
GLOC 1 7
GSIZE 50 22
ENDGROUP

FIELD export
FLOC 3 31
MENUBUTTON "Export" 9 3
ENDFIELD

FIELD cancel
FLOC 15 31
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD viewlog
FLOC 27 31
MENUBUTTON "Viewlog..." 9 3 
ENDFIELD

FIELD help
FLOC 40 31
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
