FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 32 16
HEADER "Balloon Parameters"

POPUP <dim_bllntypepop>"Circle""100","Square""101","Triangle""102","Oblong""103".


TILE
TEXT "Type:"
TLOC 5 4
ENDTEXT

TEXT "Size:"
TLOC 5 6
ENDTEXT

TEXT "Text block:"
TLOC 5 8
ENDTEXT

TEXT "Next value:"
TLOC 5 10
ENDTEXT

TEXT "Max characters:"
TLOC 5 21
ENDTEXT

FIELD type
FLOC 17 4
ENUMSET 9
POP "dim_bllntypepop"
ENDFIELD

FIELD size
FLOC 17 6
REALFILLIN 5 5
DECIMAL 2
ENDFIELD

FIELD text_block
FLOC 17 8
INTSLIDEBAR 2 2
MIN 1
MAX 64
ENDFIELD

FIELD value
FLOC 17 10
STRFILLIN 12 12
ENDFIELD

FIELD autoincr
FLOC 2 12
CHECKLIST "Auto-increment value"
ENDFIELD

FIELD autosize
FLOC 2 19
CHECKLIST "Auto-size"
ENDFIELD

FIELD max_character
FLOC 21 21
INTSLIDEBAR 2 2
MIN 1
MAX 12
ENDFIELD

FIELD done
FLOC 1 26
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD help
FLOC 23 26
MENUBUTTON "Help" 9 3
ENDFIELD

GROUP "General"
GLOC 1 1
GSIZE 31 14
ENDGROUP

GROUP "Auto-size"
GLOC 1 16
GSIZE 31 8
ENDGROUP

ENDTILE

ENDFORM
