FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Microstrip Step"

POPUP <actlayers>"""".
POPUP <gndlayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE MSTEP

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 20

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground:"
TLOC 1 3
ENDTEXT

FIELD ground_subclass
FLOC 7 3
ENUMSET 16
POP "gndlayers"
ENDFIELD

FIELD groundlayervar
FLOC 26 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Input Width:"
TLOC 1 5
ENDTEXT

FIELD inwidthvar
FLOC 10 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD inputwidth
FLOC 1 7
REALFILLIN 10 10
ENDFIELD

TEXT inputwidthunitslabel
TLOC 14 7
INFO inputwidthunits
ENDTEXT

TEXT "Output Width:"
TLOC 1 9
ENDTEXT

FIELD outwidthvar
FLOC 11 9
MENUBUTTON "..." 3 3
ENDFIELD

FIELD outputwidth
FLOC 1 11
REALFILLIN 10 10
ENDFIELD

TEXT outputwidthunitslabel
TLOC 14 11
INFO outputwidthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 13
ENDTEXT

FIELD angle
FLOC 14 13
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 15
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD
ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 22
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 22
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbmstep.bmp
OPTIONS STRETCH
FLOC 32 4
FSIZE 40 12
ENDFIELD

ENDTILE

ENDFORM

