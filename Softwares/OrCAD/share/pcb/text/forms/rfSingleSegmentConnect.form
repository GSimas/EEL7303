FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 28 52
HEADER "Single Segment Connect"

POPUP <subclass>"""".
POPUP <gndAboveLayers>"""".
POPUP <gndBelowLayers>"""".

TILE

TEXT "Act"
TLOC 24 1
INFO acttext 5
ENDTEXT

FIELD boxActColor
FLOC 1 1
COLOR 2 1
ENDFIELD

FIELD listAct
FLOC 4 1
ENUMSET 17
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

TEXT "Alt"
TLOC 24 3
INFO alttext 5
ENDTEXT

FIELD boxAltColor
FLOC 1 3
COLOR 2 1
ENDFIELD

FIELD listAlt
FLOC 4 3
ENUMSET 17
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

TEXT "Ground above:" lblGndAbove
TLOC 1 6
ENDTEXT

FIELD listAbove
FLOC 12 6
ENUMSET 13
POP "gndAboveLayers"
ENDFIELD

TEXT "Ground below:" lblGndBelow
TLOC 1 8
ENDTEXT

FIELD listBelow
FLOC 12 8
ENUMSET 13
POP "gndBelowLayers"
ENDFIELD

TEXT "Line width:" lblLineWidth
TLOC 1 12
ENDTEXT

FIELD txtLineWidth
FLOC 12 12
REALFILLIN 10 10
ENDFIELD

TEXT 
TLOC 24 12
INFO widthUnit 5
ENDTEXT

FIELD chkSnap2Point
FLOC 1 16
CHECKLIST "Snap to connect point"
ENDFIELD

FIELD chkClearance
FLOC 1 20
CHECKLIST "Initialize clearance"
ENDFIELD

FIELD chkAdd2Assembly
FLOC 3 22
CHECKLIST "Add into existing assembly"
ENDFIELD

ENDTILE

ENDFORM
