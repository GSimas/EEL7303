FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 59 20
HEADER "Padstack Export to ADS"
DEFAULT cancel
TILE

TEXT "Available padstack names"
TLOC 2 2
ENDTEXT

FIELD availablePadstacks
FLOC 2 4
LIST "" 21 15
OPTIONS multiselect
OPTIONS alphanumsort
ENDFIELD

FIELD addSelected
FLOC 24 5
MENUBUTTON "Add ->" 12 3
ENDFIELD

FIELD addAll
FLOC 24 8
MENUBUTTON "Add All ->" 12 3
ENDFIELD

FIELD removeSelected
FLOC 24 11
MENUBUTTON "<- Remove" 12 3
ENDFIELD

FIELD removeAll
FLOC 24 14
MENUBUTTON "<- Remove All" 12 3
ENDFIELD

TEXT "Selected padstack names"
TLOC 37 2
ENDTEXT

FIELD selectedPadstacks
FLOC 37 4
LIST "" 21 15
OPTIONS multiselect
OPTIONS alphanumsort
ENDFIELD

TEXT "Padstack filter:"
TLOC 5 35
ENDTEXT

FIELD padstackFilter
FLOC 17 35
STRFILLIN 25 25
ENDFIELD

TEXT "Via group name:"
TLOC 5 39
ENDTEXT

FIELD viaGroupName
FLOC 17 39
STRFILLIN 25 200
ENDFIELD

TEXT "(no path)"
TLOC 44 39
ENDTEXT

TEXT "Output directory:"
TLOC 5 43
ENDTEXT

FIELD outputDir
FLOC 17 43
STRFILLIN 25 100
ENDFIELD

FIELD outputDirBrowser
FLOC 44 43
MENUBUTTON "..." 5 3
ENDFIELD

FIELD exportPadstacks
FLOC 12 47
MENUBUTTON "Export" 11 3
ENDFIELD

FIELD cancel
FLOC 34 47
MENUBUTTON "Close" 11 3
ENDFIELD

ENDTILE
ENDFORM
