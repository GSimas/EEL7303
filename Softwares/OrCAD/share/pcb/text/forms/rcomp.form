FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 16
HEADER "Resistor"

POPUP <actlayers>"""".
POPUP <gndlayers>"""".
POPUP <resunits>"mOhms""0","Ohms""1","kOhms""2","MegOhms""3","GOhms""4".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE R

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 14

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Resistance:"
TLOC 1 3
ENDTEXT

FIELD resistancevar
FLOC 10 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD resistance
FLOC 1 5
REALFILLIN 10 10
ENDFIELD

FIELD resunit
FLOC 14 5
ENUMSET 9
POP "resunits"
ENDFIELD

TEXT "Rotation Lock:"
TLOC 1 7
ENDTEXT

FIELD angle
FLOC 14 7
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 9
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"

TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 16
MENUBUTTON "" 14 3
ENDFIELD

FIELD help
FLOC 22 16
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbr.bmp
OPTIONS STRETCH
FLOC 32 3
FSIZE 28 14
ENDFIELD

ENDTILE

ENDFORM

