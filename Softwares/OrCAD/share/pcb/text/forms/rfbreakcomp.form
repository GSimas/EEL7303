FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 30 20
HEADER "RF Break"

TILE

FIELD percentage
FLOC 2 1
CHECKLIST "percentage" "rg"
ENDFIELD

FIELD electriclength
FLOC 16 1
CHECKLIST "electrical length" "rg"
ENDFIELD

FIELD length
FLOC 2 3
CHECKLIST "length" "rg"
ENDFIELD

FIELD angle
FLOC 16 3
CHECKLIST "angle" "rg"
ENDFIELD

FIELD param1
FLOC 6 5
REALFILLIN 6 12
MIN 0.0
ENDFIELD

FIELD param2
FLOC 20 5
REALFILLIN 6 12
MIN 0.0
ENDFIELD

FIELD trackbar
FLOC 2 7
TRACKBAR 27 4
ENDFIELD

TEXT
TLOC 4 9
INFO minvalue 2
ENDTEXT

TEXT
TLOC 17 9
INFO maxvalue 12
OPTIONS RIGHT
ENDTEXT

FIELD splitmode
FLOC 2 11
CHECKLIST "Split mode" 2
ENDFIELD

FIELD truncatemode
FLOC 16 11
CHECKLIST "Truncate mode" 2
ENDFIELD

FIELD autoshove
FLOC 1 14
CHECKLIST "AutoShove Connected Objects"
ENDFIELD

FIELD Clearance
FLOC 1 17
CHECKLIST "Clearance"
ENDFIELD

FIELD clearancedefault
FLOC 2 19
CHECKLIST "Update clearance shapes to default" 1 
ENDFIELD

FIELD clearanceretain
FLOC 2 21
CHECKLIST "Retain clearance shapes" 1
ENDFIELD

ENDTILE

ENDFORM
