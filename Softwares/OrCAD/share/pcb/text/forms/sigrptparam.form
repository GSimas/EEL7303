FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 55 13
HEADER "Signal Report Units"

POPUP <voltpop>"mV""mV","V""V".

POPUP <resipop>"mOhm""mOhm","Ohm""Ohm".

POPUP <capapop>"pF""pF","nF""nF","uF""uF".

POPUP <indupop>"pH""pH","nH""nH","uH""uH".

POPUP <timepop>"ns""ns","us""us","ms""ms","s""s".

POPUP <distpop>"um""um","mm""mm","m""m".


ACTIVEFIELD "Cancel"

TILE
TEXT "Design Voltage Units"
TLOC 2 1
ENDTEXT

TEXT "Noise Voltage Units"
TLOC 2 3
ENDTEXT

TEXT "Design Resistance Units"
TLOC 2 5
ENDTEXT

TEXT "Parasitic Resistance Units"
TLOC 2 7
ENDTEXT

TEXT "Design Capacitance Units"
TLOC 2 9
ENDTEXT

TEXT "Parasitic Capacitance Units"
TLOC 2 11
ENDTEXT

TEXT "Parasitic Inductance Units"
TLOC 2 13
ENDTEXT

TEXT "Delay Time Units"
TLOC 2 15
ENDTEXT

TEXT "Length Units"
TLOC 2 17
ENDTEXT

TEXT "Spacing Units"
TLOC 2 19
ENDTEXT

FIELD DesignVoltageUnits
FLOC 30 1
ENUMSET 22
POP "voltpop"
ENDFIELD

FIELD NoiseVoltageUnits
FLOC 30 3
ENUMSET 22
POP "voltpop"
ENDFIELD

FIELD DesignResistanceUnits
FLOC 30 5
ENUMSET 22
POP "resipop"
ENDFIELD

FIELD ParasiticResistanceUnits
FLOC 30 7
ENUMSET 22
POP "resipop"
ENDFIELD

FIELD DesignCapacitanceUnits
FLOC 30 9
ENUMSET 22
POP "capapop"
ENDFIELD

FIELD ParasiticCapacitanceUnits
FLOC 30 11
ENUMSET 22
POP "capapop"
ENDFIELD

FIELD ParasiticInductanceUnits
FLOC 30 13
ENUMSET 22
POP "indupop"
ENDFIELD

FIELD DelayTimeUnits
FLOC 30 15
ENUMSET 22
POP "timepop"
ENDFIELD

FIELD LengthUnits
FLOC 30 17
ENUMSET 23
POP "distpop"
ENDFIELD

FIELD SpacingUnits
FLOC 30 19
ENUMSET 23
POP "distpop"
ENDFIELD

FIELD OK
FLOC 2 23
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD Cancel
FLOC 12 23
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 46 23
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
