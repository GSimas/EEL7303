BFILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 70 11 
HEADER "Simulator Preferences"

ACTIVEFIELD "Close"

TILE

TEXT "Simulator Command"
TLOC 3 3 
FGROUP "Backplane Simulator Preferences"
ENDTEXT

FIELD SimulatorCommand
FLOC 23 3 
STRFILLIN 45 80 
FGROUP "Backplane Simulator Preferences"
ENDFIELD

FIELD SimulatorOptions
FLOC 23 6 
MENUBUTTON "Set Simulator Options..." 25 3
FGROUP "Backplane Simulator Preferences"
ENDFIELD

GROUP "Simulator Preferences"
GLOC 1 0
GSIZE 70 9
ENDGROUP

FIELD Close
FLOC 4 11
MENUBUTTON "Close" 11 3
ENDFIELD


FIELD help
FLOC 58 11
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
