FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 33 15
HEADER ""

POPUP <PENCPLATPOP>"Plated""1","Non-Plated""2","Optional""3".

POPUP <PENCFIGPOP>"Null""0","Circle""2","Square""5","Hexagon X""15",\
"Hexagon Y""16","Octagon""3","Cross""4","Diamond""7",\
"Triangle""18","Oblong X""11","Oblong Y""12","Rectangle""6".

MESSAGE ROUNDED 1 "Drill Size Value Rounded Off."

TILE
TEXT "Plating Type:"
TLOC 2 4
TGROUP "Drill Hole"
ENDTEXT

TEXT "Size:"
TLOC 2 6
TGROUP "Drill Hole"
ENDTEXT

TEXT "Offset:"
TLOC 2 8
TGROUP "Drill Hole"
ENDTEXT

TEXT "X:"
TLOC 12 8
TGROUP "Drill Hole"
ENDTEXT

TEXT "Y:"
TLOC 12 10
TGROUP "Drill Hole"
ENDTEXT

TEXT "Figure:"
TLOC 2 17
TGROUP "Drill Symbol"
ENDTEXT

TEXT "Character:"
TLOC 2 19
TGROUP "Drill Symbol"
ENDTEXT

TEXT "Width:"
TLOC 2 21
TGROUP "Drill Symbol"
ENDTEXT

TEXT "Height:"
TLOC 2 23
TGROUP "Drill Symbol"
ENDTEXT

FIELD plating
FLOC 16 4
FGROUP "Drill Hole"
ENUMSET 10
POP "PENCPLATPOP"
ENDFIELD

FIELD size
FLOC 16 6
FGROUP "Drill Hole"
STRFILLIN 10 10
ENDFIELD

FIELD x_offset
FLOC 16 8
FGROUP "Drill Hole"
STRFILLIN 10 10
ENDFIELD

FIELD y_offset
FLOC 16 10
FGROUP "Drill Hole"
STRFILLIN 10 10
ENDFIELD

FIELD figure
FLOC 14 17
FGROUP "Drill Symbol"
ENUMSET 12
POP "PENCFIGPOP"
ENDFIELD

FIELD character
FLOC 16 19
FGROUP "Drill Symbol"
STRFILLIN 1 1
ENDFIELD

FIELD width
FLOC 16 21
FGROUP "Drill Symbol"
STRFILLIN 10 10
ENDFIELD

FIELD height
FLOC 16 23
FGROUP "Drill Symbol"
STRFILLIN 10 10
ENDFIELD

FIELD done
FLOC 1 27
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD help
FLOC 21 27
MENUBUTTON "Help" 9 3
ENDFIELD

GROUP "Drill Hole"
GLOC 1 1
GSIZE 29 12
ENDGROUP

GROUP "Drill Symbol"
GLOC 1 14
GSIZE 29 12
ENDGROUP

ENDTILE

ENDFORM
