ENDFIELD
 
TEXT
TLOC 39 21
INFO "fpRoAutoLabel" 3
ENDTEXT
 
FIELD fpRoAreaToUse
FLOC 42 21
STRFILLIN 2 2
ENDFIELD
 
TEXT
TLOC 46 21
INFO "fpRoPcntLabel2" 1
ENDTEXT

FIELD Done
FLOC 1 30
MENUBUTTON "OK" 10 3
ENDFIELD
 
FIELD Apply
FLOC 14 30
MENUBUTTON "Apply" 10 3
ENDFIELD

FIELD Cancel
FLOC 26 30
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 39 30
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM

