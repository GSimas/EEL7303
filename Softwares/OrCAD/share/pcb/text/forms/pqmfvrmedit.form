FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 63 24
HEADER "Voltage Regulator Module Editor"

POPUP <ruleTypePop>"""".

POPUP <PRINTP>"to File""0","to Printer""1","to Script""2".

ACTIVEFIELD "inputIndCalc"

MESSAGE badentry 0 "%s"

TILE

FIELD vrm_bitmap
  THUMBNAIL pqmfvrmedit.bmp
  FLOC 1 1
  FSIZE 62 7
ENDFIELD

TEXT "Slew inductance:"
  TLOC 2 10
ENDTEXT  

FIELD inputInductance
  FLOC 22 10
  STRFILLIN 18 32
ENDFIELD 
 
FIELD inputIndCalc
  FLOC 42 10
  MENUBUTTON "Calculate" 10 3
ENDFIELD

TEXT "Flat resistance:"
  TLOC 2 12
ENDTEXT

FIELD inputResistance
  FLOC 22 12
  STRFILLIN 18 32
ENDFIELD 
 
TEXT "Output inductance:"
  TLOC 2 14
ENDTEXT     

FIELD outputInductance
  FLOC 22 14
  STRFILLIN 18 32
ENDFIELD 

TEXT "Output resistance:"
  TLOC 2 16
ENDTEXT

FIELD outputResistance
  FLOC 22 16
  STRFILLIN 18 32
ENDFIELD 


FIELD graphResponse
  FLOC 23 19
  MENUBUTTON "Graph Response" 10 3
ENDFIELD


FIELD done
  FLOC 3 23
  MENUBUTTON "Done" 10 3
ENDFIELD

FIELD apply
  FLOC 19 23
  MENUBUTTON "Apply" 10 3
ENDFIELD

FIELD cancel
  FLOC 35 23
  MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
  FLOC 51 23
  MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
