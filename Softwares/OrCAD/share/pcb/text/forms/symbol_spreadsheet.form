FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 38 35

HEADER "Symbol to Spreadsheet"

POPUP <primary_pop> "" "".
POPUP <secondary_pop> "" "".

TILE

TEXT "File name:"
TLOC 1 2
ENDTEXT
FIELD report_name
FLOC 13 2
STRFILLIN 18 128
ENDFIELD
FIELD report_browse
FLOC 33 2
MENUBUTTON "..." 4 3
ENDFIELD

TEXT "File type:"
TLOC 1 4
ENDTEXT
FIELD xml_output
FLOC 13 4
CHECKLIST "XML" "file_type"
ENDFIELD
FIELD txt_output
FLOC 21 4
CHECKLIST "TXT" "file_type"
ENDFIELD
FIELD csv_output
FLOC 29 4
CHECKLIST "CSV" "file_type"
ENDFIELD

TEXT "Primary text:"
TLOC 1 7
ENDTEXT
FIELD primary_field
FLOC 13 7
ENUMSET 22
POP "primary_pop"
ENDFIELD

TEXT "Secondary text:"
TLOC 1 9
ENDTEXT
FIELD secondary_field
FLOC 13 9
ENUMSET 22
POP "secondary_pop"
ENDFIELD

TEXT "Delimiter:"
TLOC 1 11
ENDTEXT
FIELD delimiter
FLOC 13 11
STRFILLIN 23 128
ENDFIELD

FIELD add_labels
FLOC 1 14
CHECKLIST "Add row and column headers on top/left sides"
ENDFIELD

FIELD done
FLOC 1 18
MENUBUTTON "Ok" 9 3
ENDFIELD

FIELD cancel
FLOC 11 18
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 28 18
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM
