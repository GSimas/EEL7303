FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 28 52
HEADER "Any Angle Bend"

POPUP <subclass>"""".
POPUP <gndAboveLayers>"""".
POPUP <gndBelowLayers>"""".

TILE

TEXT "Act"
TLOC 24 1
INFO acttext 5
ENDTEXT

FIELD boxActColor
FLOC 1 1
COLOR 2 1
ENDFIELD

FIELD listAct
FLOC 4 1
ENUMSET 17
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

TEXT "Alt"
TLOC 24 3
INFO alttext 5
ENDTEXT

FIELD boxAltColor
FLOC 1 3
COLOR 2 1
ENDFIELD

FIELD listAlt
FLOC 4 3
ENUMSET 17
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

TEXT "Ground above:" lblGndAbove
TLOC 1 6
ENDTEXT

FIELD listAbove
FLOC 12 6
ENUMSET 13
POP "gndAboveLayers"
ENDFIELD

TEXT "Ground below:" lblGndBelow
TLOC 1 8
ENDTEXT

FIELD listBelow
FLOC 12 8
ENUMSET 13
POP "gndBelowLayers"
ENDFIELD

TEXT "Line width:" lblLineWidth
TLOC 1 12
ENDTEXT

FIELD txtLineWidth
FLOC 12 12
REALFILLIN 10 10
ENDFIELD

TEXT 
TLOC 24 12
INFO widthUnit 5
ENDTEXT

TEXT "MiterFraction:" lblMiterFraction
TLOC 1 14
ENDTEXT

FIELD txtMiterFraction
FLOC 12 14
REALFILLIN 10 10
ENDFIELD

TEXT "Taper: " lblTaper
TLOC 1 16
ENDTEXT

FIELD rgTaperLength
FLOC 12 16
CHECKLIST "Length" "rg"
ENDFIELD

FIELD rgTaperAngle
FLOC 20 16
CHECKLIST "Angle" "rg"
ENDFIELD

FIELD txtTaperData
FLOC 12 18
REALFILLIN 10 10
ENDFIELD

TEXT 
TLOC 24 18
INFO taperDataUnit 5
ENDTEXT

FIELD chkSnap2Point
FLOC 1 20
CHECKLIST "Snap to connect point"
ENDFIELD

FIELD chkSnap2PadEdge
FLOC 3 22
CHECKLIST "Snap to pad edge"
ENDFIELD

FIELD chkVariableWidth
FLOC 3 24
CHECKLIST "Variable line width"
ENDFIELD

FIELD chkTaperWidth
FLOC 3 26
CHECKLIST "Taper width difference"
ENDFIELD

FIELD chkClearance
FLOC 1 28
CHECKLIST "Initialize clearance"
ENDFIELD

FIELD chkAdd2Assembly
FLOC 3 30
CHECKLIST "Add into existing assembly"
ENDFIELD

ENDTILE

ENDFORM
