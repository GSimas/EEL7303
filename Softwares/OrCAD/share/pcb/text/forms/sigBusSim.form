FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 76 50
HEADER "Analysis Bus Simulation"

POPUP <CasesPopup>"case1""case1".
POPUP <BusNamesPopup>"""0".

ACTIVEFIELD "OK"

TILE

GROUP "Case Selection"
GLOC 1 0
GSIZE 75 5
ENDGROUP

TEXT "Current Case:"
TLOC 2 2
TGROUP "Case Selection"
ENDTEXT

FIELD StdCurrentCase
FLOC 16 2
ENUMSET 50 
POP "CasesPopup"
FGROUP "Case Selection"
ENDFIELD

GROUP "Bus To Simulate"
GLOC 1 5
GSIZE 75 8
ENDGROUP

TEXT "Bus Name:"
TLOC 3 7
TGROUP "Bus To Simulate"
ENDTEXT

FIELD BusName
FLOC 16 7
ENUMSET 35
POP BusNamesPopup
FGROUP "Bus To Simulate"
ENDFIELD

FIELD BusSetup
FLOC 55 7
MENUBUTTON "Bus Setup..." 11 3
FGROUP "Bus To Simulate"
ENDFIELD

FIELD AssignStimulus
FLOC 16 10
MENUBUTTON "Assign Bus Stimulus..." 11 3
FGROUP "Bus To Simulate"
ENDFIELD

GROUP "Fast/Typical/Slow Mode"
GLOC 1 13
GSIZE 75 5
ENDGROUP

FIELD StandardFast
FLOC 2 15
CHECKLIST "Fast"
FGROUP "Fast/Typical/Slow Mode"
ENDFIELD

FIELD StandardTypical
FLOC 15 15
CHECKLIST "Typical"
FGROUP "Fast/Typical/Slow Mode"
ENDFIELD

FIELD StandardSlow
FLOC 28 15
CHECKLIST "Slow"
FGROUP "Fast/Typical/Slow Mode"
ENDFIELD

FIELD StandardFastSlow
FLOC 41 15
CHECKLIST "Fast/Slow"
FGROUP "Fast/Typical/Slow Mode"
ENDFIELD

FIELD StandardSlowFast
FLOC 54 15
CHECKLIST "Slow/Fast"
FGROUP "Fast/Typical/Slow Mode"
ENDFIELD

GROUP "Receiver Selection"
GLOC 1 18
GSIZE 75 5
ENDGROUP

FIELD AllReceivers
FLOC 2 20
CHECKLIST "All Receivers" "Receivers"
FGROUP "Receiver Selection"
ENDFIELD

FIELD EachReceiver
FLOC 28 20
CHECKLIST "Each Receiver" "Receivers"
FGROUP "Receiver Selection"
ENDFIELD

GROUP "Simulation Type"
GLOC 1 23
GSIZE 75 5
ENDGROUP

FIELD ReflectionSimulation
FLOC 2 25
CHECKLIST "Reflection"
FGROUP "Simulation Type"
ENDFIELD

FIELD ComprehensiveSimulation
FLOC 28 25
CHECKLIST "Comprehensive"
FGROUP "Simulation Type"
ENDFIELD

GROUP "Simulation Output"
GLOC 1 28
GSIZE 75 5
ENDGROUP

FIELD ShowReport
FLOC 2 30
CHECKLIST "Show Report"
FGROUP "Simulation Output"
ENDFIELD

FIELD ShowWaveforms
FLOC 28 30
CHECKLIST "Show Waveform"
FGROUP "Simulation Output"
ENDFIELD

FIELD SaveCircuitFiles
FLOC 54 30
CHECKLIST "Save Circuit Files"
FGROUP "Simulation Output"
ENDFIELD

FIELD RunSimulation
FLOC 26 34
MENUBUTTON "Simulate" 20 3
ENDFIELD

FIELD OK
FLOC 2 37
MENUBUTTON "OK" 11 3
ENDFIELD

FIELD Cancel
FLOC 20 37
MENUBUTTON "Cancel" 11 3
ENDFIELD

FIELD SimPrefs
FLOC 38 37
MENUBUTTON "Preferences..." 20 3
ENDFIELD

FIELD Help
FLOC 65 37
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
