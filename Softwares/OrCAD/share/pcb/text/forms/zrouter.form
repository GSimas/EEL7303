FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 44 11
HEADER "Zrouter"

MESSAGE nexist 0 "Please input a filename."
MESSAGE nerror 0 "A problem occurred - please check the logfile."
MESSAGE ndone 0 "Zrouter completed."
MESSAGE ndemo 0 "Cannot Run in Demo Mode."
MESSAGE nogrid 0 "Grid must be greater than 0."

ACTIVEFIELD "filename"
TILE
TEXT "Connections file name:"
TLOC 1 1
ENDTEXT

TEXT "Min. distance between"
TLOC 1 16
ENDTEXT

TEXT "via and pad edges:"
TLOC 3 18
ENDTEXT

TEXT "X-grid spacing:"
TLOC 1 7
ENDTEXT

TEXT "Y-grid spacing:"
TLOC 1 9
ENDTEXT

TEXT "X-grid offset:"
TLOC 1 11
ENDTEXT

TEXT "Y-grid offset:"
TLOC 1 13
ENDTEXT

FIELD filename
FLOC 1 3
STRFILLIN 19 128
ENDFIELD

FIELD execute
FLOC 32 6
MENUBUTTON "Run" 11 3
ENDFIELD

FIELD cancel
FLOC 32 9
MENUBUTTON "Close" 11 3
ENDFIELD

FIELD browse
FLOC 32 3
MENUBUTTON "Browse..." 11 3
ENDFIELD

FIELD viewlog
FLOC 32 15 
MENUBUTTON "View Log" 11 3
ENDFIELD


FIELD help
FLOC 32 18
MENUBUTTON "Help" 11 3
ENDFIELD

FIELD minspace
FLOC 19 18
REALFILLIN 8 8 
DECIMAL 2
ENDFIELD

FIELD xgrid
FLOC 19 7
REALFILLIN 8 8 
DECIMAL 2
ENDFIELD

FIELD ygrid
FLOC 19 9
REALFILLIN 8 8 
DECIMAL 2
ENDFIELD

FIELD xoffset
FLOC 19 11
REALFILLIN 8 8 
DECIMAL 2
ENDFIELD

FIELD yoffset
FLOC 19 13
REALFILLIN 8 8 
DECIMAL 2
ENDFIELD

ENDTILE

ENDFORM



