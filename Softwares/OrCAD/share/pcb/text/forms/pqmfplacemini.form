FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 70 24
HEADER "MiniStatus"

POPUP <ANGLE_POP>"0""0","90""90".

ACTIVEFIELD "surfaceMount"

TILE

GROUP "Object Orientation"
  GLOC 1 1
  GSIZE 31 9
ENDGROUP

TEXT "Angle:"
TLOC 6 3
TGROUP
ENDTEXT

FIELD angle
FLOC 12 3
STRFILLIN 8 50
POP "ANGLE_POP"
FGROUP 
ENDFIELD

TEXT "Side:"
TLOC 7 5
TGROUP
ENDTEXT

FIELD top
FLOC 12 5
CHECKLIST "Top" "side"
ENDFIELD 
 
FIELD bottom
FLOC 12 7
CHECKLIST "Bottom" "side"
ENDFIELD

TEXT "Effective wavelength:"
TLOC 1 12
TGROUP
ENDTEXT

FIELD effective
FLOC 20 12
STRFILLIN 10 50
