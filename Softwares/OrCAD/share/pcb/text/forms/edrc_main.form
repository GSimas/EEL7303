FILE_TYPE=FORM_DEFN VERSION=2
FORM
VARIABLE
PORT 40 13
HEADER "Edit DRC Rules"


POPUP <EDITP>"Edit""1","Copy""2","Delete""3".

MESSAGE CANT_DELETE 1 "Cannot delete the default rule set"

TILE StdTile
TPANEL 2
FIELD done
FLOC 1 1
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD help
FLOC 11 1
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD cancel
FLOC 27 1
MENUBUTTON "Cancel" 9 3
ENDFIELD

ENDTILE

TILE TITLE_TILE
TPANEL 0
TEXT "# DRC Classes"
TLOC 23 0
ENDTEXT

TEXT "Name"
TLOC 6 2
ENDTEXT

TEXT "Using Rule"
TLOC 25 2
ENDTEXT

ENDTILE

TILE RULE_TILE
TPANEL 1
TEXT
TLOC 6 0
INFO rule_name 19
ENDTEXT

FIELD edit_rule
FLOC 1 0
MENUBUTTON "" 4 2
POP "EDITP"
ENDFIELD

TEXT
TLOC 25 0
INFO count 6
ENDTEXT

ENDTILE

ENDFORM
