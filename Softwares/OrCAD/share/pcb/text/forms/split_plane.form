ENDFIELD

FIELD addParam
FLOC -45 14
MENUBUTTON "Add Param" 11 5
ENDFIELD

FIELD addLayer
FLOC 33 14
FGROUP "Edit Params"
CHECKLIST "Add" "Add"
ENDFIELD

FIELD delLayer
FLOC 33 16
FGROUP "Edit Params"
CHECKLIST "Delete" "Delete"
ENDFIELD

FIELD done
FLOC 1 23
MENUBUTTON "Done" 10 3
ENDFIELD

FIELD help
FLOC 14 23
MENUBUTTON "Help" 10 3
ENDFIELD

FIELD cancel
FLOC 28 23
MENUBUTTON "Cancel" 10 3
ENDFIELD

GROUP "Edit Params"
GLOC 0 6
GSIZE 46 15
ENDGROUP

ENDTILE

ENDFORM
