FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 47 14
HEADER "SPLIT PLANE"


POPUP <plane_list>"GND""GND","VCC""VCC".


ACTIVEFIELD "showLayers"

TILE
TEXT "Select Plane:"
TLOC 2 1
ENDTEXT

TEXT
TLOC 2 9
INFO layerInfo 30
TGROUP "Edit Params"
ENDTEXT

FIELD planePopup
FLOC 20 1
ENUMSET 16
