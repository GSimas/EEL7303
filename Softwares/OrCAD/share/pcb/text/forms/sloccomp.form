FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 75 20
HEADER "Stripline Open-Circuited Stub"

POPUP <actlayers>"subclass""subclass".
POPUP <gndlayers1>"""".
POPUP <gndlayers2>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE SLOCS

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 22

TAB "Parameters"
TEXT "Ground 1:"
TLOC 1 1
ENDTEXT

FIELD ground1_subclass
FLOC 9 1
ENUMSET 14
POP "gndlayers1"
ENDFIELD

FIELD ground1layervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Layer:"
TLOC 1 3
ENDTEXT

FIELD active_subclass
FLOC 9 3
ENUMSET 14
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 3
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Ground 2:"
TLOC 1 5
ENDTEXT

FIELD ground2_subclass
FLOC 9 5
ENUMSET 14
POP "gndlayers2"
ENDFIELD

FIELD ground2layervar
FLOC 26 5
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Line Length:"
TLOC 1 7
ENDTEXT

FIELD lengthvar
FLOC 10 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD length
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 9
INFO lengthunits
ENDTEXT

TEXT "Conductor Line Width:"
TLOC 1 11
ENDTEXT

FIELD widthvar
FLOC 17 11
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 13
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 13
INFO swidthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 15
ENDTEXT

FIELD angle
FLOC 14 15
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 24
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 24
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbsloc.bmp
OPTIONS STRETCH
FLOC 32 6
FSIZE 42 10
ENDFIELD
ENDTILE

ENDFORM
