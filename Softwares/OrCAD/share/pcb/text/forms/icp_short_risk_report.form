FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 45 15
HEADER "Opens / Shorts Report"

TILE

TEXT "Report file:"
TLOC 1 1
ENDTEXT
FIELD report_name
FLOC 3 3
STRFILLIN 32 1024
ENDFIELD
FIELD report_browse
FLOC 37 3
MENUBUTTON "..." 6 3
ENDFIELD

FIELD write_report
FLOC 3 5
CHECKLIST "Write report"
ENDFIELD

FIELD view_report
FLOC 17 5
CHECKLIST "View report"
ENDFIELD

TEXT "Minimum ball spacing:"
TLOC 1 8
ENDTEXT
FIELD ball_spacing
FLOC 3 10
STRFILLIN 32 1024
ENDFIELD

TEXT "Minimum routing spacing:"
TLOC 1 12
ENDTEXT
FIELD trace_spacing
FLOC 3 14
STRFILLIN 32 1024
ENDFIELD
FIELD check_all_layers
FLOC 3 16
CHECKLIST "Check all layers (Warning: Time-consuming)"
ENDFIELD

FIELD report
FLOC 1 20
MENUBUTTON "Report" 9 3
ENDFIELD

FIELD close
FLOC 12 20
MENUBUTTON "Close" 10 3
ENDFIELD

ENDTILE

ENDFORM
