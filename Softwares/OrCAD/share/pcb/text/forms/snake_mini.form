FILE_TYPE=FORM_DEFN VERSION=1
FORM autogreytext
FIXED
PORT 29 13

HEADER "Snake"

POPUP <subclass>"subclass""subclass".

TILE SNAKE_MINI

TEXT "Active etch subclass:"
TLOC 1 1
ENDTEXT

FIELD subcolor
FLOC 2 3
COLOR 2 1
OPTIONS colorchooser
ENDFIELD

FIELD subclass
FLOC 5 3
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

TEXT "Line Width"
FLOC 1 6
ENDFIELD

FIELD snake_line_width
FLOC 18 6
STRFILLIN 8 8
ENDFIELD

TEXT "# Lines Between Pads"
FLOC 1 8
ENDFIELD

FIELD snake_num_lines_1
FLOC 18 8
CHECKLIST "1" "NumLinesBetweenPads"
ENDFIELD

FIELD snake_num_lines_2
FLOC 18 10
CHECKLIST "2" "NumLinesBetweenPads"
ENDFIELD

TEXT "Line To Line Space"
FLOC 1 12
ENDFIELD

FIELD snake_line_to_line_space
FLOC 18 12
STRFILLIN 8 8
ENDFIELD

