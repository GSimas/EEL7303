FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 66 8
HEADER "Create Temporary Package"


TILE
TEXT "Package Name:"
TLOC 2 1
ENDTEXT

TEXT
TLOC 16 1
INFO packageName 29
ENDTEXT

TEXT "Pin Count:"
TLOC 46 1
ENDTEXT

TEXT
TLOC 57 1
INFO pinCount 6
ENDTEXT

TEXT "Height:"
TLOC 2 6
TGROUP "Package Size"
ENDTEXT

TEXT "Width:"
TLOC 2 8
TGROUP "Package Size"
ENDTEXT

FIELD height
FLOC 11 6
FGROUP "Package Size"
STRFILLIN 12 20
ENDFIELD

FIELD sip
FLOC 29 6
FGROUP "Package Type"
CHECKLIST "SIP" "packageStyle"
ENDFIELD

FIELD dip
FLOC 38 6
FGROUP "Package Type"
CHECKLIST "DIP" "packageStyle"
ENDFIELD

FIELD thruHole
FLOC 49 6
FGROUP "Mounting Method"
CHECKLIST "Thru Hole" "mountingStyle"
ENDFIELD

FIELD width
FLOC 11 8
FGROUP "Package Size"
STRFILLIN 12 20
ENDFIELD

FIELD pga
FLOC 29 8
FGROUP "Package Type"
CHECKLIST "PGA" "packageStyle"
ENDFIELD

FIELD qfp
FLOC 38 8
FGROUP "Package Type"
CHECKLIST "QFP" "packageStyle"
ENDFIELD

FIELD smd
FLOC 49 8
FGROUP "Mounting Method"
CHECKLIST "SMD" "mountingStyle"
ENDFIELD

FIELD done
FLOC 1 13
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 16 13
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 55 13
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "Mounting Method"
GLOC 47 4
GSIZE 18 7
ENDGROUP

GROUP "Package Type"
GLOC 27 4
GSIZE 19 7
ENDGROUP

GROUP "Package Size"
GLOC 1 4
GSIZE 25 7
ENDGROUP

ENDTILE

ENDFORM
