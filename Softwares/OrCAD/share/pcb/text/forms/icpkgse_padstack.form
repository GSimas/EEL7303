FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 80 34
HEADER "Symbol Editor - Padstack Info."

POPUP <PADPOPUP>"""".
POPUP <MAJORPOPUP>"""".
POPUP <MINORPOPUP>"""".

TILE

GROUP "Method"
GLOC 3 1
GSIZE 75 9
ENDGROUP

FIELD pad_create
FLOC 5 3
CHECKLIST "New" "pad_method"
ENDFIELD

FIELD pad_available
FLOC 5 5
CHECKLIST "Available Padstack:" "pad_method"
ENDFIELD

FIELD pad_database
FLOC 24 5
ENUMSET 39
POP "PADPOPUP"
ENDFIELD

FIELD pad_library
FLOC 5 7
CHECKLIST "Load from Disk:" "pad_method"
ENDFIELD

FIELD pad_file_name
FLOC 24 7
STRFILLIN 40 128
ENDFIELD

FIELD pad_file_browse
FLOC 67 7
MENUBUTTON "Browse" 9 3
ENDFIELD

GROUP "Specifications"
GLOC 3 11
GSIZE 75 23
ENDGROUP

TEXT "Name:"
TLOC 5 13
ENDTEXT

FIELD pad_name
FLOC 24 13
STRFILLIN 40 128
ENDFIELD

TEXT "Layer:"
TLOC 5 15
ENDTEXT

FIELD pad_layer
FLOC 24 15
ENUMSET 39
POP "MAJORPOPUP"
ENDFIELD

#ifdef CHIPIO
TEXT "RDL:"
TLOC 5 17
INFO pad_alt_label 6
ENDTEXT

FIELD pad_alt_layer
FLOC 24 17
ENUMSET 39
POP "MINORPOPUP"
ENDFIELD
#endif

GROUP "Padstack"
GLOC 5 19
GSIZE 71 14
ENDGROUP

GROUP "Shape"
GLOC 7 21
GSIZE 32 10
ENDGROUP

FIELD pad_circle
FLOC 9 24
CHECKLIST "Circle" "pad_type"
ENDFIELD

FIELD pad_rectangle
FLOC 9 26
CHECKLIST "Rectangle" "pad_type"
ENDFIELD

FIELD pad_octagon
FLOC 9 28
CHECKLIST "Octagon" "pad_type"
ENDFIELD

GROUP "Dimensions"
GLOC 42 21
GSIZE 32 10
ENDGROUP

TEXT "Width:"
TLOC 44 24
ENDTEXT
FIELD pad_width
FLOC 50 24
STRFILLIN 20 128
ENDFIELD

TEXT "Height:"
TLOC 44 26
ENDTEXT
FIELD pad_height
FLOC 50 26
STRFILLIN 20 128
ENDFIELD

FIELD done
FLOC 3 35
MENUBUTTON "Ok" 9 3
ENDFIELD

FIELD cancel
FLOC 15 35
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 69 35
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM

