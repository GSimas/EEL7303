FILE_TYPE=FORM_DEFN VERSION=2
    FORM
    FIXED
    PORT 62 31
    HEADER "Decoupling Capacitor Model Editor"

    ACTIVEFIELD Capacitance
    DEFAULT Done

    TILE
    FIELD Done
      FLOC 2 29
      MENUBUTTON "Done" 10 3
    ENDFIELD

    FIELD Cancel
      FLOC 14 29
      MENUBUTTON "Cancel" 10 3
    ENDFIELD

    FIELD Help
      FLOC 51 29
      MENUBUTTON "Help" 10 3
    ENDFIELD

    FIELD ModelBitmap
        THUMBNAIL pqcapschem.bmp
        FLOC 1 1
        FSIZE 60 4
    ENDFIELD

    GROUP ""
        GLOC 1 5
        GSIZE 61 23
    ENDGROUP

    TEXT "Model name:"
        TLOC 2 7
    ENDTEXT

    TEXT
        TLOC 21 7
        INFO ModelName 27
    ENDTEXT

    FIELD TextEdit
      FLOC 50 7
      MENUBUTTON "Text Edit" 10 3
    ENDFIELD

    TEXT "Capacitance:"
        TLOC 2 10
    ENDTEXT

    FIELD Capacitance
      FLOC 21 10
      STRFILLIN 27 50
    ENDFIELD

    TEXT "Intrinsic inductance (ESL):"
      TLOC 2 13
    ENDTEXT

    FIELD ESL
      FLOC 21 13
      STRFILLIN 27 50
    ENDFIELD

    FIELD ESLCalc
      FLOC 50 13
      MENUBUTTON "Estimate" 10 3
    ENDFIELD

    TEXT "Mounted inductance:"
      TLOC 2 16
    ENDTEXT

    FIELD MountInduct
      FLOC 21 16
      STRFILLIN 27 50
    ENDFIELD

    FIELD MountInductCalc
      FLOC 50 16
      MENUBUTTON "Estimate" 10 3
    ENDFIELD

    TEXT "Intrinsic resistance (ESR):"
      TLOC 2 19
    ENDTEXT

    FIELD ESR
      FLOC 21 19
      STRFILLIN 27 50
    ENDFIELD

    TEXT "Resonant frequency:"
      TLOC 2 22
    ENDTEXT

    TEXT
      TLOC 21 22
      INFO ResonantFreq 27
    ENDTEXT

    FIELD GraphResponse
      FLOC 50 22
      MENUBUTTON "Plot Graph" 10 3
    ENDFIELD

    TEXT "Pin count:"
    TLOC 2 25
    ENDTEXT

    TEXT
      TLOC 21 25
      INFO PinNum 27
    ENDTEXT

    ENDTILE

ENDFORM
