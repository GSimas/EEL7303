FILE_TYPE=FORM_DEFN VERSION=3
FORM toolwindow
VARIABLE
PORT 62 15

HEADER "Resize/Respace Diff Pairs"

TILE TOP
TPANEL 1

TEXT "Current Width"
TLOC 1 1
ENDTEXT

TEXT "Current Gap"
TLOC 12 1
ENDTEXT

TEXT "Layer"
TLOC 22 1
ENDTEXT

TEXT "New Width"
TLOC 32 1
ENDTEXT

TEXT "New Gap"
TLOC 42 1
ENDTEXT

#TEXT "Copy"
#TLOC 52 1
#ENDTEXT

ENDTILE

TILE DIFFPAIR_RULE
TPANEL 1

TEXT ""
TLOC 1 0
INFO old_width_text 9
ENDTEXT

TEXT ""
TLOC 12 0
INFO old_gap_text 9
ENDTEXT

TEXT ""
TLOC 22 0
INFO subclass_text 15
ENDTEXT

FIELD new_width
FLOC 32 0
STRFILLIN 8 8
ENDFIELD

FIELD new_gap
FLOC 42 0
STRFILLIN 8 8
ENDFIELD

FIELD copy
FLOC 52 0
MENUBUTTON "Copy..." 8 3
ENDFIELD

ENDTILE



TILE BOTTOM

FIELD ok
FLOC 18 1
MENUBUTTON "OK" 7 3
ENDFIELD

#FIELD apply
#FLOC 9 1
#MENUBUTTON "Apply" 7 3
#ENDFIELD

#FIELD oops
#FLOC 17 1
#MENUBUTTON "Oops" 7 3
#ENDFIELD

FIELD cancel
FLOC 26 1
MENUBUTTON "Cancel" 7 3
ENDFIELD

FIELD help
FLOC 34 1
MENUBUTTON "Help" 5 3
ENDFIELD

ENDTILE

ENDFORM
