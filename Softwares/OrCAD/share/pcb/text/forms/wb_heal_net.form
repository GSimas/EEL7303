FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 45 7

HEADER "Healed Shape Net Selection"

TILE

TEXT "Ring shapes on two different nets will be merged into a single shape. Please select the net for this new shape from the list below:"
TLOC 1 1
FSIZE 43 5
ENDTEXT

FIELD net_1
FLOC 5 5
CHECKLIST " " "chosen_net"
ENDFIELD
TEXT ""
TLOC 9 5
INFO net_1_name 32 256
ENDTEXT

FIELD net_2
FLOC 5 7
CHECKLIST " " "chosen_net"
ENDFIELD
TEXT ""
TLOC 9 7
INFO net_2_name 32 256
ENDTEXT

TEXT "* This operation may result in some wire bonds being shorted to the wrong net. Please check your design carefully."
TLOC 1 9
FSIZE 43 5
ENDTEXT

FIELD done
FLOC 1 14
MENUBUTTON "OK" 15 3
ENDFIELD

FIELD help
FLOC 36 14
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM

