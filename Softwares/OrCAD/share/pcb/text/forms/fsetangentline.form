FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 27 20
HEADER "Tangent Line/Arc"

POPUP <class>"class""class".
POPUP <subclass>"subclass""subclass".

TILE

TEXT "Active Class and Subclass:"
TLOC 1 1
ENDTEXT

FIELD class
FLOC 5 4
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "class"
ENDFIELD

FIELD subcolor
FLOC 2 7
COLOR 2 1
OPTIONS colorchooser
ENDFIELD

FIELD subclass
FLOC 5 7
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

FIELD mousepick
FLOC 1 11
CHECKLIST "Pick tangent point by mouse" "OPModes"
ENDFIELD

FIELD endpoint
FLOC 1 13
CHECKLIST "Use end point as tangent point" "OPModes"
ENDFIELD

TEXT "Tangent arc radius:"
TLOC 1 16
ENDTEXT

FIELD radius
FLOC 3 18
REALFILLIN 11 128
ENDFIELD

TEXT "Tangent line/arc length:"
TLOC 1 20
ENDTEXT

FIELD length
FLOC 3 22
REALFILLIN 11 128
ENDFIELD

FIELD uselength
FLOC 1 24
CHECKLIST "Use specified length"
ENDFIELD

FIELD isClockwise
FLOC 1 27
CHECKLIST "Clockwise"
ENDFIELD

FIELD isReverse
FLOC 1 29
CHECKLIST "Reverse direction"
ENDFIELD

ENDTILE

ENDFORM

