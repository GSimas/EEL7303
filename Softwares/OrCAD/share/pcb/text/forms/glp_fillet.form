FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 38 57
HEADER "Fillet and Tapered Trace"

POPUP <OBJECTSP>"Pins""1","Vias""2","Ts""3","All""4".

MESSAGE BAD_MAX_ANGLE      1 "Max angle must be greater than or equal to desired angle."
MESSAGE BAD_MIN_LINE_WIDTH 2 "Min line width must be less than max line width."
MESSAGE BAD_MAX_LINE_WIDTH 3 "Max line width must be greater than min line width."
MESSAGE BAD_FILLET_ANGLE   4 "Desired angle must be between 1 and 99."
MESSAGE BAD_MAX_ARC_OFFSET 5 "Max arc offset must be greater than or equal to min arc offset."
MESSAGE BAD_MIN_ARC_OFFSET 6 "Min arc offset must be greater than or equal to 0."



TILE

FIELD DYNAMIC_FILLETS
TLOC 2 3
CHECKLIST "Dynamic"
ENDFIELD

FIELD CURVED_LINES
TLOC 22 3
CHECKLIST "Curved"
ENDFIELD

FIELD ALLOW_DRC
TLOC 2 5
CHECKLIST "Allow DRC"
ENDFIELD

FIELD UNUSED_NETS
FLOC 22 5
CHECKLIST "Unused nets"
ENDFIELD

TEXT "Max size"
TLOC 22 10
ENDTEXT

FIELD ROUND_PADS
FLOC 2 12
CHECKLIST "Circular pads"
ENDFIELD

FIELD MAX_CIRCLE_SIZE
FLOC 22 12
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD SQUARE_PADS
FLOC 2 14
CHECKLIST "Square pads"
ENDFIELD

FIELD MAX_SQUARE_SIZE
FLOC 22 14
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD RECTANGULAR_PADS
FLOC 2 16
CHECKLIST "Rectangular pads"
ENDFIELD

FIELD MAX_RECTANGLE_SIZE
FLOC 22 16
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD OBLONG_PADS
FLOC 2 18
CHECKLIST "Oblong pads"
ENDFIELD

FIELD MAX_OBLONG_SIZE
FLOC 22 18
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD OCTAGON_PADS
FLOC 2 20
CHECKLIST "Octagon pads"
ENDFIELD

FIELD MAX_OCTAGON_SIZE
FLOC 22 20
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD SHAPE_PADS
FLOC 2 23
CHECKLIST "Pads as shapes"
ENDFIELD

FIELD PINS
FLOC 22 23
CHECKLIST "Pins"
ENDFIELD

FIELD PADS_WITHOUT_DRILLS
FLOC 2 25
CHECKLIST "Pads without drills"
ENDFIELD

FIELD VIAS
FLOC 22 25
CHECKLIST "Vias"
ENDFIELD

FIELD FINGERS
FLOC 2 27
CHECKLIST "Bond fingers"
ENDFIELD

FIELD TEES
FLOC 22 27
CHECKLIST "T connections"
ENDFIELD

TEXT "Fillet Object:"
TLOC 2 33
ENDTEXT

FIELD FILLET_OBJECT
FLOC 22 33
ENUMSET 9
POP "OBJECTSP"
ENDFIELD

TEXT "Desired angle:"
TLOC 2 35
ENDTEXT

FIELD FILLET_ANGLE
FLOC 22 35
INTFILLIN 2 2
ENDFIELD

TEXT "Max angle:"
TLOC 2 37
ENDTEXT

FIELD MAX_FILLET_ANGLE
FLOC 22 37
INTFILLIN 2 2
ENDFIELD

TEXT "Max offset:"
TLOC 2 39
ENDTEXT

FIELD MAX_FILLET_OFFSET
FLOC 22 39
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

TEXT "Max arc offset:"
TLOC 2 41
ENDTEXT

FIELD MAX_ARC_OFFSET
FLOC 22 41
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

TEXT "Min arc offset:"
TLOC 2 43
ENDTEXT

FIELD MIN_ARC_OFFSET
FLOC 22 43
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

TEXT "Min line width:"
TLOC 2 45
ENDTEXT

FIELD MIN_LINE_WIDTH
FLOC 22 45
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

TEXT "Max line width:"
TLOC 2 47
ENDTEXT

FIELD MAX_LINE_WIDTH
FLOC 22 47
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD TAPERED_TRACES
FLOC 2 53
CHECKLIST "Tapered traces"
ENDFIELD

TEXT "Desired angle:"
TLOC 2 55
ENDTEXT

FIELD TAPER_FACTOR
FLOC 22 55
INTFILLIN 2 2
MIN 1
MAX 99
ENDFIELD

TEXT "Max offset:"
TLOC 2 57
ENDTEXT

FIELD MAX_TAPER_OFFSET
FLOC 22 57
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD Done
FLOC 1 62
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD help
FLOC 22 62
MENUBUTTON "Help" 9 3
ENDFIELD

GROUP "Global Options"
GLOC 1 1
GSIZE 35 6
ENDGROUP

GROUP "Objects"
GLOC 1 8
GSIZE 35 22
ENDGROUP

GROUP "Fillet Options"
GLOC 1 31
GSIZE 35 19
ENDGROUP

GROUP "Tapered Trace Options"
GLOC 1 51
GSIZE 35 9
ENDGROUP

ENDTILE

ENDFORM
