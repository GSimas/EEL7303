FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "PC Tapered Line"

POPUP <actlayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE PCTAPER

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 22

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Start Width:"
TLOC 1 3
ENDTEXT

FIELD inwidthvar
FLOC 10 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 5
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 5
INFO swidthunits
ENDTEXT

TEXT "End Width:"
TLOC 1 7
ENDTEXT

FIELD outwidthvar
FLOC 10 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD width
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT widthunitslabel
TLOC 14 9
INFO widthunits
ENDTEXT

TEXT "Length:"
TLOC 1 11
ENDTEXT

FIELD lengthvar
FLOC 8 11
MENUBUTTON "..." 3 3
ENDFIELD

FIELD length
FLOC 1 13
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 13
INFO lengthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 15
ENDTEXT

FIELD angle
FLOC 14 15
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 17
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 24
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 24
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbpctaper.bmp
OPTIONS STRETCH
FLOC 32 2
FSIZE 40 20
ENDFIELD

ENDTILE

ENDFORM

