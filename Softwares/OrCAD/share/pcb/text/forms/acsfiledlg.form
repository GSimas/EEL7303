FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 62 12
HEADER ""

POPUP <filter> """".
POPUP <drives> """".

TILE

TEXT "File Name:"
TLOC 1 1
ENDTEXT

TEXT "Directories:"
TLOC 27 1
ENDTEXT

FIELD file
FLOC 1 3
STRFILLIN 22 256
ENDFIELD

TEXT
TLOC 27 3
INFO dir 24
ENDTEXT

FIELD files
FLOC 1 6
LIST "" 23 7
ENDFIELD

FIELD dirs
FLOC 27 6
LIST "" 23 7
ENDFIELD

TEXT "List Files of Type:"
TLOC 1 20
ENDTEXT

TEXT "Drives:"
TLOC 27 20
INFO drvstr 24
ENDTEXT

FIELD filter
FLOC 1 22
ENUMSET 21
POP "filter"
ENDFIELD

FIELD drives
FLOC 27 22
ENUMSET 21
POP "drives"
ENDFIELD

FIELD _ok
FLOC 52 1
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD _cancel
FLOC 52 4
MENUBUTTON "Cancel" 10 3
ENDFIELD

ENDTILE

ENDFORM

