FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 45 18
HEADER "Add Static Ruler"

POPUP <LINELOCKENUM> "Off""0","45""45","90""90".
POPUP <TEXTSIZEENUM> """0".
POPUP <CLINEPINVIASNAPENUM> "Center""0", "Edge""1".

TILE

TEXT "Line lock:"
FLOC 2 1
ENDTEXT

FIELD linelock
FLOC 12 1
ENUMSET 4
POP LINELOCKENUM
ENDFIELD

TEXT "Major division spacing:"
FLOC 2 4
ENDTEXT

FIELD majdivdist
FLOC 12 6
STRFILLIN 12 15
ENDFIELD

TEXT "Text size:"
FLOC 2 9
ENDTEXT

FIELD textblock
FLOC 12 9
ENUMSET 12 1
POP TEXTSIZEENUM
ENDFIELD

TEXT "Cline/Pin/Via Snap location:"
FLOC 2 12
ENDTEXT

FIELD clinepinviasnap
FLOC 12 14
ENUMSET 12
POP CLINEPINVIASNAPENUM
ENDFIELD

FIELD onesegmentonly
CHECKLIST "One segment only"
FLOC 2 17
ENDTEXT

FIELD gridless
CHECKLIST "Gridless"
FLOC 2 19
ENDTEXT


ENDTILE

ENDFORM