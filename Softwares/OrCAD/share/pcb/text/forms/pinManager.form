FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 57 15
HEADER  "LEF Pin Parameters" 

TILE

TEXT "Number of Unassigned pins: "
TLOC 6 1
ENDTEXT

TEXT ""
TLOC 27 1
INFO numUnPins 20
ENDTEXT


GROUP "Pins"
FLOC 6 3
FSIZE 45 24
ENDGROUP


TEXT "Padstack: "
TLOC 9 5
ENDTEXT


FIELD padStackList
FLOC 18 5
LIST "" 30 4
OPTIONS sort
ENDFIELD


TEXT "Pin Use: "
TLOC 9 14
ENDTEXT


FIELD pinUseList
FLOC 18 14
LIST "" 30 4
OPTIONS sort
ENDFIELD


TEXT "Pin Size: "
TLOC 9 22
ENDTEXT


TEXT ""
TLOC 19 22
INFO pinPadSize 20
ENDTEXT


TEXT "Number of matching pins:"
TLOC  9 24
ENDTEXT

TEXT ""
TLOC 28 24
INFO numMatchPins 4
ENDTEXT


GROUP "Macro Data"
FLOC 6 27
FSIZE 45 21
ENDGROUP


TEXT "Macro List: "
TLOC 9 29
ENDTEXT


FIELD macroList
FLOC 18 29
LIST "" 30 4
OPTIONS sort
ENDFIELD

TEXT "** = multiple macro associations"
TLOC 18 37
ENDTEXT

TEXT "Pin Use: "
TLOC 9 39
ENDTEXT

TEXT ""
TLOC 19 39
INFO macroPinUse 20
ENDTEXT

TEXT "Pin Size: "
TLOC 9 41
ENDTEXT

TEXT ""
TLOC 19 41
INFO macroPadSize 20
ENDTEXT

TEXT "Lef Library: "
TLOC 9 43
ENDTEXT

TEXT ""
TLOC 19 43
