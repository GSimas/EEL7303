FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 27 20
HEADER "Arc Tangent To Two Lines/Arcs"

POPUP <class>"class""class".
POPUP <subclass>"subclass""subclass".

TILE

TEXT "Active Class and Subclass:"
TLOC 1 1
ENDTEXT

FIELD class
FLOC 5 4
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "class"
ENDFIELD

FIELD subcolor
FLOC 2 7
COLOR 2 1
OPTIONS colorchooser
ENDFIELD

FIELD subclass
FLOC 5 7
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

FIELD mousepick
FLOC 1 11
CHECKLIST "Pick tangent point by mouse" "OPModes"
ENDFIELD

FIELD endpoint
FLOC 1 13
CHECKLIST "Use end point as tangent point" "OPModes"
ENDFIELD

FIELD isClockwise
FLOC 1 16
CHECKLIST "Clockwise"
ENDFIELD

FIELD isInner
FLOC 1 18
CHECKLIST "Inner tangency"
ENDFIELD

ENDTILE

ENDFORM

