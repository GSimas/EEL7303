FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Single PCB Line(Trace)"

POPUP <actlayers>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".
POPUP <frequnits>"Hz""1","KHz""1000","MHz""1000000","GHz""1000000000".

TILE PCTRACE

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 26

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Line Length:"
TLOC 1 3
ENDTEXT

FIELD lengthvar
FLOC 10 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD length
FLOC 1 5
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 5
INFO lengthunits
ENDTEXT

TEXT "Line Width:"
TLOC 1 7
ENDTEXT

FIELD widthvar
FLOC 10 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD swidth
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 9
INFO swidthunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 11
ENDTEXT

FIELD angle
FLOC 14 11
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

TEXT "Working Frequency:"
TLOC 1 13
ENDTEXT

FIELD frequency
FLOC 1 15
REALFILLIN 10 10
REALMIN 0.0
DECIMAL 10
ENDFIELD

FIELD frequnit
FLOC 13 15
ENUMSET 6
POP "frequnits"
ENDFIELD

TEXT "Electrical Length:"
TLOC 1 17
ENDTEXT

TEXT
FLOC 1 19
INFO electricallength 12
ENDTEXT

TEXT
TLOC 14 19
INFO electricallengthunits 6
ENDTEXT

FIELD newtrace
FLOC 1 21
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD
ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 28
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 28
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbpctrace.bmp
OPTIONS STRETCH
FLOC 32 8
FSIZE 40 10
ENDFIELD

ENDTILE

ENDFORM

