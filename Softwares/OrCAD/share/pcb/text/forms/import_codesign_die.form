FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 48 6
HEADER "Place Co-Design Die"

POPUP <ROTATION_POPUP> "" "".
POPUP <SUBCLASS_POPUP> "" "".

TILE

GROUP "Die Logic"
GLOC 2 1
GSIZE 44 7
ENDGROUP

TEXT "Ref Des:"
TLOC 5 3
ENDTEXT

FIELD refdes
FLOC 22 3
STRFILLIN 21 128
ENDFIELD

FIELD import_nets
FLOC 5 5
CHECKLIST "Import net assignments"
ENDFIELD

GROUP "Die Attachment"
GLOC 2 8
GSIZE 44 13
ENDGROUP

FIELD wirebond
FLOC 5 10
CHECKLIST "Wire bond" "IC type"
ENDFIELD

FIELD flipchip
FLOC 5 12
CHECKLIST "Flip chip" "IC type"
ENDFIELD

#ifdef APD_DIRECT_DIE_ATTACH_ENABLED && APD
FIELD d2d_parent
FLOC 22 10
CHECKLIST "Direct attach parent" "IC type"
ENDFIELD

FIELD d2d_child
FLOC 22 12
CHECKLIST "Direct attach child" "IC type"
ENDFIELD
#endif


FIELD chipup
FLOC 5 16
CHECKLIST "Chip up" "Chip Orientation"
ENDFIELD

FIELD chipdown
FLOC 5 18
CHECKLIST "Chip down" "Chip Orientation"
ENDFIELD

FIELD chiptype_graphic
THUMBNAIL dgflipchipcd.bmp
#ifdef APD_DIRECT_DIE_ATTACH_ENABLED && APD
FLOC 24 15
#else
FLOC 24 13
#endif
FSIZE 13 5
ENDFIELD


GROUP "Die Placement"
GLOC 2 21
GSIZE 44 22
ENDGROUP

TEXT "Pin Layer:"
TLOC 5 23
ENDTEXT

FIELD subclass
FLOC 22 23
ENUMSET 20
OPTIONS prettyprint ownerdrawn
POP "SUBCLASS_POPUP"
ENDFIELD

TEXT "Origin X:"
TLOC 5 25
ENDTEXT

FIELD origin_x
FLOC 22 25
STRFILLIN 21 128
ENDFIELD

TEXT "Origin Y:"
TLOC 5 27
ENDTEXT

FIELD origin_y
FLOC 22 27
STRFILLIN 21 128
ENDFIELD

TEXT "Rotation:"
TLOC 5 29
ENDTEXT

FIELD rotation
FLOC 22 29
ENUMSET 20
POP "ROTATION_POPUP"
ENDFIELD

FIELD optical_shrink
FLOC 5 32
CHECKLIST "Apply IC fabrication optical shrink by:"
ENDFIELD

FIELD shrinkage_value
FLOC 36 32
REALFILLIN 7 16
DECIMAL 2
REALMIN 0.0
REALMAX 100.0
ENDFIELD

FIELD scribe_lines
FLOC 5 34
CHECKLIST "Add scribe width"
ENDFIELD

FIELD north_scribe
FLOC 13 36
STRFILLIN 10 64
ENDFIELD
TEXT "North:"
TLOC 8 36
ENDTEXT

FIELD south_scribe
FLOC 32 36
STRFILLIN 10 64
ENDFIELD
TEXT "South:"
TLOC 27 36
ENDTEXT

FIELD east_scribe
FLOC 13 38
STRFILLIN 10 64
ENDFIELD
TEXT "East:"
TLOC 8 38
ENDTEXT

FIELD west_scribe
FLOC 32 38
STRFILLIN 10 64
ENDFIELD
TEXT "West:"
TLOC 27 38
ENDTEXT

#ifdef APD_DIRECT_DIE_ATTACH_ENABLED && APD
FIELD import_routing
FLOC 5 41
CHECKLIST "Import direct attach routing"
ENDFIELD
#endif

FIELD done
FLOC 3 44
MENUBUTTON "Ok" 10 3
ENDFIELD

FIELD cancel
FLOC 14 44
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD place
FLOC 25 44
MENUBUTTON "Place" 10 3
ENDFIELD

FIELD help
FLOC 36 44
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE
ENDFORM

