FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 30 12
HEADER "Wire Bond Tack Point Options"

TILE

TEXT "Wire end to move:"
TLOC 1 0
ENDTEXT

FIELD move_start
FLOC 3 2
CHECKLIST "Start (pin)" "move_point"
ENDFIELD

FIELD move_end
FLOC 14 2
CHECKLIST "End (finger)" "move_point"
ENDFIELD

TEXT "Axis of movement:"
FLOC 1 4
ENDTEXT

FIELD move_parallel
FLOC 3 6
CHECKLIST "Along axis / die edge" "move_die_axis"
ENDFIELD 

FIELD move_orthogonal
FLOC 3 8
CHECKLIST "Orthogonal to axis / die edge" "move_die_axis"
ENDFIELD 

FIELD auto
FLOC 1 11
CHECKLIST "Auto spread all wires on pad" "move_mode"
ENDFIELD

FIELD move_axis
FLOC 1 13
CHECKLIST "Movement along pad axis" "move_mode"
ENDFIELD

FIELD move_align
FLOC 3 15
CHECKLIST "Interactive" "move_axis_mode"
ENDFIELD

FIELD move_increment
FLOC 14 15
CHECKLIST "Incremental" "move_axis_mode"
ENDFIELD

FIELD radial_dist
FLOC 3 17
STRFILLIN 15 18
ENDFIELD
TEXT "(+/-)"
TLOC 22 17
ENDTEXT

FIELD move_anywhere
FLOC 1 19
CHECKLIST "Free movement inside pad" "move_mode"
ENDFIELD

FIELD copy
FLOC 1 21
CHECKLIST "Copy existing tack point offset" "move_mode"
ENDFIELD

FIELD remove
FLOC 1 23
CHECKLIST "Remove tack point offset(s)" "move_mode"
ENDFIELD

FIELD check_min_gap
FLOC 1 26
CHECKLIST "Min. distance from pad edge:"
ENDFIELD
FIELD min_gap
FLOC 4 28
STRFILLIN 15 18
ENDFIELD

ENDTILE

ENDFORM
