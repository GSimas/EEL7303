FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 80 34
HEADER "Text Wizard, Step 3A. Padstack Info."

POPUP <PADPOPUP>"""".

TILE

GROUP "Method"
GLOC 3 1
GSIZE 75 9
ENDGROUP

FIELD pad_create
FLOC 5 3
CHECKLIST "New" "pad_method"
ENDFIELD

FIELD pad_available
FLOC 5 5
CHECKLIST "Available Padstack:" "pad_method"
ENDFIELD

FIELD pad_database
FLOC 24 5
ENUMSET 39
POP "PADPOPUP"
ENDFIELD

FIELD pad_library
FLOC 5 7
CHECKLIST "Load from Disk:" "pad_method"
ENDFIELD

FIELD pad_file_name
FLOC 24 7
STRFILLIN 40 128
ENDFIELD

FIELD pad_file_browse
FLOC 67 7
MENUBUTTON "Browse" 9 3
ENDFIELD

GROUP "Specifications"
GLOC 3 11
GSIZE 75 21
ENDGROUP

TEXT "Name:"
TLOC 5 13
ENDTEXT

FIELD pad_name
FLOC 24 13
STRFILLIN 40 128
ENDFIELD

GROUP "Pad"
GLOC 5 17
GSIZE 71 14
ENDGROUP

GROUP "Shape"
GLOC 7 19
GSIZE 32 10
ENDGROUP

FIELD pad_circle
FLOC 9 22
CHECKLIST "Circle" "pad_type"
ENDFIELD

FIELD pad_rectangle
FLOC 9 24
CHECKLIST "Rectangle" "pad_type"
ENDFIELD

FIELD pad_octagon
FLOC 9 26
CHECKLIST "Octagon" "pad_type"
ENDFIELD

FIELD pad_oblong
FLOC 9 26
CHECKLIST "Oblong" "pad_type"
ENDFIELD

GROUP "Dimensions"
GLOC 42 19
GSIZE 32 10
ENDGROUP

TEXT "Width:"
TLOC 44 22
ENDTEXT
FIELD pad_width
FLOC 50 22
STRFILLIN 20 128
ENDFIELD

TEXT "Height:"
TLOC 44 24
ENDTEXT
FIELD pad_height
FLOC 50 24
STRFILLIN 20 128
ENDFIELD

#ifdef ICP_TEXT_WIZARD_PAD_OFFSET
TEXT "Offset X:"
TLOC 44 26
ENDTEXT
FIELD pad_offset_x
FLOC 50 26
STRFILLIN 8 128
ENDFIELD
TEXT "Y:"
TLOC 61 26
ENDTEXT
FIELD pad_offset_Y
FLOC 63 26
STRFILLIN 8 128
ENDFIELD
#endif

FIELD back
FLOC 3 33
MENUBUTTON "<Back" 9 3
ENDFIELD

FIELD next
FLOC 13 33
MENUBUTTON "Next>" 9 3
ENDFIELD

FIELD cancel
FLOC 24 33
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 69 33
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM

