FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 64 7
HEADER "OrCAD Layout to Allegro"

MESSAGE PADS_IN_NO_FILENAME 0 "Enter a layout MAX filename first."
MESSAGE PADS_IN_ERROR 0 "A problem occurred - please check the logfile (using Viewlog)."
MESSAGE PADS_IN_NOT_IN_DEMO 0 "Cannot Run in Demo Mode."

ACTIVEFIELD "filename"

TILE
TEXT "Layout MAX file:"
TLOC 1 1
ENDTEXT

FIELD filename
FLOC 18 1
STRFILLIN 37 512
ENDFIELD

FIELD filename_browse
FLOC 58 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Warning: Remember to save active design before translation."
TLOC 3 5
ENDTEXT

FIELD execute
FLOC 1 9
MENUBUTTON "Translate" 13 3
ENDFIELD

FIELD viewlog
FLOC 15 9
MENUBUTTON "Viewlog" 10 3
ENDFIELD

FIELD cancel
FLOC 28 9
MENUBUTTON "Close" 10 3
ENDFIELD


FIELD help
FLOC 52 9
MENUBUTTON "Help" 10 3
ENDFIELD


FLEXMODE StandButtons
FLEX filename 0 0 1 0
FLEX help 0 1 0 0

ENDTILE

ENDFORM
