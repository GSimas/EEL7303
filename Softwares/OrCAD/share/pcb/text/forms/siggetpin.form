FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 43 5
HEADER "Enter Pin"
POPUP <DesignName> """".
POPUP <ComponentName> """".
POPUP <PinNum> """".


ACTIVEFIELD "Design"

TILE
TEXT "Design"
TLOC 2 1
ENDTEXT

TEXT "Component"
TLOC 16 1
ENDTEXT

TEXT "Pin Number"
TLOC 28 1
ENDTEXT

FIELD Design
FLOC 2 3
STRFILLIN 8 40
POP "DesignName"
ENDFIELD

FIELD Component
FLOC 16 3
STRFILLIN 8 40
POP "ComponentName"
ENDFIELD

FIELD PinNumber
FLOC 30 3
STRFILLIN 8 40
POP "PinNum"
ENDFIELD

FIELD OK
FLOC 2 6
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD Cancel
FLOC 16 6
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 28 6
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
