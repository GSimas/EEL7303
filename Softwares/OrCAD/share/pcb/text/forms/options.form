FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 25 10
HEADER "Display Option"


ACTIVEFIELD "Toolbar"

TILE
FIELD Undocked
FLOC 2 4
FGROUP "Control Panel"
CHECKLIST "Undocked" "a"
ENDFIELD

FIELD Docked-left
FLOC 2 6
FGROUP "Control Panel"
CHECKLIST "Docked_left" "a"
ENDFIELD

FIELD Docked-right
FLOC 2 8
FGROUP "Control Panel"
CHECKLIST "Docked_right" "a"
ENDFIELD

FIELD Toolbar
FLOC 2 12
CHECKLIST "Toolbar present"
ENDFIELD

FIELD reset_positions
FLOC 2 15
MENUBUTTON "Restore window positions" 21 3
ENDFIELD

FIELD MFCToolbar
FLOC 2 18
MENUBUTTON "Customize Toolbar..." 21 3
ENDFIELD

FIELD done
FLOC 1 21
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD help
FLOC 14 21
MENUBUTTON "Help" 9 3
ENDFIELD

GROUP "Control Panel"
GLOC 1 1
GSIZE 22 10
ENDGROUP

ENDTILE

ENDFORM
