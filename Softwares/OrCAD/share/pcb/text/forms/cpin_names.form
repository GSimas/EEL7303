FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 48 13
HEADER "Insert/Delete from Padstack List"


MESSAGE DUPLICATE_PIN_NAME        1  "Duplicate Pin Name; Try again"
MESSAGE ILLEGAL_PADSTACK_NAME     1  "Invalid Padstack Name; Try again"
MESSAGE ILLEGAL_PADSTACK_FIGURES  1  "Invalid figures in Padstack; Try again"

TILE

TEXT "Select Padstack"
TLOC 29 6
ENDTEXT

TEXT "   from list"
TLOC 29 8
ENDTEXT

TEXT "   to Delete"
TLOC 29 10
ENDTEXT

TEXT "Insert Padstack:"
TLOC 2 18
ENDTEXT

FIELD PADLIST
FLOC 2 2
LIST "" 26 7
ENDFIELD

FIELD INSERT_PAD
FLOC 19 18
STRFILLIN 23 255
ENDFIELD

FIELD done
FLOC 2 22
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD help
FLOC 19 22
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD cancel
FLOC 36 22
MENUBUTTON "Cancel" 9 3
ENDFIELD

ENDTILE

ENDFORM
