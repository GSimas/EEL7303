FILE_TYPE=FORM_DEFN VERSION=2

    FORM
    FIXED
    PORT 68 28
    HEADER "Dynamic IRDrop Analysis"

    ACTIVEFIELD SkipAudit
    DEFAULT Done

    TILE

    FIELD Done
        FLOC 1 26
        MENUBUTTON "Analyze" 10 3
    ENDFIELD

    FIELD Cancel
        FLOC 12 26
        MENUBUTTON "Cancel" 10 3
    ENDFIELD

    FIELD Apply
        FLOC 47 26
        MENUBUTTON "Apply" 10 3
    ENDFIELD

    FIELD Help
        FLOC 58 26
        MENUBUTTON "Help" 10 3
    ENDFIELD

    GROUP "Description"
        GLOC 1 0
        GSIZE 64 14
    ENDGROUP

    TEXT "1. The dynamic IRDrop analysis is used to ...."
        TLOC 2 2
    ENDTEXT

    TEXT "2. The dynamic IRDrop analysis is used to ...."
        TLOC 2 4
    ENDTEXT

    TEXT "3. The dynamic IRDrop analysis is used to ...."
        TLOC 2 6
    ENDTEXT

    TEXT "4. The dynamic IRDrop analysis is used to ...."
        TLOC 2 8
    ENDTEXT

    TEXT "5. The dynamic IRDrop analysis is used to ...."
        TLOC 2 10
    ENDTEXT

    FIELD SkipAudit
        FLOC 2 17
        CHECKLIST "Skip audit before analysis."
    ENDFIELD

    FIELD SkipReport
        FLOC 2 19
        CHECKLIST "Skip report after analysis."
    ENDFIELD

    FIELD Preference
        FLOC 55 16
        MENUBUTTON "Preferences" 10 3
    ENDFIELD

    FIELD SessionName
        FLOC 55 19
        MENUBUTTON "Session" 10 3
    ENDFIELD

    ENDTILE
ENDFORM
