FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 64 60
HEADER "SMT Library Translator - Setup"

TILE

	GROUP PATH
		FLOC 2 1
		FSIZE 60 28
	ENDGROUP
	
	TEXT  "Input Schematic IFF FIle:"
		FLOC 4 3
	ENDTEXT
	
    FIELD InputSchFileName
        FLOC 8 5
        STRFILLIN 40 256
    ENDFIELD   

    FIELD InputSchFileNameBrowser
        FLOC 53 5
        MENUBUTTON "..." 5 3
    ENDFIELD
    
    TEXT  "Input Layout IFF File:"
		FLOC 4 8
	ENDTEXT
	
    FIELD InputLayoutFileName
        FLOC 8 10
        STRFILLIN 40 256
    ENDFIELD   

    FIELD InputLayoutFileNameBrowser
        FLOC 53 10
        MENUBUTTON "..." 5 3
    ENDFIELD
    
    TEXT  "Output Library Directory:"
		FLOC 4 13
	ENDTEXT
	
    FIELD OutputSymDirName
        FLOC 8 15
        STRFILLIN 40 256
    ENDFIELD   

    FIELD OutputSymDirNameBrowser
        FLOC 53 15
        MENUBUTTON "..." 5 3
    ENDFIELD
    
    TEXT  "Output Package Symbol Directory:"
		FLOC 4 18
	ENDTEXT
	
    FIELD OutputPkgDirName
        FLOC 8 20
        STRFILLIN 40 256
    ENDFIELD   

    FIELD OutputPkgDirNameBrowser
        FLOC 53 20
        MENUBUTTON "..." 5 3
    ENDFIELD

    TEXT  "Output Padstack Directory:"
		FLOC 4 23
	ENDTEXT
	
    FIELD OutputPadDirName
        FLOC 8 25
        STRFILLIN 40 256
    ENDFIELD   

    FIELD OutputPadDirNameBrowser
        FLOC 53 25
        MENUBUTTON "..." 5 3
    ENDFIELD
        
    GROUP OPTION
    	FLOC 2 30
    	FSIZE 60 7
    ENDGROUP
		
	FIELD OVERWRITE
		CHECKLIST  "Overwrite existing parts"
		FLOC 6 32
	ENDFIELD
	
	FIELD SCH_ONLY_MODE
		CHECKLIST "Schematic IFF only"
		FLOC 6 34
	ENDFIELD
	
	FIELD LAYER_MAP
		FLOC 35 32
		MENUBUTTON "Layer map..." 12 3
	ENDFIELD
	
	FIELD NEXT
		FLOC 18 39
		MENUBUTTON "Next >" 9 3
	ENDFIELD
	
	FIELD Cancel
		FLOC 35 39
		MENUBUTTON "Cancel" 9 3
	ENDFIELD
	
	FIELD HELP
		FLOC 50 39
		MENUBUTTON "Help" 9 3
	ENDFIELD	

ENDTILE

ENDFORM