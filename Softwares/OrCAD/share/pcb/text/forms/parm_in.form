FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 44 6
HEADER "Import Parameter File"

MESSAGE nexist 0 "Please input a parameter file."
MESSAGE nerror 0 "A problem occurred; check the logfile."
MESSAGE ndone 0 "Parameter file read completed."
MESSAGE ndemo 0 "Cannot Run in Demo Mode."
MESSAGE ncancel 0 "Parameter file read canceled."

ACTIVEFIELD "parmfile"
TILE
TEXT "Input parameter file:"
TLOC 1 1
ENDTEXT

FIELD parmfile
FLOC 3 3
STRFILLIN 35 512
ENDFIELD

FIELD browse
FLOC 41 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD libbrowse
FLOC 34 6
MENUBUTTON "Library..." 10 3
ENDFIELD

FIELD viewlog
FLOC 25 9
MENUBUTTON "Viewlog..." 9 3
ENDFIELD

FIELD execute
FLOC 1 9
MENUBUTTON "Import" 9 3
ENDFIELD

FIELD cancel
FLOC 13 9
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 36 9
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM



