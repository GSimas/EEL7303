FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 20
HEADER "Dimension Line"

POPUP <DL_TERM1POP>"Arrow""0","Bullet""1","Slash""2","None""3".

POPUP <DL_ARROWTYPEPOP>"3 Point""0","4 Point""1","Open""2".

POPUP <DL_ODDDIMPOP>"Horiz/Vert""0","Odd Axis""3".


TILE
TEXT "Leader:"
TLOC 5 4
TGROUP "Terminations"
ENDTEXT

TEXT "Linear Top/Left:"
TLOC 5 6
TGROUP "Terminations"
ENDTEXT

TEXT "Linear Bottom/Right:"
TLOC 5 8
TGROUP "Terminations"
ENDTEXT

TEXT "Head Type:"
TLOC 5 17
TGROUP "Arrows"
ENDTEXT

TEXT "Head Length:"
TLOC 5 19
TGROUP "Arrows"
ENDTEXT

TEXT "Head Width:"
TLOC 5 21
TGROUP "Arrows"
ENDTEXT

TEXT "Bullet Diameter:"
TLOC 5 27
ENDTEXT

TEXT "Slash Length:"
TLOC 5 29
ENDTEXT

TEXT "Odd Angle Axis:"
TLOC 5 32
ENDTEXT

FIELD leader
FLOC 26 4
FGROUP "Terminations"
ENUMSET 7
POP "DL_TERM1POP"
ENDFIELD

FIELD linear_top
FLOC 26 6
FGROUP "Terminations"
ENUMSET 7
POP "DL_TERM1POP"
ENDFIELD

FIELD linear_bottom
FLOC 26 8
FGROUP "Terminations"
ENUMSET 7
POP "DL_TERM1POP"
ENDFIELD

FIELD fill
FLOC 2 10
FGROUP "Terminations"
CHECKLIST "Fill"
ENDFIELD

FIELD arrow_type
FLOC 23 17
FGROUP "Arrows"
ENUMSET 8
POP "DL_ARROWTYPEPOP"
ENDFIELD

FIELD arrow_length
FLOC 23 19
FGROUP "Arrows"
REALFILLIN 10 10
DECIMAL 3
ENDFIELD

FIELD arrow_width
FLOC 23 21
FGROUP "Arrows"
REALFILLIN 10 10
DECIMAL 3
ENDFIELD

FIELD arrow_inside
FLOC 2 23
FGROUP "Arrows"
CHECKLIST "Arrows Inside Extension Lines"
ENDFIELD

FIELD bullet_diameter
FLOC 23 27
REALFILLIN 10 10
DECIMAL 3
ENDFIELD

FIELD slash_length
FLOC 23 29
REALFILLIN 10 10
DECIMAL 3
ENDFIELD

FIELD odd_angle
FLOC 23 32
ENUMSET 11
POP "DL_ODDDIMPOP"
ENDFIELD

FIELD done
FLOC 2 36
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD help
FLOC 28 36
MENUBUTTON "Help" 9 3
ENDFIELD

GROUP "Terminations"
GLOC 1 1
GSIZE 36 12
ENDGROUP

GROUP "Arrows"
GLOC 1 14
GSIZE 36 12
ENDGROUP

ENDTILE

ENDFORM
