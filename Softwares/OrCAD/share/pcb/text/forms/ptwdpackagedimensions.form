FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 80 38
HEADER "Text Wizard, Step 4. Package Info."

POPUP <PADLAYERPOPUP>"""".

TILE

GROUP "Identifiers"
GLOC 3 1
GSIZE 75 7
ENDGROUP

TEXT "Name:"
TLOC 5 3
ENDTEXT
FIELD package_name
FLOC 15 3
STRFILLIN 59 256
ENDFIELD

TEXT "Ref Des:"
TLOC 5 5
ENDTEXT
FIELD package_refdes
FLOC 15 5
STRFILLIN 59 256
ENDFIELD

GROUP "Placement"
GLOC 3 8
GSIZE 75 7
ENDGROUP

TEXT "X coord:"
TLOC 5 10
ENDTEXT
FIELD package_x
FLOC 12 10
STRFILLIN 10 128
ENDFIELD

TEXT "Y coord:"
TLOC 27 10
ENDTEXT
FIELD package_y
FLOC 34 10
STRFILLIN 10 128
ENDFIELD

TEXT "Rotation:"
TLOC 5 12
ENDTEXT
FIELD package_rot
FLOC 12 12
STRFILLIN 20 128
ENDFIELD

TEXT "Pad layer:"
TLOC 42 12
ENDTEXT
FIELD package_layer
FLOC 50 12
ENUMSET 20
POP "PADLAYERPOPUP"
ENDFIELD

FIELD package_center
FLOC 50 10
CHECKLIST "Center pins on symbol origin"
ENDFIELD

GROUP "Dimensions"
GLOC 3 15
GSIZE 75 9
ENDGROUP

FIELD package_use_dim
FLOC 5 17
CHECKLIST "Center around origin" "use_extents"
ENDFIELD

TEXT "Width:"
TLOC 5 19
ENDTEXT
FIELD package_width
FLOC 15 19
STRFILLIN 10 128
ENDFIELD

TEXT "Height:"
TLOC 5 21
ENDTEXT
FIELD package_height
FLOC 15 21
STRFILLIN 10 128
ENDFIELD

FIELD package_use_extents
FLOC 30 17
CHECKLIST "Extents" "use_extents"
ENDFIELD

TEXT "Bottom left X:"
TLOC 30 19
ENDTEXT
FIELD package_min_x
FLOC 41 19
STRFILLIN 10 128
ENDFIELD

TEXT "Bottom left Y:"
TLOC 30 21
ENDTEXT
FIELD package_min_y
FLOC 41 21
STRFILLIN 10 128
ENDFIELD

TEXT "Top right X:"
TLOC 55 19
ENDTEXT
FIELD package_max_x
FLOC 65 19
STRFILLIN 10 128
ENDFIELD

TEXT "Top right Y:"
TLOC 55 21
ENDTEXT
FIELD package_max_y
FLOC 65 21
STRFILLIN 10 128
ENDFIELD

GROUP "Advanced Options"
GLOC 3 24
GSIZE 75 14
ENDGROUP

FIELD package_flip
FLOC 5 26
CHECKLIST "Flip placed symbol"
ENDFIELD

FIELD package_device_file
FLOC 30 26
CHECKLIST "Reuse device file"
ENDFIELD

FIELD scribe_lines
FLOC 5 30
CHECKLIST "Add Scribe Width"
ENDFIELD

FIELD north_scribe
FLOC 15 32
STRFILLIN 15 64
ENDFIELD
TEXT "North"
TLOC 10 32
INFO nscribe_label 6
ENDTEXT

FIELD south_scribe
FLOC 40 32
STRFILLIN 15 64
ENDFIELD
TEXT "South"
TLOC 35 32
INFO sscribe_label 6
ENDTEXT

FIELD east_scribe
FLOC 15 34
STRFILLIN 15 64
ENDFIELD
TEXT "East"
TLOC 10 34
INFO escribe_label 6
ENDTEXT

FIELD west_scribe
FLOC 40 34
STRFILLIN 15 64
ENDFIELD
TEXT "West"
TLOC 35 34
INFO wscribe_label 6
ENDTEXT

FIELD optical_shrink
FLOC 5 28
CHECKLIST "Apply IC Fabrication Optical Shrink by:"
ENDFIELD

FIELD shrink_value
FLOC 40 28
REALFILLIN 10 16
DECIMAL 2
REALMIN 0.0
REALMAX 100.0
ENDFIELD
TEXT "%"
TLOC 52 28
INFO shrink_percent 6
ENDTEXT

GROUP "Attachment Method"
GLOC 3 38
GSIZE 75 8
INFO package_attach_group
ENDGROUP

FIELD package_wirebond_die
FLOC 5 40
CHECKLIST "Wire bond" "IC type"
ENDFIELD

FIELD package_flipchip_die
FLOC 5 42
CHECKLIST "Flip chip" "IC type"
ENDFIELD

FIELD package_chipup
FLOC 30 40
CHECKLIST "Chip up" "Chip Orientation"
ENDFIELD

FIELD package_chipdown
FLOC 30 42
CHECKLIST "Chip down" "Chip Orientation"
ENDFIELD

FIELD package_bitmap
THUMBNAIL dgwirebondcu.bmp
FLOC 55 40
FSIZE 15 5
ENDFIELD

FIELD back
FLOC 3 47
MENUBUTTON "<Back" 9 3
ENDFIELD

FIELD next
FLOC 13 47
MENUBUTTON "Next>" 9 3
ENDFIELD

FIELD cancel
FLOC 24 47
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 69 47
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM


