FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 62 7
HEADER "PCAD IN"

MESSAGE PADS_IN_NO_FILENAME 0 "You must enter a PDF filename first."
MESSAGE PADS_IN_NO_OPTIONS 0 "You must enter an options filename first."
MESSAGE PADS_IN_ERROR 0 "A problem occurred - please check the logfile (using Viewlog)."
MESSAGE PADS_IN_NOT_IN_DEMO 0 "Cannot Run in Demo Mode."

ACTIVEFIELD "filename"

TILE
TEXT "PDIF Input File:"
TLOC 1 1
ENDTEXT

TEXT "Aperture File:"
TLOC 1 3
ENDTEXT

TEXT "Options File:"
TLOC 1 5
ENDTEXT

TEXT "[Drill Table:]"
TLOC 1 7
ENDTEXT

TEXT "Output Design:"
TLOC 1 9
ENDTEXT

FIELD filename
FLOC 18 1
STRFILLIN 37 512
ENDFIELD

FIELD filename_browse
FLOC 58 1
MENUBUTTON "..." 3 3
ENDFIELD

FIELD aperturefile
FLOC 18 3
STRFILLIN 37 512
ENDFIELD

FIELD aperturefile_browse
FLOC 58 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD options
FLOC 18 5
STRFILLIN 37 512
ENDFIELD

FIELD options_browse
FLOC 58 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD drilltable
FLOC 18 7
STRFILLIN 37 512
ENDFIELD

FIELD drilltable_browse
FLOC 58 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD destination
FLOC 18 9
STRFILLIN 37 512
ENDFIELD

FIELD destination_browse
FLOC 58 9
MENUBUTTON "..." 3 3
ENDFIELD

FIELD show_options_dialog
FLOC 1 11
CHECKLIST "Show options dialog"
ENDFIELD

FIELD execute
FLOC 1 13
MENUBUTTON "Translate" 13 3
ENDFIELD

FIELD viewlog
FLOC 15 13
MENUBUTTON "Viewlog" 10 3
ENDFIELD

FIELD cancel
FLOC 28 13
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD help
FLOC 52 13
MENUBUTTON "Help" 10 3
ENDFIELD


FLEXMODE StandButtons
FLEX filename 0 0 1 0
FLEX options 0 0 1 0
FLEX drilltable 0 0 1 0
FLEX aperturefile 0 0 1 0
FLEX destination 0 0 1 0
FLEX help 0 1 0 0

ENDTILE

ENDFORM
