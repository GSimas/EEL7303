FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 76 26
HEADER "Select Component From Which Model Will Be Updated"

ACTIVEFIELD "Cancel"

TILE

TEXT
TLOC 70 3
INFO BusDesignComp 20
ENDTEXT

TEXT "Sort By:"
TLOC 2 1
ENDTEXT

FIELD DeviceSort
FLOC 10 1
CHECKLIST "Device" "SortType"
ENDFIELD

FIELD RefdesSort
FLOC 40 1
CHECKLIST "RefDes" "SortType"
ENDFIELD

TEXT "Filters:"
TLOC 2 3
ENDTEXT

FIELD DeviceFilter
FLOC 11 3
STRFILLIN 24 30
ENDFIELD

FIELD RefdesFilter
FLOC 41 3
STRFILLIN 24 30
ENDFIELD

FIELD CompList
FLOC 11 6
LIST "" 60 14
ENDFIELD

TEXT "Selected Component:"
TLOC 20 34
ENDTEXT

TEXT
TLOC 40 34
INFO SelectedComp 20
ENDTEXT

FIELD OK
FLOC 3 37
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD Cancel
FLOC 34 37
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 64 37
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
