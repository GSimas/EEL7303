FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 80 33
HEADER "Text Wizard Step 2, File Info."

POPUP <UNITPOPUP>"""".

TILE

TEXT "Please identify the column delimeters used in this file. Delimiters are characters used to mark the boundary between two columns of data."
TLOC 3 1
FSIZE 77 4
ENDTEXT

GROUP "Delimiters"
GLOC 3 4
GSIZE 75 9
ENDGROUP

FIELD delimiter_tab
FLOC 5 6
CHECKLIST "Tab"
ENDFIELD

FIELD delimiter_semicolon
FLOC 5 8
CHECKLIST "Semicolon"
ENDFIELD

FIELD delimiter_comma
FLOC 5 10
CHECKLIST "Comma"
ENDFIELD

FIELD delimiter_space
FLOC 17 6
CHECKLIST "Space"
ENDFIELD

FIELD delimiter_other
FLOC 17 8
CHECKLIST "Other:"
ENDFIELD

FIELD delimiter_char
FLOC 24 8
STRFILLIN 1 1
ENDFIELD

FIELD delimiter_consec
FLOC 35 6
CHECKLIST "Ignore consecutive delimiters"
ENDFIELD

FIELD delimiter_trailing
FLOC 35 8
CHECKLIST "Remove trailing delimiters"
ENDFIELD

TEXT "File name:"
TLOC 3 13
INFO file_name 55
ENDGROUP

GRID grid
FLOC 3 15
FSIZE 75 15
ENDGRID

FIELD back
FLOC 3 30
MENUBUTTON "<Back" 9 3
ENDFIELD

FIELD next
FLOC 13 30
MENUBUTTON "Next>" 9 3
ENDFIELD

FIELD cancel
FLOC 24 30
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 69 30
MENUBUTTON "Help" 9 3
ENDFIELD


ENDTILE

ENDFORM

