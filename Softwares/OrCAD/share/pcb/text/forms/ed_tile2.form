
FILE_TYPE=FORM_DEFN VERSION=2
FORM
VARIABLE
PORT 52 14
HEADER "Edit Tile Parameters"

POPUP <BOOLEAN_VALUES>"TRUE""T","FALSE""F".
#POPUP <SHOW>"Sort by Element""1","Sort by Parameter""2","Inherited Parameters""3","Turn off Show""4".
POPUP <SHOW>"Sort by Element""1","Sort by Parameter""2","Turn off Show""3".

TILE standard_tile
TPANEL 2

FIELD apply
FLOC 8 1
MENUBUTTON "Apply" 9 3
ENDFIELD

FIELD show
FLOC 21 1
MENUBUTTON "Show" 9 3
POP "SHOW"
ENDFIELD
ENDTILE

TILE header_tile
TPANEL 0
TEXT "Parameter"
TLOC 1 0
ENDTEXT
TEXT "Value"
TLOC 19 0
ENDTEXT
ENDTILE

TILE parameter_value_tile
TPANEL 1
TEXT
TLOC 1 0
INFO ele_parameter_name 20
OPTIONS prettyprint
ENDTEXT

FIELD ele_parameter_value
FLOC 16 0
REALFILLIN 9 512
DECIMAL 2
ENDFIELD

TEXT
FLOC 16 3
INFO full_value 1
FSIZE 31 3
ENDTEXT

TEXT "pin pitches"
TLOC 27 0
ENDTEXT

GROUP "Parameter Description"
GLOC 1 5
GSIZE 45 25
ENDGROUP

TEXT
FLOC 3 8
FGROUP "Information"
INFO message 1
FSIZE 40 7
ENDTEXT

FIELD bitmap
THUMBNAIL ""
FGROUP "Information"
FLOC 2 13
FSIZE 30 14 
ENDFIELD

#FIELD slidebar
#FLOC 32 26
#INTSLIDEBAR 1 1
#MIN 1
#MAX 2
#ENDFIELD

FIELD next
FLOC 35 27
MENUBUTTON "Next" 8 3
ENDFIELD

ENDTILE

TILE bool_parameter_tile
TPANEL 1
TEXT
TLOC 1 0
INFO ele_parameter_name 20
OPTIONS prettyprint
ENDTEXT

FIELD bool_parameter_value
FLOC 18 0
ENUMSET 6
POP "BOOLEAN_VALUES"
ENDFIELD

GROUP "Parameter Description"
GLOC 1 3
GSIZE 45 26
ENDGROUP

TEXT
FLOC 3 6
FGROUP "Information"
INFO message 1
FSIZE 40 7
ENDTEXT

FIELD bitmap
THUMBNAIL ""
FGROUP "Information"
FLOC 2 13
FSIZE 30 14 
ENDFIELD

#FIELD slidebar
#FLOC 25 26
#INTSLIDEBAR 2 2
#MIN 1
#MAX 2
#ENDFIELD

ENDTILE


ENDFORM

