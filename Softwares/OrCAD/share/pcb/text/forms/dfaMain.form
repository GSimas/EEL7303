FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 82 13
HEADER "Design For Assembly"


TILE
TEXT "Constraint File Name"
TLOC 3 3
TGROUP "DFA"
ENDTEXT

TEXT "Max Message Count"
TLOC 3 9
TGROUP "DFA"
ENDTEXT

FIELD FileName
FLOC 20 3
FGROUP "DFA"
STRFILLIN 39 256
ENDFIELD

FIELD Browse
FLOC 61 3
FGROUP "DFA"
MENUBUTTON "Browse..." 7 3
ENDFIELD

FIELD Save
FLOC 72 3
FGROUP "DFA"
MENUBUTTON "Save..." 7 3
ENDFIELD
 
TEXT "Mapping  File(s)"
TLOC 3 6
TGROUP "DFA"
ENDTEXT

FIELD MapFileName
FLOC 20 6
FGROUP "DFA"
STRFILLIN 39 256
ENDFIELD

FIELD Count
FLOC 20 9
INTFILLIN 8 8
ENDFIELD

FIELD Constraints
FLOC 5 12
FGROUP "DFA"
MENUBUTTON "Constraint Setup..." 15 3
ENDFIELD

FIELD Execute
FLOC 27 12
FGROUP "DFA"
MENUBUTTON "Run Audit" 10 3
ENDFIELD

FIELD Markers
FLOC 43 12
FGROUP "DFA"
MENUBUTTON "Explore Violations..." 15 3
ENDFIELD

FIELD Report
FLOC 67 12
FGROUP "DFA"
MENUBUTTON "Report..." 11 3
ENDFIELD

FIELD done
FLOC 3 16
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 16 16
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 70 16
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "DFA"
GLOC 2 0
GSIZE 80 15
ENDGROUP

ENDTILE

ENDFORM
