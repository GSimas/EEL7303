FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 6
HEADER "D2D Vias"

POPUP <PADSHAPE_POPUP> "Square" "SQUARE", "Circle" "CIRCLE", "Octagon" "OCTAGON".
POPUP <START_LAYER_POPUP> "" "".
POPUP <END_LAYER_POPUP> "" "".

TILE

TEXT "Start Layer:"
TLOC 1 1
ENDTEXT
FIELD start_layer
FLOC 3 3
ENUMSET 20
POP "START_LAYER_POPUP"
ENDFIELD

TEXT "End Layer:"
TLOC 1 5
ENDTEXT
FIELD end_layer
FLOC 3 7
ENUMSET 20
POP "END_LAYER_POPUP"
ENDFIELD

TEXT "Padstack Name:"
TLOC 1 10
ENDTEXT
FIELD pad_name
FLOC 3 12
STRFILLIN 20 128
ENDFIELD

FIELD pad_layer_suffix
FLOC 3 14
CHECKLIST "Add layer names to base pad name"
ENDFIELD

TEXT "Pad Shape:"
TLOC 1 16
ENDTEXT
FIELD pad_shape
FLOC 3 18
ENUMSET 20
POP "PADSHAPE_POPUP"
ENDFIELD

TEXT "Pad Size:"
TLOC 1 20
ENDTEXT
FIELD pad_size
FLOC 3 22
STRFILLIN 20 128
ENDFIELD

FIELD generate
FLOC 1 26
MENUBUTTON "Generate Padstacks" 30 3
ENDFIELD


TEXT "Add vias to pins"
TLOC 1 30
ENDTEXT

FIELD add_vias_up
FLOC 3 32
CHECKLIST "To layers above"
ENDFIELD
TEXT "Max layers:"
TLOC 5 34
ENDTEXT
FIELD max_layers_up
FLOC 14 34
INTFILLIN 3 10
ENDFIELD

FIELD add_vias_down
FLOC 3 36
CHECKLIST "To layers below"
ENDFIELD
TEXT "Max layers:"
TLOC 5 38
ENDTEXT
FIELD max_layers_down
FLOC 14 38
INTFILLIN 3 10
ENDFIELD

ENDTILE

ENDFORM
