FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 44 15

HEADER "Batch Compare Layer"

POPUP <new_class_popup>     "" "".
POPUP <new_subclass_popup>  "" "".

TILE

TEXT "Class:"
TLOC 1 1
ENDTEXT
FIELD new_class
FLOC 10 1
ENUMSET 30
POP "new_class_popup"
ENDFIELD

TEXT "Subclass:"
TLOC 1 3
ENDTEXT
FIELD new_subclass
FLOC 10 3
STRFILLIN 30 1024
POP "new_subclass_popup"
ENDFIELD 

FIELD done
FLOC 1 7
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD cancel
FLOC 11 7
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 34 7
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM
