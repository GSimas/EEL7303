FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 27 20
HEADER "Undo Multi-Layer Shape Z-Copy"

POPUP <class>"class""class"

TILE

TEXT "Class/Subclass:"
TLOC 1 0
ENDTEXT

FIELD class
FLOC 1 2
ENUMSET 24
POP "class"
ENDFIELD

FIELD subclass
FLOC 1 4
TREEVIEW 26 5
ENDFIELD

FIELD deselAllSubclasses
FLOC 1 14
MENUBUTTON "Clear All Subclasses" 26 3
ENDFIELD

FIELD deselectAllClasses
FLOC 1 16
MENUBUTTON " Clear All Class Selections" 26 3
ENDFIELD

FIELD deleteAll
FLOC 1 20
MENUBUTTON "Delete Selected Z-Copied Shapes" 26 3
ENDFIELD

ENDTILE

ENDFORM

