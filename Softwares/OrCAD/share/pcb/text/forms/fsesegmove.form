FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 28 20
HEADER "Edge Move"

POPUP <class>"class""class".
POPUP <subclass>"subclass""subclass".

TILE

TEXT "Active Class and Subclass:"
TLOC 1 1
ENDTEXT

FIELD class
FLOC 5 4
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "class"
ENDFIELD

FIELD subcolor
FLOC 2 7
COLOR 2 1
OPTIONS colorchooser
ENDFIELD

FIELD subclass
FLOC 5 7
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

FIELD mousepick
FLOC 1 11
CHECKLIST "Move by mouse" "OPModes"
ENDFIELD

FIELD accurateinput
FLOC 1 13
CHECKLIST "Move by accurate offsets" "OPModes"
ENDFIELD

TEXT "Horizontal offset:"
TLOC 3 15
ENDTEXT

FIELD offsetx
FLOC 15 15
REALFILLIN 11 128
ENDFIELD

TEXT "Vertical offset:"
TLOC 3 17
ENDTEXT

FIELD offsety
FLOC 15 17
REALFILLIN 11 128
ENDFIELD

FIELD reference
FLOC 1 20
CHECKLIST "Use reference edge"
ENDFIELD

ENDTILE

ENDFORM

