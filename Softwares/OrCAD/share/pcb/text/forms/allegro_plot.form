FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 76 20
HEADER "Physical Design Plot"


POPUP <SCRIPTP>"Record""record","Replay""replay","Stop""stop".

MESSAGE CANT_SAVE 1 "Unable to save parameters."
MESSAGE SAVED 1 "Saved: %s"
MESSAGE OPEN_FAILED 1 "Failed to open %s, loading defaults."
MESSAGE OPEN_SUCCEEDED 1 "Read:  %s"
MESSAGE GENERAL_MSG 1 "%s"

TILE

FIELD options
FLOC 8 5
MENUBUTTON "Plot options..." 18 5
ENDFIELD

FIELD submit
FLOC 31 5
MENUBUTTON "Plot" 18 5
ENDFIELD

FIELD checkq
FLOC 54 5
MENUBUTTON "Queue status..." 18 5
ENDFIELD

TEXT "IPF file name:"
TLOC 3 14
ENDTEXT

FIELD ipfFileName
FLOC 16 14
STRFILLIN 46 256
ENDFIELD

TEXT "Current plotter:"
TLOC 3 16
ENDTEXT

TEXT
TLOC 16 16
INFO currentPlotter 43
ENDTEXT

TEXT "Parameter file:"
TLOC 3 18
ENDTEXT

FIELD paramFileName
FLOC 16 18
STRFILLIN 46 256
ENDFIELD

FIELD loadParam
FLOC 65 18
MENUBUTTON "Load" 8 3
ENDFIELD

FIELD saveParam
FLOC 65 20
MENUBUTTON "Save" 8 3
ENDFIELD

TEXT "Number copies:"
TLOC 3 20
ENDTEXT

FIELD numberOfCopies
FLOC 16 20
INTFILLIN 2 2
MIN 1
ENDFIELD

FIELD toFileOnly
FLOC 2 24
CHECKLIST "File only:"
ENDFIELD

FIELD outputFileName
FLOC 16 24
STRFILLIN 55 1000
ENDFIELD

FIELD sendMail
FLOC 2 26
CHECKLIST "Mail log to:"
ENDFIELD

FIELD userName
FLOC 16 26
STRFILLIN 20 256
ENDFIELD

FIELD plotHeader
FLOC 2 28
CHECKLIST "Plot header:"
ENDFIELD

FIELD titleLine1
FLOC 16 28
STRFILLIN 55 80
ENDFIELD

FIELD titleLine2
FLOC 16 30
STRFILLIN 55 80
ENDFIELD

FIELD titleLine3
FLOC 16 32
STRFILLIN 55 80
ENDFIELD

FIELD done 
FLOC 1 36
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD reset
FLOC 12 36
MENUBUTTON "Reset" 9 3
ENDFIELD

FIELD help
FLOC 23 36
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD script
FLOC 34 36
MENUBUTTON "Script" 10 3
POP "SCRIPTP"
ENDFIELD

GROUP "Commands:"
GLOC 1 1
GSIZE 74 9
ENDGROUP

GROUP "Setup:"
GLOC 1 11
GSIZE 74 24
ENDGROUP

ENDTILE

ENDFORM
