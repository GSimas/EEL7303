FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 100 50
HEADER "3D Field Solver Solution Progress"

TILE

TEXT "Project : "
TLOC 2 2
ENDTEXT

TEXT
TLOC 15 2 
INFO ProjectName 60 
ENDTEXT

TEXT "Start time : "
TLOC 2 4
ENDTEXT

TEXT
TLOC 15 4
INFO StartTime 40
ENDTEXT

TEXT "Clock time elapsed"
TLOC 70 2 
ENDTEXT

TEXT
TLOC 72 4
INFO TimeElapsed 20
ENDTEXT

TEXT "Extract design data : "
TLOC 2 8
ENDTEXT

TEXT 
TLOC 30 8
INFO DataExtStatus 20
ENDTEXT

TEXT "Launch 3D Solver and initialize : "
TLOC 2 11
ENDTEXT

TEXT 
TLOC 30 11
INFO InitStatus 20
ENDTEXT

TEXT "CAD data parsing : "
TLOC 2 14
ENDTEXT

TEXT 
TLOC 30 14
INFO ParseStatus 10
ENDTEXT

FIELD ParseMeter 
FLOC 30 14 
PROGRESS 60 4 
ENDFIELD

TEXT "CAD to mesh : "
TLOC 2 17 
ENDTEXT

TEXT
TLOC 30 17 
INFO CADMeshStatus 10
ENDTEXT

FIELD CADMeshMeter
FLOC 30 17 
PROGRESS 60 4 
ENDFIELD

TEXT 
TLOC 2 20 
INFO 3DMesh 20
ENDTEXT

FIELD 3DMeshMeter
FLOC 30 20 
PROGRESS 60 4 
ENDFIELD

GROUP "Progress Status"
GLOC 1 23
GSIZE 95 15
ENDGROUP

TEXT "Current sweep step"
TLOC 3 25
ENDTEXT

TEXT
TLOC 30 25
INFO ProgressInfo 60
ENDTEXT

TEXT "Matrix formulation"
TLOC 3 28
ENDTEXT

FIELD MatrixMeter
FLOC 30 28
PROGRESS 60 4
ENDFIELD

TEXT "Equation solver"
TLOC 3 31
ENDTEXT

FIELD EquationMeter
FLOC 30 31
PROGRESS 60 4
ENDFIELD

TEXT "FE post processing"
TLOC 3 34
ENDTEXT

FIELD PostMeter
FLOC 30 34 
PROGRESS 60 4
ENDFIELD

TEXT
TLOC 2 40 
INFO Calculation 20
ENDTEXT

FIELD CalculationMeter
FLOC 30 40 
PROGRESS 60 4
ENDFIELD

TEXT "Result Processing"
TLOC 2 43 
INFO Calculation 20
ENDTEXT

FIELD ResultMeter
FLOC 30 43 
PROGRESS 60 4
ENDFIELD

TEXT "* Click the Stop button in the lower-right corner of main frame or press ESC key to cancel."
TLOC 2 46
ENDTEXT

ENDTILE

ENDFORM
