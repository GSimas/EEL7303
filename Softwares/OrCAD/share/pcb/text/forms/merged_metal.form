FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 52 6
HEADER "Generate Merged Metal"

POPUP <COND_LAYER_POP> """".
POPUP <DEST_CLASS_POP> """".
POPUP <DEST_SUBCLASS_POP> """".

TILE

TEXT "Conductor layer: "
TLOC 1 1
ENDTEXT
FIELD cond_layer
FLOC 20 1
ENUMSET 25
POP "COND_LAYER_POP"
ENDFIELD

TEXT "Output Layer: "
TLOC 1 4
ENDTEXT
TEXT "Class: "
TLOC 3 6
ENDTEXT
FIELD dest_class
FLOC 20 6
ENUMSET 25
POP "DEST_CLASS_POP"
ENDFIELD
TEXT "Subclass: "
TLOC 3 8
ENDTEXT
FIELD dest_subclass
FLOC 20 8
STRFILLIN 24 255
POP "DEST_SUBCLASS_POP"
ENDFIELD

FIELD remove_existing
FLOC 3 10
CHECKLIST "Remove existing objects on destination layer"
ENDFIELD

FIELD generate
FLOC 1 13
MENUBUTTON "Generate" 10 3
ENDFIELD

FIELD done
FLOC 12 13
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 23 13
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 41 13
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
