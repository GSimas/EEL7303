FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 52 6
HEADER "Substrate Layer Config"

POPUP <TOP_LAYER_POP> """".
POPUP <BOT_LAYER_POP> """".

TILE

TEXT "Top substrate layer: "
TLOC 1 1
ENDTEXT
FIELD top_substrate
FLOC 20 1
ENUMSET 25
POP "TOP_LAYER_POP"
ENDFIELD

TEXT "Bottom substrate layer: "
TLOC 1 3
ENDTEXT
FIELD bot_substrate
FLOC 20 3
ENUMSET 25
POP "BOT_LAYER_POP"
ENDFIELD

TEXT "The top and bottom substrate layers are the bounding physical layers of your substrate. These layers are used by commands like the SiP die stack editor tool to determine whether a die component lies within a substrate cavity or sits on the substrate surface."
TLOC 1 6
FSIZE 50 8
ENDTEXT

FIELD done
FLOC 1 13
MENUBUTTON "Ok" 10 3
ENDFIELD

FIELD cancel
FLOC 12 13
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 41 13
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
