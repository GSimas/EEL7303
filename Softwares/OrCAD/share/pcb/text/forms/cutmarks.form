FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 40 20
HEADER "Cut Mark Options"

ACTIVEFIELD apply

TILE

GROUP "Line options"
GLOC 1 1
GSIZE 26 14
ENDGROUP

GROUP "Corner type"
GLOC 1 15
GSIZE 26 7
ENDGROUP

TEXT "Offset:"
TLOC 4 3
ENDTEXT

TEXT "Length:"
TLOC 4 5
ENDTEXT

TEXT "Line width:"
TLOC 4 7
ENDTEXT

FIELD cut_offset
FLOC 14 3
REALFILLIN 8 16
REALMIN 0.00
ENDFIELD

FIELD cut_length
FLOC 14 5
REALFILLIN 8 16
REALMIN 0.01
ENDFIELD

FIELD cut_width
FLOC 14 7
REALFILLIN 8 16
REALMIN 0.00
ENDFIELD

FIELD cut_arcsascorners
FLOC 3 10
CHECKLIST "Arcs as corners" "arc"
ENDFIELD

FIELD cut_arcsalwaysfull
FLOC 3 12
CHECKLIST "Always complete arcs" "arc"
ENDFIELD

FIELD cut_outside
FLOC 4 17
CHECKLIST "Outside corners"
ENDFIELD

FIELD cut_inside
FLOC 4 19
CHECKLIST "Inside corners"
ENDFIELD

FIELD apply
FLOC 30 5
MENUBUTTON "Apply" 9 3
ENDFIELD

FIELD cancel
FLOC 30 8
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 30 20
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD ok
FLOC 30 2
MENUBUTTON "OK" 9 3
ENDFIELD

ENDTILE

ENDFORM
