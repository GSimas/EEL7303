FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 30 35
HEADER "Shape to Padstack"

POPUP <LAYERPOPUP>"""".
POPUP <ORIGINPOPUP>"""".
POPUP <ORIGINLAYERPOPUP>"""".

TILE

FIELD create_via
FLOC 1 1
CHECKLIST "Create via and remove shape"
ENDFIELD

FIELD add_to_via_list
FLOC 1 3
CHECKLIST "Add padstack to via list"
ENDFIELD

TEXT "Shape name:"
TLOC 1 6
ENDTEXT
FIELD shape_name
FLOC 3 8
STRFILLIN 20 1024
ENDFIELD

TEXT "Padstack name:"
TLOC 1 11
ENDTEXT
FIELD pad_name
FLOC 3 13
STRFILLIN 20 1024
ENDFIELD

TEXT "Layer:"
TLOC 1 15
ENDTEXT
FIELD pad_layer
FLOC 3 17
ENUMSET 20
POP "LAYERPOPUP"
ENDFIELD

TEXT "Origin:"
TLOC 1 19
ENDTEXT
FIELD pad_origin
FLOC 3 21
ENUMSET 20
POP "ORIGINPOPUP"
ENDFIELD
FIELD pad_origin_layer
FLOC 3 23
ENUMSET 20
POP "ORIGINLAYERPOPUP"
ENDFIELD

ENDTILE
ENDFORM
