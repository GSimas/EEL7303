FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 65 2000
HEADER "DXF In"

POPUP <units>"MILS""MILS","INCHES""INCHES","MM""MM","CM""CM",\
"MICRONS""MICRONS".

ACTIVEFIELD "DxfFile"

TILE
TEXT "DXF file:"
TLOC 5 5
ENDTEXT

TEXT "DXF units:"
TLOC 5 8
ENDTEXT

TEXT "Accuracy:"
TLOC 5 12
ENDTEXT

TEXT
TLOC 16 12
INFO Accuracy 2
ENDTEXT

TEXT "Layer conversion file:"
TLOC 5 22
ENDTEXT

FIELD DxfFile
FLOC 16 5
STRFILLIN 34 1024
ENDFIELD

FIELD BrowseDXF
FLOC 52 5
MENUBUTTON "..." 3 3
ENDFIELD

FIELD Units
FLOC 16 8
STRFILLIN 8 9
POP "units"
ENDFIELD

FIELD TextTable
FLOC 42 9
CHECKLIST "Use default text table"
ENDFIELD

FIELD Incremental
FLOC 42 12
CHECKLIST "Incremental addition"
ENDFIELD

FIELD FillShapes
FLOC 42 15
CHECKLIST "Fill Shapes"
ENDFIELD

FIELD LayerCnvFile
FLOC 21 22
STRFILLIN 29 1024
ENDFIELD

FIELD BrowseLyrCnv
FLOC 52 22
MENUBUTTON "..." 3 3
ENDFIELD

FIELD LibLayer
FLOC 56 22
MENUBUTTON "Lib..." 5 3
ENDFIELD

FIELD EditCnvProfile
FLOC 39 27
MENUBUTTON "Edit/View layers..." 20 3
ENDFIELD

GROUP "DXF file specifications"
GLOC 3 2
GSIZE 60 16
ENDGROUP

GROUP "Conversion profile"
GLOC 3 19
GSIZE 60 11
ENDGROUP

FIELD Import
FLOC 3 32
MENUBUTTON "Import" 10 3
ENDFIELD

FIELD ViewLog
FLOC 18 32
MENUBUTTON "Viewlog..." 10 3
ENDFIELD

FIELD Cancel
FLOC 36 32
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD Help
FLOC 53 32
MENUBUTTON "Help" 10 3
ENDFIELD
 
ENDTILE
 
ENDFORM
