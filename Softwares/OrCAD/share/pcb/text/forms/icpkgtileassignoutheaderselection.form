FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
#VARIABLE
PORT 80 36
HEADER "Header Information."

TILE

TEXT "Select the header information you would like included in the text file for your die."
TLOC 3 1
ENDTEXT

TEXT "The header, as it will look in the file, is previewed in the text window below."
TLOC 3 3
ENDTEXT

GROUP "Basics"
GLOC 3 5
GSIZE 18 10
ENDGROUP

FIELD file_line
FLOC 5 7
CHECKLIST "File name"
ENDFIELD

FIELD date_line
FLOC 5 9
CHECKLIST "Date/Time"
ENDFIELD

FIELD units_line
FLOC 5 11
CHECKLIST "Design units"
ENDFIELD

FIELD instruction_line
FLOC 5 13
CHECKLIST "Instruction"
ENDFIELD

GROUP "Die"
GLOC 23 5
GSIZE 55 10
ENDGROUP

FIELD packname_line
FLOC 25 7
CHECKLIST "Name"
ENDFIELD

FIELD refdes_line
FLOC 25 9
CHECKLIST "Reference designator"
ENDFIELD

FIELD origin_line
FLOC 45 7
CHECKLIST "Origin"
ENDFIELD

FIELD rotate_line
FLOC 45 9
CHECKLIST "Orientation"
ENDFIELD

FIELD bbox_line
FLOC 65 7
CHECKLIST "Extents" "sizes_options"
ENDFIELD

FIELD size_line
FLOC 65 9
CHECKLIST "Dimensions" "sizes_options"
ENDFIELD

FIELD nosize_line
FLOC 65 11
CHECKLIST "No size info" "sizes_options"
ENDFIELD

TEXT "File name:"
TLOC 3 16
INFO file_name 55
ENDGROUP

GRID grid
FLOC 3 18
FSIZE 75 13
ENDGRID

FIELD back
FLOC 3 32
MENUBUTTON "<Back" 9 3
ENDFIELD

FIELD next
FLOC 13 32
MENUBUTTON "Next>" 9 3
ENDFIELD

FIELD cancel
FLOC 24 32
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 69 32
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM

