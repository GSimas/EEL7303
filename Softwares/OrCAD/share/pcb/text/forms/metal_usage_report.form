FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 38 37

HEADER "Metal Usage Report"

POPUP <sort_pop> "" "".
POPUP <grid_pop> "" "".

TILE

TEXT "File Name:"
TLOC 1 2
ENDTEXT
FIELD report_name
FLOC 11 2
STRFILLIN 20 128
ENDFIELD
FIELD report_browse
FLOC 33 2
MENUBUTTON "..." 4 3
ENDFIELD

FIELD select_design
FLOC 3 5
CHECKLIST "Use substrate outline" "selection_type"
ENDFIELD

FIELD select_window
FLOC 3 7
CHECKLIST "Select window region" "selection_type"
ENDFIELD

FIELD select_symbol
FLOC 3 9
CHECKLIST "Select symbol" "selection_type"
ENDFIELD

FIELD select_shape
FLOC 3 11
CHECKLIST "Select shape" "selection_type"
ENDFIELD

FIELD ratio_table
FLOC 1 14
CHECKLIST "Include ratio table for selected layers"
ENDFIELD

GRID grid
FLOC 1 16
FSIZE  36 18
OPTIONS VLINES HLINES USERSIZE
ENDGRID

FIELD write_report
FLOC 1 34
CHECKLIST "Write report"
ENDFIELD

FIELD view_report
FLOC 17 34
CHECKLIST "View report"
ENDFIELD

FIELD report
FLOC 1 37
MENUBUTTON "Report" 9 3
ENDFIELD

FIELD close
FLOC 11 37
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 28 37
MENUBUTTON "Help" 9 3
ENDFIELD


ENDTILE
ENDFORM
