FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 20
HEADER "Ministatus"

POPUP <class_list>"""".
POPUP <subclass_list>"""".

TILE 

GROUP "Shape Options"
GLOC 1 8
GSIZE 25 18
ENDGROUP

TEXT "Copy to Class/Subclass:"
TLOC 1 1
ENDTEXT

FIELD class_name
FLOC 3 4
ENUMSET 20
POP "class_list"
ENDFIELD

FIELD subclass_name
FLOC 3 6
STRFILLIN 20 40
POP "subclass_list"
ENDFIELD

FIELD dynamic_shape
FLOC 2 10
CHECKLIST "Create dynamic shape" 
ENDFIELD

TEXT "Copy:"
TLOC 2 12
ENDTEXT

FIELD copy_void
FLOC 7 13
CHECKLIST "Voids" 
ENDFIELD

FIELD copy_netname
FLOC 7 15
CHECKLIST "Netname"
ENDFIELD

TEXT "Size:"
TLOC 2 17
ENDTEXT

FIELD contract
FLOC 7 18
CHECKLIST "Contract" "ExpandOrContract"
ENDFIELD

FIELD expand
FLOC 7 20
CHECKLIST "Expand" "ExpandOrContract"
ENDFIELD

TEXT "Offset:"
TLOC 3 22
ENDTEXT

FIELD offset
FLOC 9 22
REALFILLIN 12 12
REALMIN 0.0
ENDFIELD

ENDTILE

ENDFORM
