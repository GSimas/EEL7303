FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 40 9
HEADER "Create Assembly Variants"

POPUP <VAR_LIST>"""".
 
TILE

TEXT "Variant:"
TLOC 1 1
ENDTEXT 

FIELD variant_list
FLOC 10 1
ENUMSET 20
POP "var_list"
ENDFIELD 
 
FIELD top_view
FGROUP "Component View"
FLOC 2 7
CHECKLIST "Top Side" "view_group"
ENDFIELD

FIELD bottom_view
FGROUP "Component View"
FLOC 2 9
CHECKLIST "Bottom Side" "view_group"
ENDFIELD

GROUP "Component View"
GLOC 1 5
GSIZE 15 6
ENDGROUP

FIELD pkg_assembly
FGROUP "Package Geometry Option"
FLOC 19 7
CHECKLIST "Use Assembly Data" "pkg_option"
ENDFIELD

FIELD pkg_place_bound
FGROUP "Package Geometry Option"
FLOC 19 9
CHECKLIST "Use Place Bound Data" "pkg_option"
ENDFIELD

GROUP "Package Geometry Option"
GLOC 18 5
GSIZE 22 6
ENDGROUP

FIELD done
FLOC 1 13
MENUBUTTON "OK" 10 3
ENDFIELD 
 
FIELD cancel
FLOC 13 13
MENUBUTTON "Cancel" 10 3
ENDFIELD 
 
FIELD help
FLOC 29 13
MENUBUTTON "Help" 10 3
ENDFIELD 
 
ENDTILE

ENDFORM




