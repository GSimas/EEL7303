FILE_TYPE=FORM_DEFN VERSION=2

    FORM
    FIXED
    PORT 68 36
    HEADER "SMT Library Layer Mapping"

    ACTIVEFIELD "OK"  
    DEFAULT OK       
	
#	MESSAGE warning 2 "%s"
	
    TILE
	
#	POPUP <Layer> "ETCH/TOP""ETCH/TOP", "ETCH/BOTTOM""ETCH/BOTTOM", "BOARD GEOMETRY/SOLDERMASK_TOP""BOARD GEOMETRY/SOLDERMASK_TOP", "BOARD GEOMETRY/SOLDERMASK_BOTTOM""SOLDERMASK_BOTTOM", "BOARD GEOMETRY/PASTEMASK_TOP""BOARD GEOMETRY/PASTEMASK_TOP", "BOARD GEOMETRY/SILKSCREEN_TOP""BOARD GEOMETRY/SILKSCREEN_TOP", "BOARD GEOMETRY/SILKSCREEN_BOTTOM""BOARD GEOMETRY/SILKSCREEN_BOTTOM"

	POPUP <Layer> "TOP""TOP", "BOTTOM""BOTTOM", "SOLDERMASK_TOP""SOLDERMASK_TOP", "SOLDERMASK_BOTTOM""SOLDERMASK_BOTTOM", "PASTEMASK_TOP""PASTEMASK_TOP", "PASTEMASK_BOTTOM""PASTEMASK_BOTTOM", "SILKSCREEN_TOP""SILKSCREEN_TOP", "SILKSCREEN_BOTTOM""SILKSCREEN_BOTTOM", "ASSEMBLY_TOP""ASSEMBLY_TOP", "ASSEMBLY_BOTTOM""ASSEMBLY_BOTTOM", "PIN_NUMBER""PIN_NUMBER"
	POPUP <Class> "ETCH""ETCH", "BOARD GEOMETRY""BOARD GEOMETRY", "PACKAGE GEOMETRY""PACKAGE GEOMETRY", "PIN""PIN", "REF DES""REF DES"
	
    FIELD Help
        FLOC 55 4
        MENUBUTTON "Help" 10 3
    ENDFIELD
    
    FIELD OK
        FLOC 55 8
        MENUBUTTON "OK" 10 3
    ENDFIELD     

    FIELD Cancel
        FLOC 55 12
        MENUBUTTON "Cancel" 10 3
    ENDFIELD
    

    GRID LayerMap
    FLOC 2 1
    FSIZE 50 15
    OPTIONS HLINES VLINES USERSIZE
    
    GHEAD TOP
        HEADSIZE 2
        OPTIONS 3D
    ENDGHEAD
    ENDGRID

    ENDTILE

ENDFORM