GROUP "Area Selection Type:"
GLOC 1 1
GSIZE 27 13
ENDGROUP

FIELD rectangle
FLOC 3 3
CHECKLIST "Rectangle" "Figure"
ENDFIELD

FIELD circle
FLOC 3 5
CHECKLIST "Circle" "Figure"
ENDFIELD

FIELD polygon
FLOC 3 7
CHECKLIST "Polygon" "Figure"
ENDFIELD

TEXT "Line Type:"
TLOC 5 9
ENDTEXT
FIELD line_type
FLOC 15 9
ENUMSET 10
POP "LINETYPE"
ENDFIELD

TEXT "Line Lock:"
TLOC 5 11
ENDTEXT
FIELD line_lock
FLOC 15 11
ENUMSET 10
POP "LOCKTYPE"
ENDFIELD


FIELD polygon
FLOC 3 7
CHECKLIST "Polygon" "Figure"
ENDFIELD

ENDTILE
ENDFORM

