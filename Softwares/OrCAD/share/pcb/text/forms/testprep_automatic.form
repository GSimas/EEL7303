FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 60 31
HEADER "Testprep Automatic"

TILE

FIELD ALLOW_DIRECT_TEST
FLOC 2 1 
CHECKLIST "Allow test directly on pad"
ENDFIELD

FIELD ALLOW_TRACE_TEST
FLOC 2 3 
CHECKLIST "Allow test directly on trace"
ENDFIELD

FIELD ALLOW_AUTO_INSERT
FLOC 2 5 
CHECKLIST "Allow pin escape insertion"
ENDFIELD

FIELD TEST_UNUSED_PINS
FLOC 2 8
CHECKLIST "Test unused pins"
ENDFIELD

TEXT "Execute mode: "
TLOC 5 11
ENDTEXT

FIELD EXECUTE_ABSOLUTE
FLOC 19 11
CHECKLIST "Overwrite" "EXECUTE MODE"
ENDFIELD

FIELD EXECUTE_INCREMENT
FLOC 19 13
CHECKLIST "Incremental" "EXECUTE MODE"
ENDFIELD

TEXT "Via displacement:"
TLOC 5 15
ENDTEXT

TEXT "Min:"
TLOC 19 15
ENDTEXT

FIELD MIN_TEST_DISPLACE
FLOC 24 15
STRFILLIN 14 14
ENDFIELD

TEXT "Max:"
TLOC 19 17
ENDTEXT

FIELD MAX_TEST_DISPLACE
FLOC 24 17
STRFILLIN 14 14
ENDFIELD

FIELD TPTESTPREP
FLOC 42 1
MENUBUTTON "Generate testpoints" 18 3
ENDFIELD

FIELD TPPARAMETERS
FLOC 42 4 
MENUBUTTON "Parameters..." 18 3
ENDFIELD

FIELD TPCLOSE
FLOC 42 7 
MENUBUTTON "Close" 18 3
ENDFIELD

FIELD TPCANCEL
FLOC 42 10
MENUBUTTON "Cancel" 18 3
ENDFIELD

FIELD TPVIEWLOG
FLOC 42 13
MENUBUTTON "View log" 18 3
ENDFIELD

FIELD help
FLOC 42 17
MENUBUTTON "Help" 18 3
ENDFIELD

ENDTILE

ENDFORM
