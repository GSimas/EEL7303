#
# Interface Import Form for Interface Based Design project
#
FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 80 14
HEADER "BGA Abstract Import"
POPUP <comp_pop>"""0".

TILE

TEXT "Component:"
TLOC 2 2
ENDTEXT

FIELD component
FLOC 12 2
STRFILLIN 20 255
POP "comp_pop"
ENDFIELD

TEXT "Input File:"
TLOC 2 5
ENDTEXT

FIELD file
FLOC 12 5
STRFILLIN 50 255
ENDFIELD

FIELD browse
FLOC 65 5
MENUBUTTON "Browse" 9 3
ENDFIELD

FIELD ripup
FLOC 2 9
CHECKLIST "Ripup"
ENDFIELD

FIELD ok
FLOC 4 13
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD cancel
FLOC 18 13
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 34 13
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
