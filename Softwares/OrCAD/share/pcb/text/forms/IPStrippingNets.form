FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 71 18
HEADER "Net Selection for IP Stripping"

TILE

TEXT "Please select the nets you want to keep in the database"
TLOC 1 1
ENDTEXT

GROUP "Available Nets"
GLOC 1 3
GSIZE 32 21
ENDGROUP

TEXT "Name filter:"
TLOC 2 6
ENDTEXT

FIELD namefilter
FLOC 11 6
STRFILLIN 19 255
ENDFIELD

FIELD available_netlist
FLOC 2 9
LIST "" 30 8
ENDFIELD

GROUP "Nets to Keep"
GLOC 39 3
GSIZE 32 21
ENDGROUP

FIELD selected_netlist
FLOC 40 6
LIST "" 30 9
ENDFIELD

FIELD select_all
FLOC 33 12
MENUBUTTON "All-->" 1 3
ENDFIELD

FIELD deselect_all
FLOC 33 15
MENUBUTTON "<--All" 1 3
ENDFIELD

FIELD done
FLOC 10 26
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 30 26
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 50 26
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
