FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 7
HEADER "Center Lines Between Pads"


TILE
TEXT "Minimum move size:"
TLOC 1 1
ENDTEXT

TEXT "Adjacent pad tolerance:"
TLOC 1 3
ENDTEXT

TEXT "Corner type:"
TLOC 1 5
ENDTEXT

TEXT "Line spacing:"
TLOC 1 7
ENDTEXT

FIELD MINIMUM_MOVE
FLOC 20 1
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD PAD_TOLERANCE
FLOC 20 3
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

FIELD CORNER_45
FLOC 12 5
CHECKLIST "45" "CORNER_TYPE_GROUP"
ENDFIELD

FIELD CORNER_90
FLOC 22 5
CHECKLIST "90" "CORNER_TYPE_GROUP"
ENDFIELD

FIELD LINE_SPACING_MIN
FLOC 12 7
CHECKLIST "Minimum" "LINE_SPACING_GROUP"
ENDFIELD

FIELD LINE_SPACING_EVEN
FLOC 22 7
CHECKLIST "Even" "LINE_SPACING_GROUP"
ENDFIELD

FIELD Done
FLOC 1 10
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD layers
FLOC 12 10
MENUBUTTON "Gloss layers..." 9 3
ENDFIELD

FIELD help
FLOC 28 10
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE

ENDFORM
