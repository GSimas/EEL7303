FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 68 60
HEADER "SMT Library Translator - Library"

POPUP <PART_TYPE> "Resistor""Resistor", "Capacitor""Capacitor", "Inductor""Inductor", "Diode""Diode", "Triode""Triode", "SMT IC""SMT IC".

TILE

	GROUP "Library List"
		FLOC 2 0
		FSIZE 33 23
	ENDGROUP
	
	FIELD ComponentNameList
        FLOC 5 3
        LIST ""  28 10
        OPTIONS alphanumsort
    ENDFIELD
    
    GROUP "Part type"
    	FLOC 38 0
    	FSIZE 25 23
    ENDGROUP
    
	FIELD Resistor
		FLOC 42 3
		CHECKLIST Resistor 1
	ENDFIELD
	
	FIELD Capacitor
		FLOC 42 6
		CHECKLIST Capacitor 1
	ENDFIELD
	
	FIELD Inductor
		FLOC 42 9
		CHECKLIST Inductor 1
	ENDFIELD

	FIELD Diode
		FLOC 42 12
		CHECKLIST Diode 1
	ENDFIELD
	
	FIELD Triode
		FLOC 42 15
		CHECKLIST Triode 1
	ENDFIELD
	
	FIELD SMT_IC
		FLOC 42 18
		CHECKLIST "SMT IC" 1
	ENDFIELD
	

	FIELD BACK
		FLOC 3 25
		MENUBUTTON "< Back" 9 3
	ENDFIELD
	
	FIELD NEXT
		FLOC 18 25
		MENUBUTTON "Next >" 9 3
	ENDFIELD
	
	FIELD Cancel
		FLOC 35 25
		MENUBUTTON "Cancel" 9 3
	ENDFIELD
	
	FIELD HELP
		FLOC 50 25
		MENUBUTTON "Help" 9 3
	ENDFIELD

ENDTILE

ENDFORM