FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 38 37
HEADER "Update Symbols"

TILE

TEXT "Select definitions for update:"
TLOC 3 1
ENDTEXT

FIELD tree
FLOC 3 3
TREEVIEW 33 11
ENDFIELD

TEXT "or enter a file containing a list of symbols:" note
TLOC 2 25
ENDTEXT

FIELD symbolfile
FLOC 4 27
STRFILLIN 26 1024
ENDFIELD

FIELD browse
FLOC 32 27
MENUBUTTON "..." 3 3
ENDFIELD

FIELD padstacks
FLOC 2 30
CHECKLIST "Update symbol padstacks"
ENDFIELD

FIELD viewlog
FLOC 27 30
MENUBUTTON "Viewlog..." 10 3
ENDFIELD

FIELD reset_drill_data
FLOC 2 32
CHECKLIST "Reset customizable drill data"
ENDFIELD

FIELD reset_symbol_text
FLOC 2 34
CHECKLIST "Reset symbol text location and size"
ENDFIELD

FIELD reset_pin_escape
FLOC 2 36
CHECKLIST "Reset pin escapes (fanouts)"
ENDFIELD

FIELD ripup_etch
FLOC 2 38
CHECKLIST "Ripup Etch"
ENDFIELD

FIELD ignore_fixed
FLOC 2 40
CHECKLIST "Ignore FIXED property"
ENDFIELD

FIELD refresh
FLOC 1 43
MENUBUTTON "Refresh" 10 3
ENDFIELD

FIELD close
FLOC 13 43
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD help
FLOC 27 43
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

FLEXMODE EdgeGravityOne
FLEX tree 0 0 1 1
FLEX note 0 1 0 0
FLEX symbolfile 0 1 1 0
FLEX browse 1 1 0 0
FLEX viewlog 0 1 0 0
FLEX reset_symbol_text 0 1 0 0

ENDFORM
