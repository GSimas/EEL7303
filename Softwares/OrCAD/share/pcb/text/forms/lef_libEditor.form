FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 67 15
HEADER  "LEF Library Manager" 

POPUP <LEFLIBPOPUP>"""".


TILE

TEXT "Current library directory:"
TLOC 2 1
ENDTEXT

TEXT ""
TLOC 22 1

INFO libFileDir 100
ENDTEXT

TEXT "Current library definition file:"
TLOC 2 4
ENDTEXT

TEXT ""
TLOC 25 4
INFO libFilePath 100
ENDTEXT

FIELD llf_select
FLOC  50 4
MENUBUTTON "&Browse..." 14 3
ENDFIELD

TEXT "New library name:"
TLOC 2 8
ENDTEXT

FIELD lef_newlib
FLOC 16 8
STRFILLIN 15 15
ENDFIELD

FIELD add_lib
FLOC 34 8
MENUBUTTON "&Add" 14 3
ENDFIELD

TEXT "Current library:"
TLOC 2 12
ENDTEXT

FIELD lef_library
FLOC 16 12
ENUMSET 14
POP "LEFLIBPOPUP"
ENDFIELD

FIELD remove_lib
FLOC 34 12
MENUBUTTON "&Remove" 14 3
ENDFIELD

FIELD create_cml
FLOC 50 12
MENUBUTTON "Recreate .cml" 12 3

ENDFIELD

FIELD up_file
FLOC 2 18
MENUBUTTON "" 10 3
BITMAP arrow_up.bmp
ENDFIELD

FIELD down_file
FLOC 2 21
MENUBUTTON "" 10 3
BITMAP arrow_down.bmp
ENDFIELD

FIELD add_file
FLOC 2 24
MENUBUTTON "A&dd..." 10 3
ENDFIELD

FIELD remove_file
FLOC 2 27
MENUBUTTON "R&emove" 10 3
ENDFIELD

FIELD view_macros
FLOC 2 30
MENUBUTTON "&View" 10 3
ENDFIELD

TEXT "LEF files:"
TLOC 14 15
ENDTEXT

FIELD lefpath
FLOC 14 17
LIST "" 34 9
ENDFIELD

TEXT "I/O macros:"
TLOC 50 15
ENDTEXT

FIELD lefmacros
FLOC 50 17
LIST "" 15 9
ENDFIELD

FIELD OK
FLOC 31 35
MENUBUTTON "&OK" 10 3
ENDFIELD
 
FIELD Cancel
FLOC 43 35
MENUBUTTON "&Cancel" 10 3
ENDFIELD

FIELD Help
FLOC 55 35
MENUBUTTON "&Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
