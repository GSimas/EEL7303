BFILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 60 42
HEADER "SPEF Report"

POPUP <freq>"0""0".

POPUP <delimiter>"0""0".

POPUP <res_unit>"0""0"

POPUP <induc_unit>"0""0".

POPUP <cap_unit>"0""0".

POPUP <ind_opt>"0""0".

TILE

TEXT "Output File"
TLOC 2 2
ENDTEXT

TEXT "(optional)"
TLOC 2 3
ENDTEXT

FIELD FileName
FLOC 16 2 
STRFILLIN 28 255 
ENDFIELD

FIELD Browse
FLOC 46 2
MENUBUTTON "Browse" 11 3 
ENDFIELD

FIELD WholeDesign 
FLOC 2 7 
FGROUP "Options"
CHECKLIST "SPEF report for the whole design"
ENDFIELD

FIELD IncludePackage 
FLOC 2 9 
FGROUP "Options"
CHECKLIST "Include parasitic from package interconnection"
ENDFIELD

TEXT "Operation Frequency"
TLOC 2 11 
TGROUP "Options"
ENDTEXT

FIELD Frequency
FLOC 25 11 
FGROUP "Options"
ENUMSET 23
POP "freq" 
ENDFIELD

TEXT "Inductance Option"
TLOC 2 13 
TGROUP "Options"
ENDTEXT

FIELD IndOpt 
FLOC 25 13
ENUMSET 23
POP "ind_opt"
FGROUP "Options"
ENDFIELD

GROUP "Options"
GLOC 1 5 
GSIZE 58 12  
ENDGROUP

TEXT "Design Flow"
TLOC 2 21 
TGROUP "SPEF Header"
ENDTEXT

TEXT 
TLOC 2 23
INFO FlowHint 60 
TGROUP "SPEF Header"
ENDTEXT

FIELD DesignFlow 
FLOC 25 21 
FGROUP "SPEF Header"
STRFILLIN 28 30
ENDFIELD

TEXT "Pin Delimiter"
TLOC 2 25 
TGROUP "SPEF Header"
ENDTEXT

FIELD Delimiter 
FLOC 25 25 
ENUMSET 23
FGROUP "SPEF Header"
POP "delimiter"
ENDFIELD

TEXT "Resistance Unit"
TLOC 2 27 
TGROUP "SPEF Header"
ENDTEXT

FIELD ResUnit 
FLOC 25 27 
ENUMSET 23
FGROUP "SPEF Header"
POP "res_unit"
ENDFIELD

TEXT "Capacitance Unit"
TLOC 2 29 
TGROUP "SPEF Header"
ENDTEXT

FIELD CapUnit 
FLOC 25 29 
ENUMSET 23
FGROUP "SPEF Header"
POP "cap_unit"
ENDFIELD

TEXT "Inductance Unit"
TLOC 2 31 
TGROUP "SPEF Header"
ENDTEXT

FIELD IndUnit 
FLOC 25 31 
ENUMSET 23
FGROUP "SPEF Header"
POP "induc_unit"
ENDFIELD

GROUP "SPEF Header"
GLOC 1 18 
GSIZE 58 17 
ENDGROUP

FIELD OK 
FLOC 6 36 
MENUBUTTON "OK" 11 3
ENDFIELD

FIELD Cancel 
FLOC 24 36 
MENUBUTTON "Cancel" 11 3
ENDFIELD

FIELD Help 
FLOC 42 36 
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE

ENDFORM


