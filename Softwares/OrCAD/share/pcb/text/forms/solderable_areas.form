FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 52 6
HEADER "Generate Solderable Areas"

POPUP <COND_LAYER_POP> """".
POPUP <SOLDER_LAYER_POP> """".
POPUP <DEST_CLASS_POP> """".
POPUP <DEST_SUBCLASS_POP> """".

TILE

TEXT "Conductor layer: "
TLOC 1 1
ENDTEXT
FIELD cond_layer
FLOC 20 1
ENUMSET 25
POP "COND_LAYER_POP"
ENDFIELD

TEXT "Solder mask layer: "
TLOC 1 3
ENDTEXT
FIELD solder_layer
FLOC 20 3
ENUMSET 25
POP "SOLDER_LAYER_POP"
ENDFIELD

TEXT "Output Layer: "
TLOC 1 5
ENDTEXT
TEXT "Class: "
TLOC 3 7
ENDTEXT
FIELD dest_class
FLOC 20 7
ENUMSET 25
POP "DEST_CLASS_POP"
ENDFIELD
TEXT "Subclass: "
TLOC 3 9
ENDTEXT
FIELD dest_subclass
FLOC 20 9
ENUMSET 25
POP "DEST_SUBCLASS_POP"
ENDFIELD

FIELD remove_existing
FLOC 1 11
CHECKLIST "Remove existing objects on destination layer"
ENDFIELD

FIELD generate
FLOC 1 14
MENUBUTTON "Generate" 10 3
ENDFIELD

FIELD done
FLOC 12 14
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 23 14
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 41 14
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
