FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 42 20
HEADER "Signal Analysis Simulation Preferences"

POPUP <NeighborSwitchMode>"Odd""Odd","Even""Even","Odd/Even""Odd/Even".


ACTIVEFIELD "NeighborSwitchMode"

TILE
TEXT "Pulse Clock Frequency"
TLOC 2 4
TGROUP "Pulse Stimuli"
ENDTEXT

TEXT "Pulse Duty Cycle"
TLOC 2 6
TGROUP "Pulse Stimuli"
ENDTEXT

TEXT "Pulse/Step Offset"
TLOC 2 8
TGROUP "Pulse Stimuli"
ENDTEXT

TEXT "Neighbor Switching Mode"
TLOC 2 21
TGROUP "Crosstalk"
ENDTEXT

TEXT "Waveform Resolution"
TLOC 2 32
TGROUP "Waveforms"
ENDTEXT

FIELD PulseClockFrequency
FLOC 28 4
FGROUP "Pulse Stimuli"
STRFILLIN 8 20
ENDFIELD

FIELD PulseDutyCycle
FLOC 28 6
FGROUP "Pulse Stimuli"
STRFILLIN 8 20
ENDFIELD

FIELD PulseStepOffset
FLOC 28 8
FGROUP "Pulse Stimuli"
STRFILLIN 8 20
ENDFIELD

FIELD UseTimingWindows
FLOC 2 15
FGROUP "Crosstalk"
CHECKLIST "Use Timing Windows"
ENDFIELD

FIELD XtalkEachPrimary
FLOC 2 17
FGROUP "Crosstalk"
CHECKLIST "Test Each Driver on Primary Net"
ENDFIELD

FIELD XtalkEachNDrvr
FLOC 2 19
FGROUP "Crosstalk"
CHECKLIST "Stimulate All Neighbor Net Drivers"
ENDFIELD

FIELD NeighborSwitchMode
FLOC 28 21
FGROUP "Crosstalk"
ENUMSET 8
POP "NeighborSwitchMode"
ENDFIELD

FIELD SaveCktWaveFiles
FLOC 2 28
FGROUP "Waveforms"
CHECKLIST "Save Circuit and Waveform Files"
ENDFIELD

FIELD FixedDuration
FLOC 2 30
FGROUP "Waveforms"
CHECKLIST "Fixed Duration"
ENDFIELD

FIELD FixedDurationTime
FLOC 28 30
FGROUP "Waveforms"
STRFILLIN 8 20
ENDFIELD

FIELD WaveFormResolution
FLOC 28 32
FGROUP "Waveforms"
STRFILLIN 8 20
ENDFIELD

FIELD OK
FLOC 2 36
MENUBUTTON "OK" 11 3
ENDFIELD

FIELD Cancel
FLOC 15 36
MENUBUTTON "Cancel" 11 3
ENDFIELD

FIELD help
FLOC 28 36
MENUBUTTON "Help" 11 3
ENDFIELD

GROUP "Pulse Stimuli"
GLOC 1 1
GSIZE 40 10
ENDGROUP

GROUP "Crosstalk"
GLOC 1 12
GSIZE 40 12
ENDGROUP

GROUP "Waveforms"
GLOC 1 25
GSIZE 40 10
ENDGROUP

ENDTILE

ENDFORM
