FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 42 8
HEADER "Select Noise Source Component"

POPUP <compsPop>"None""None".

TILE

TEXT "Components:"
TLOC 1 1
ENDTEXT

FIELD components
FLOC 14 1
STRFILLIN 22 32
POP "compsPop"
ENDFIELD

FIELD done
FLOC 2 5
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 30 5 
MENUBUTTON "Cancel" 10 3
ENDFIELD

ENDTILE

ENDFORM

