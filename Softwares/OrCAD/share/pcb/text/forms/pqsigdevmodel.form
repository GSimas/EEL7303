FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 60 22
HEADER "Model Browser"

POPUP <ModelTypePOP>"placeholder""0".

POPUP <ShowModelsFromPOP>"placeholder""0".

POPUP <AddModelPOP>"placeholder""0".


ACTIVEFIELD "Done"

TILE
TEXT "Show Models From"
TLOC 1 1
ENDTEXT

TEXT "Model Type Filter"
TLOC 1 3
ENDTEXT

TEXT "Model Name Pattern"
TLOC 1 5
ENDTEXT

TEXT "ModelName             ModelType"
TLOC 10 7
ENDTEXT

TEXT "Library    :"
TLOC 1 30
ENDTEXT

TEXT
TLOC 16 30
INFO SelectedDevLib 42
ENDTEXT

FIELD ShowModelsFrom
FLOC 22 1
ENUMSET 34
POP "ShowModelsFromPOP"
ENDFIELD

FIELD ModelTypeFilter
FLOC 22 3
ENUMSET 34
POP "ModelTypePOP"
ENDFIELD

FIELD ModelNamePattern
FLOC 22 5
STRFILLIN 34 40
ENDFIELD

FIELD Model
FLOC 1 9
LIST "" 58 10
ENDFIELD

FIELD AddModel
FLOC 1 33
MENUBUTTON "Add Model" 14 3
POP "AddModelPOP"
ENDFIELD

FIELD DeleteModel
FLOC 16 33
MENUBUTTON "Delete" 14 3
ENDFIELD

FIELD EditModel
FLOC 1 36
MENUBUTTON "Edit" 14 3
ENDFIELD

FIELD TextEdit
FLOC 16 36
MENUBUTTON "TextEdit" 14 3
ENDFIELD

FIELD Close
FLOC 1 41
MENUBUTTON "OK" 14 3
ENDFIELD

FIELD help
FLOC 46 41
MENUBUTTON "Help" 14 3
ENDFIELD

ENDTILE

ENDFORM
