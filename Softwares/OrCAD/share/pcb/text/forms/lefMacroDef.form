FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 68 15
HEADER  "Macro Pin Identification" 

TILE

TEXT "Macro: "
TLOC 2 1
ENDTEXT

TEXT ""
TLOC 9 1
INFO macroName 20
ENDTEXT

TEXT "LEF file: "
TLOC 2 3
ENDTEXT

TEXT ""
TLOC 9 3
INFO lefFilePath 90
ENDTEXT

TEXT "Macro dimensions:"
TLOC  2 5
ENDTEXT

TEXT ""
TLOC 16 5
INFO macroSize 30
ENDTEXT

TEXT "microns"
TLOC  28 5
ENDTEXT

FIELD is_buffer
FLOC 38 5
CHECKLIST "Macro extents define a driver"
ENDFIELD

TEXT "Status: "
TLOC 2 7
ENDTEXT

TEXT ""
TLOC 9 7
INFO statusInfo 20
ENDTEXT

TEXT "I/O pad pins:"
TLOC 2 9
ENDTEXT

FIELD io_pads
FLOC 2 11
LIST "" 15 5
OPTIONS sort
ENDFIELD


TEXT "Pad size:"
TLOC 2 20
ENDTEXT

TEXT ""
TLOC  10 20
INFO iopadSize 14
ENDTEXT

TEXT ""
TLOC 2 22
INFO buffDefined 40
ENDTEXT


FIELD move_to_pad
FLOC 18 12
MENUBUTTON "<<" 3 3
ENDFIELD

FIELD move_from_pad
FLOC 18 14
MENUBUTTON ">>" 3 3
ENDFIELD


TEXT "Unselected pins:"
TLOC 24 9
ENDTEXT

FIELD unselected_pins
FLOC 24  11 
LIST "" 15 5
OPTIONS sort
ENDFIELD


TEXT "Pin size:"
TLOC 24 20
ENDTEXT

TEXT ""
TLOC  31 20
INFO pinSize 14
ENDTEXT

TEXT "Layer:"
TLOC 24 22
ENDTEXT

TEXT ""
TLOC 31 22
INFO pinLayer 40
ENDTEXT

FIELD move_to_cpt
FLOC 41 12
MENUBUTTON ">>" 3 3
ENDFIELD

FIELD move_from_cpt
FLOC 41 14
MENUBUTTON "<<" 3 3
ENDFIELD

TEXT "Driver connection points:"
TLOC 45 9
INFO connectpt_text 25
ENDTEXT

FIELD connectpt_pins
FLOC 46 11
LIST "" 15 5
OPTIONS sort
ENDFIELD

TEXT "Pad size:"
TLOC 45 20
ENDTEXT

TEXT ""
TLOC 53 20
INFO  cptPad 20
ENDTEXT

TEXT "Pad offset"
TLOC 45 22
ENDTEXT

TEXT ""
TLOC 53 22
INFO cptOffset 20
ENDTEXT


FIELD Back
FLOC 18 25
MENUBUTTON "&Back" 10 3
ENDFIELD


FIELD Next
FLOC 29 25
MENUBUTTON "&Next" 10 3
ENDFIELD

FIELD Cancel
FLOC 40 25
MENUBUTTON "&Cancel" 10 3
ENDFIELD

FIELD Help
FLOC 51 25
MENUBUTTON "&Help" 10 3
ENDFIELD

FLEXMODE edgegravity
FLEX unselected_pins 0 0 1 1

ENDTILE

ENDFORM
