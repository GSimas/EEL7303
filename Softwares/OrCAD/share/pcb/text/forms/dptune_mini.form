FILE_TYPE=FORM_DEFN VERSION=1
FORM autogreytext
FIXED
PORT 29 13

HEADER "Phase Tune"

POPUP <length_dbrep_vals>"0""0".
POPUP <height_dbrep_vals>"0""0".

TILE DPTUNE_MINI

TEXT "Bump Style"
FLOC 1 1
ENDFIELD

FIELD dptune_line_mode
FLOC 14 1
CHECKLIST "Line" "BumpStyle"
ENDFIELD

FIELD dptune_arc_mode
FLOC 14 3
CHECKLIST "Arc" "BumpStyle"
ENDFIELD

TEXT "Bump Length"
FLOC 1 5
INFO dptune_length_label 16
ENDFIELD

FIELD dptune_length
FLOC 14 5
STRFILLIN 8 8
POP "length_dbrep_vals"
ENDFIELD

TEXT "Bump Height"
FLOC 1 7
INFO dptune_height_label 16
ENDFIELD

FIELD dptune_height
FLOC 14 7
STRFILLIN 8 8
POP "height_dbrep_vals"
ENDFIELD

TEXT ""
FLOC 1 9
INFO dptune_length_per_bump 35
ENDTEXT

FIELD dptune_dimension_bitmap
THUMBNAIL dptune_line
FLOC 1 11
#FSIZE 22 13
FSIZE 28 12
ENDFIELD

ENDTILE

ENDFORM
