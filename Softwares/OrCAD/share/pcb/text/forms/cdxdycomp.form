FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 20
HEADER "Capacitor dxdy (Artwork)"

POPUP <actlayers>"""".
POPUP <gndlayers>"""".
POPUP <capunits>"fF""0","pF""1","nF""2","uF""3","mF""4".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".

TILE C_dxdy

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 22

TAB "Parameters"
TEXT "Layer:"
TLOC 1 1
ENDTEXT

FIELD active_subclass
FLOC 7 1
ENUMSET 16
POP "actlayers"
ENDFIELD

FIELD activelayervar
FLOC 26 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Device Horiztonal Offset:"
TLOC 1 3
ENDTEXT

FIELD widthvar
FLOC 19 3
MENUBUTTON "..." 3 3
ENDFIELD

FIELD width
FLOC 1 5
REALFILLIN 10 10
ENDFIELD

TEXT widthunitslabel
TLOC 14 5
INFO widthunits
ENDTEXT

TEXT "Device Vertical Offset:"
TLOC 1 7
ENDTEXT

FIELD lengthvar
FLOC 17 7
MENUBUTTON "..." 3 3
ENDFIELD

FIELD length
FLOC 1 9
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 9
INFO lengthunits
ENDTEXT

TEXT "Capacitance:"
TLOC 1 11
ENDTEXT

FIELD capacitancevar
FLOC 11 11
MENUBUTTON "..." 3 3
ENDFIELD

FIELD capacitance
FLOC 1 13
REALFILLIN 10 10
ENDFIELD

FIELD capunit
FLOC 14 13
ENUMSET 9
POP "capunits"
ENDFIELD

TEXT "Rotation Lock:"
TLOC 1 15
ENDTEXT

FIELD angle
FLOC 14 15
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 17
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"

TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 24
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 24
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbcdxdy.bmp
OPTIONS STRETCH
FLOC 32 3
FSIZE 48 24
ENDFIELD

ENDTILE

ENDFORM

