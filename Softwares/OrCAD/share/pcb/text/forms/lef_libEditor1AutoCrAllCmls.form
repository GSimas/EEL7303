FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 53 27
HEADER  "LEF Library Manager" 

POPUP <LEFLIBPOPUP>"""".

TILE


GROUP " Library definition file "
GLOC 2 1
GSIZE 50 7
ENDGROUP

TEXT "Current directory:"
TLOC 4 3
ENDTEXT

TEXT ""
TLOC 17 3
INFO libFileDir 100
ENDTEXT

TEXT "File name:"
TLOC 4 5
ENDTEXT

FIELD libFilePath
FLOC 17 5
STRFILLIN 18 18
ENDFIELD

FIELD llf_select
FLOC  40 5
MENUBUTTON "&Browse..." 10 3
ENDFIELD

GROUP " Library settings "
GLOC 2 8
GSIZE 50 43
ENDGROUP

GROUP "Current library from Library Definition file"
GLOC 3 10
GSIZE 48 6
ENDGROUP

FIELD lef_library
FLOC 5 13
ENUMSET 17
POP "LEFLIBPOPUP"
ENDFIELD

FIELD add_lib
FLOC 28 13
MENUBUTTON "&Add..." 10 3
ENDFIELD

FIELD remove_lib
FLOC 40 13
MENUBUTTON "&Remove" 10 3
ENDFIELD

GROUP " LEF files "
GLOC 3 16
GSIZE 48 34
ENDGROUP

FIELD lefpath
FLOC 5 18
LIST "" 33 6
ENDFIELD

FIELD up_file
FLOC 40 18
MENUBUTTON "" 10 3
BITMAP arrow_up.bmp
ENDFIELD

FIELD down_file
FLOC 40 21
MENUBUTTON "" 10 3
BITMAP arrow_down.bmp
ENDFIELD

FIELD add_file
FLOC 40 24
MENUBUTTON "A&dd..." 10 3
ENDFIELD

FIELD remove_file
FLOC 40 27
MENUBUTTON "R&emove" 10 3
ENDFIELD

TEXT "I/O macros:"
TLOC 5 29
ENDTEXT

FIELD lefmacros
FLOC 5 31
LIST "" 33 4
ENDFIELD

FIELD view_macros
FLOC 40 32
MENUBUTTON "&View" 10 3
ENDFIELD

FIELD editMacro
FLOC 40 35
MENUBUTTON "Edit..." 10 3
ENDFIELD

GROUP " CML settings "
GLOC 4 38
GSIZE 46 8
ENDGROUP

TEXT "File name:"
TLOC 6 41
ENDTEXT

TEXT ""
TLOC 15 41
INFO cmlFileName 100
ENDTEXT

TEXT "Status:"
TLOC 6 43
ENDTEXT

TEXT ""
TLOC 15 43
INFO cmlFileStatus 100
ENDTEXT


FIELD cmlOptions
FLOC 38 40
MENUBUTTON "&Options..." 11 3
ENDFIELD

FIELD create_cml
FLOC 38 43
MENUBUTTON "Create cml" 11 3
ENDFIELD

FIELD autoCreateCml
FLOC 4 47
MENUBUTTON "Auto create all cml files" 46 3
ENDFIELD


FIELD OK
FLOC 8 52
MENUBUTTON "&OK" 11 3
ENDFIELD
 
FIELD Cancel
FLOC 22 52
MENUBUTTON "&Cancel" 11 3
ENDFIELD

FIELD Help
FLOC 36 52
MENUBUTTON "&Help" 11 3
ENDFIELD

ENDTILE

ENDFORM
