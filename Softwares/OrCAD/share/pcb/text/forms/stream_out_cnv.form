FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 95 1
HEADER "Stream Out Edit Layer Conversion File"

POPUP <subClassFilters>""""".
POPUP <classFilters>"""".

TILE

FIELD SelectAll
FLOC 1 1
CHECKLIST "Select all"
ENDFIELD

TEXT "Class filter:"
TLOC 13 1
ENDTEXT

FIELD ClassFilter
FLOC 23 1
ENUMSET 7
POP "classFilters"
STRFILLIN 17 20
ENDFIELD

TEXT "Subclass filter:"
TLOC 45 1
ENDTEXT

FIELD SubClassFilter
FLOC 57 1
ENUMSET 7
POP "subClassFilters"
STRFILLIN 31 48
ENDFIELD

GRID StrOutGrid

FLOC 2 5
FSIZE 93 18
OPTIONS HLINES VLINES

GHEAD TOP 
HEADSIZE 2
OPTIONS 3D MULTI
ENDGHEAD

ENDGRID

GROUP "Map selected items"
GLOC 1 23
GSIZE 93 7
ENDGROUP

TEXT "Layer:"
TLOC 3 25
ENDTEXT
FIELD StrMapLayer
FLOC 8 25
INTFILLIN 10 10
MIN 0
MAX 255
ENDFIELD

TEXT "Data Type:"
TLOC 25 25
ENDTEXT
FIELD StrMapDataType
FLOC 34 25
INTFILLIN 10 10
ENDFIELD

FIELD Map
FLOC 53 25
MENUBUTTON "Map" 18 3
ENDFIELD

FIELD UnMap
FLOC 53 27
MENUBUTTON "Unmap" 18 3
ENDFIELD

FIELD ShowSelectLayers
FLOC 75 25
MENUBUTTON "Show Selected Layers" 18 3
FGROUP "Map selected items"
ENDFIELD

FIELD ShowOriginalLayers
FLOC 75 27
MENUBUTTON "Restore Layer Visibility" 18 3
FGROUP "Map selected items"
ENDFIELD

FIELD done
FLOC 2 31
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 15 31
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 84 31
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
