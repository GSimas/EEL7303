FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 28 20
HEADER "Multi-Layer Scaled Shape Z-Copy"

POPUP <class>"class""class"

TILE

TEXT "Class/Subclass:"
TLOC 1 0
ENDTEXT

FIELD class
FLOC 1 2
ENUMSET 24
POP "class"
ENDFIELD

FIELD subclass
FLOC 1 4
TREEVIEW 26 5
ENDFIELD

FIELD selAllSubclasses
FLOC 1 14
MENUBUTTON "Select All Subclasses" 26 3
ENDFIELD

FIELD deselAllSubclasses
FLOC 1 16
MENUBUTTON "Deselect All Subclasses" 26 3
ENDFIELD

FIELD deselectAllClasses
FLOC 1 18
MENUBUTTON " Clear All Class Selections" 26 3
ENDFIELD

FIELD dynamic
FLOC 1 21
CHECKLIST "Create Dynamic Shape"
ENDFIELD

FIELD setAllDynamic
FLOC 1 23
MENUBUTTON "Dynamic For All Subclasses" 26 3
ENDFIELD

TEXT "Shape Net:" netlabel
TLOC 1 26
ENDTEXT

FIELD srcnetname_list
FLOC 10 26
MENUBUTTON "..." 3 3
ENDFIELD

TEXT
TLOC 1 28
INFO srcnetname 26
ENDTEXT

FIELD setAllSubclassNets
FLOC 1 30
MENUBUTTON "Same Net For All Subclasses" 26 3
ENDFIELD
 
TEXT "Expand(+)/Contract(-):"
TLOC 1 33
ENDTEXT

FIELD expand
FLOC 1 35
REALFILLIN 15 128
ENDFIELD

FIELD setAllSubclassExpands
FLOC 1 37
MENUBUTTON "Same Value For All Subclasses" 26 3
ENDFIELD

TEXT "Offset X:"
TLOC 1 40
ENDTEXT

FIELD offsetx
FLOC 7 40
REALFILLIN 11 128
ENDFIELD

TEXT "Offset Y:"
TLOC 1 42
ENDTEXT

FIELD offsety
FLOC 7 42
REALFILLIN 11 128
ENDFIELD

FIELD setAllSubclassOffsets
FLOC 1 44
MENUBUTTON "Same Value For All Subclasses" 26 3
ENDFIELD

ENDTILE

ENDFORM

