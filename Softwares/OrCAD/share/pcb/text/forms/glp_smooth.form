FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 17
HEADER "Line Smoothing"
TILE

GROUP "Line Smoothing Eliminate"
GLOC 1 1
GSIZE 36 11
ENDGROUP

FIELD ELIMINATE_BUBBLE
FLOC 2 3
CHECKLIST "Bubbles"
ENDFIELD

FIELD ELIMINATE_JOG
FLOC 2 5
CHECKLIST "Jogs"
ENDFIELD

FIELD DANGLING_ELIMINATE
FLOC 2 7
CHECKLIST "Dangling lines"
ENDFIELD

FIELD DANGLING_ELIMINATE_NONET
FLOC 2 9
CHECKLIST "No-net dangling lines"
ENDFIELD

GROUP "Line Smoothing Line Segments"
GLOC 1 13
GSIZE 36 15
ENDGROUP

FIELD PRESERVE_ODDANGLE
FLOC 2 15
CHECKLIST "Preserve odd angle lines if possible"
ENDFIELD

FIELD ADD_FORTYFIVE
FLOC 2 17
CHECKLIST "Convert 90's to 45's"
ENDFIELD

FIELD UTMOST_FORTYFIVE
FLOC 2 19
CHECKLIST "Extend 45's"
ENDFIELD

TEXT "Maximum 45 length:"
TLOC 5 21
ENDTEXT

FIELD MAX_FORTYFIVE
FLOC 24 21
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

TEXT "Length limit:"
TLOC 5 23
ENDTEXT

FIELD LENGTH_LIMIT
FLOC 24 23
REALFILLIN 9 9
DECIMAL 2
ENDFIELD

TEXT "Corner type:"
TLOC 5 25
ENDTEXT

FIELD CORNER_FORTYFIVE
FLOC 17 25
CHECKLIST "45" "CORNER_TYPE_GROUP"
ENDFIELD

FIELD CORNER_NINETY
FLOC 22 25
CHECKLIST "90" "CORNER_TYPE_GROUP"
ENDFIELD

TEXT "Number of executions:"
TLOC 5 29
ENDTEXT

FIELD NUMBER_EXECUTIONS
FLOC 27 29
INTFILLIN 3 3
MIN 0
MAX 32765
ENDFIELD

FIELD Done
FLOC 1 32
MENUBUTTON "Done" 9 3
ENDFIELD

#ifndef central_param_form

FIELD EXECUTE
FLOC 1 32
MENUBUTTON "Gloss" 9 3
ENDFIELD

FIELD CLOSE
FLOC 11 32
MENUBUTTON "Close" 9 3
ENDFIELD

#else

FIELD CLOSE
FLOC 1 32
MENUBUTTON "Close" 9 3
ENDFIELD

#endif

FIELD help
FLOC 28 32
MENUBUTTON "Help" 9 3
ENDFIELD


ENDTILE

ENDFORM
