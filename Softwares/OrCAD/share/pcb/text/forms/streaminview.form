FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 44 2000
HEADER "Stream In View Data"

POPUP <layerFilters> "FORMAT""1","PACKAGE""2","MECHANICAL""3","VIA""4".
POPUP <structFilters>"FORMAT""1","PACKAGE""2","MECHANICAL""3","VIA""4".

TILE

TABSET "Tabs"
FLOC 2 1
TSSIZE 42 43

TAB "Layers" LyrTab

GRID ViewLayersGrid
FLOC 2 9
FSIZE 37 29
OPTIONS HLINES VLINES

GHEAD TOP 
HEADSIZE 2
OPTIONS 3D MULTI
ENDGHEAD

ENDGRID

FIELD SelectAllView
FLOC 2 4
CHECKLIST "Select all to view"
ENDFIELD

FIELD SelectAllImport
FLOC 2 6
CHECKLIST "Select all to import"
ENDFIELD

FIELD LayerFilter
FLOC 16 1
ENUMSET 7
FSIZE  20 20
POP "layerFilters"
STRFILLIN 20 20
ENDFIELD

TEXT "Stream layer filter:"
TLOC 2 1
ENDTEXT

FIELD ViewLayers
FLOC 2 38
MENUBUTTON "View selected layers" 13 3
ENDFIELD

FIELD LayerMapping
FLOC 25 38
MENUBUTTON "Layer mapping..." 13 3
ENDFIELD

ENDTAB

TAB "Structures" StrTab

GRID ViewStructsGrid

FLOC 2 7
FSIZE 37 30
OPTIONS HLINES VLINES

GHEAD TOP 
HEADSIZE 2
OPTIONS 3D MULTI
ENDGHEAD

ENDGRID

FIELD StrSelectAll
FLOC 2 4
CHECKLIST "Select all to view"
ENDFIELD

FIELD StructFilter
FLOC 19 1
ENUMSET 7
FSIZE  14 14
STRFILLIN  14 14
POP "structFilters"
ENDFIELD

TEXT "Stream structure filter:" BlkFilterTxt
TLOC 2 1
ENDTEXT

FIELD ViewStructs
FLOC 19 38
MENUBUTTON "View selected structs" 13 3
ENDFIELD

ENDTAB

ENDTABSET

FIELD Cancel
FLOC 2 46
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD Help
FLOC 32 46
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
