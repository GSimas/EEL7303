FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 31 40
HEADER "Add RF Element"

POPUP <subclass>"subclass""subclass".
POPUP <ElementTypes>"Dummy""0"

TILE

TEXT "Active layer:"
TLOC 1 1
ENDTEXT

FIELD activelayer
FLOC 3 3
ENUMSET 20
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

TEXT "Component categories:"
TLOC 1 5
ENDTEXT

FIELD radio1
FLOC 3 7
CHECKLIST "Microstrip" "rg"
ENDFIELD

FIELD radio2
FLOC 15 7
CHECKLIST "Stripline" "rg"
ENDFIELD

FIELD radio3
FLOC 3 9
CHECKLIST "Multi-layer" "rg"
ENDFIELD

FIELD radio4
FLOC 15 9
CHECKLIST "PCB" "rg"
ENDFIELD

FIELD radio5
FLOC 3 11
CHECKLIST "Waveguide" "rg"
ENDFIELD

FIELD radio6
FLOC 15 11
CHECKLIST "Miscellaneous" "rg"
ENDFIELD

FIELD radio7
FLOC 3 13
CHECKLIST "Lumped" "rg"
ENDFIELD

FIELD radio8
FLOC 15 13
CHECKLIST "Special vias" "rg"
ENDFIELD

TEXT "Element type:"
TLOC 1 15
ENDTEXT

FIELD element_list
FLOC 3 17
#ENUMSET 20
#POP "ElementTypes"
LIST "" 24 10
ENDFIELD

FIELD snaptoconnect
FLOC 1 38
CHECKLIST "Snap to connect point"
ENDFIELD

FIELD snaptopadedge
FLOC 3 40
CHECKLIST "Snap to pad edge"
ENDFIELD

TEXT "Offset to connect point:"
TLOC 1 42
ENDTEXT

FIELD offset
FLOC 3 44
REALFILLIN 12 32
ENDFIELD

FIELD enableinsertion
FLOC 3 47
CHECKLIST "Enable insertion"
ENDFIELD

FIELD enabledrccheck
FLOC 1 49
CHECKLIST "Enable DRC check"
ENDFIELD

FIELD acinit
FLOC 1 51
CHECKLIST "Initialize Clearance"
ENDFIELD

FIELD addassembly
FLOC 3 53
CHECKLIST "Add into existing assembly"
ENDFIELD


ENDTILE

ENDFORM

