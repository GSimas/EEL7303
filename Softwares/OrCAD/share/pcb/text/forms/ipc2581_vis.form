FILE_TYPE=FORM_DEFN VERSION=2
FORM
VARIABLE
PORT 38 12 
HEADER "IPC2581 Compare"


TILE HEADER
TPANEL 0

TEXT "Global visibility:"
TLOC 12 1
ENDTEXT 

FIELD global_on
FLOC 25 1
MENUBUTTON "On" 3 2
ENDFIELD

FIELD global_off
FLOC 31 1
MENUBUTTON "Off" 3 2
ENDFIELD

TEXT "IPC2581 Layer"
TLOC 0 5
OPTIONS underline
ENDTEXT 

TEXT "IPC"    
TLOC 25 5 
OPTIONS underline  
ENDTEXT 

TEXT "Film"    
TLOC 30 5
OPTIONS underline  
ENDTEXT 

TEXT "All"
TLOC 0 7
ENDTEXT 

FIELD all_ipc_visible
FLOC 25 7
CHECKLIST
ENDFIELD

FIELD all_film_visible
FLOC 30 7
CHECKLIST
ENDFIELD
   
ENDTILE



TILE LAYER 
TPANEL 1

TEXT
TLOC 0 0
INFO layer_name 32 
ENDTEXT

FIELD ipc2581_colorvisible
FLOC 25 0
COLOR 2 1 
OPTIONS colorchooser
ENDFIELD

FIELD film_visible
FLOC 30 0
CHECKLIST
ENDFIELD

FIELD film_info
FLOC 33 0
MENUBUTTON "..." 3 2
ENDFIELD


ENDTILE


TILE HELP
TPANEL 2

FIELD close
FLOC 1 1
MENUBUTTON "Close" 3 3
ENDFIELD

FIELD help
FLOC 10 1
MENUBUTTON "Help" 3 3
ENDFIELD
   
ENDTILE


ENDFORM



