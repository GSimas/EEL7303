FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 28 20
HEADER "Vertex Insert"

POPUP <class>"class""class".
POPUP <subclass>"subclass""subclass".

TILE

TEXT "Active Class and Subclass:"
TLOC 1 1
ENDTEXT

FIELD class
FLOC 5 4
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "class"
ENDFIELD

FIELD subcolor
FLOC 2 7
COLOR 2 1
OPTIONS colorchooser
ENDFIELD

FIELD subclass
FLOC 5 7
ENUMSET 19
OPTIONS prettyprint ownerdrawn
POP "subclass"
ENDFIELD

GROUP "Initial insertion offset:"
GLOC 1 10
GSIZE 27 9
ENDGROUP

FIELD initoffset
FLOC 2 12
REALFILLIN 11 128
ENDFIELD

FIELD fromstart
FLOC 2 14
CHECKLIST "From edge start point" "InitOffsetModes"
ENDFIELD

FIELD fromend
FLOC 2 16
CHECKLIST "From edge end point" "InitOffsetModes"
ENDFIELD

GROUP "Destination insertion parameters:"
GLOC 1 19
GSIZE 27 11
ENDGROUP

TEXT "Horizontal offset:"
TLOC 2 21
ENDTEXT

FIELD offsetx
FLOC 15 21
REALFILLIN 10 128
ENDFIELD

TEXT "Vertical offset:"
TLOC 2 23
ENDTEXT

FIELD offsety
FLOC 15 23
REALFILLIN 10 128
ENDFIELD

TEXT "Start remain:"
TLOC 2 25
ENDTEXT

FIELD leftremains
FLOC 15 25
REALFILLIN 7 128
ENDFIELD

TEXT "End remain:"
TLOC 2 27
ENDTEXT

FIELD rightremains
FLOC 15 27
REALFILLIN 7 128
ENDFIELD

ENDTILE

ENDFORM

