FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 83 20
HEADER "Plot Options"

POPUP <PLOTTERNAMES>"PlotterNames""PlotterNames".
POPUP <SIZEUNITS>"Inches""0","Centimeters""1","Millimeters""2".
POPUP <PAPERSIZES>"PaperSizes""PaperSizes".
POPUP <CENTERING>"Auto Center""0","Fit To Page""1","None""2".
POPUP <ENDCAPS>"Octagon""0","Square""1".
POPUP <ARCAPPROX>"Fine""0","Coarse""1".
POPUP <WIDTHS>"True Width""0","Center Line""1","Use Threshold""2".
POPUP <FILLTYPES>"Solid""0","XHatch""1","Hollow""2".

MESSAGE CANT_SAVE 1 "Unable to save parameters."
MESSAGE SAVED 1 "Saved: %s"
MESSAGE OPEN_FAILED 1 "Failed to open %s, loading defaults."
MESSAGE OPEN_SUCCEEDED 1 "Read:  %s"
MESSAGE GENERAL_MSG 1 "%s"

TILE
TEXT "Centering:"
TLOC 45 4
ENDTEXT

TEXT "Plotter name:"
TLOC 3 5
ENDTEXT

TEXT "Paper size:"
TLOC 3 7
ENDTEXT

TEXT "Stipples file:"
TLOC 3 9
ENDTEXT

TEXT "Scale factor:"
TLOC 45 10
ENDTEXT

TEXT "Sizing units:"
TLOC 3 11
ENDTEXT

TEXT "Offset     X:"
TLOC 45 12
ENDTEXT

TEXT "Y:"
TLOC 67 12
ENDTEXT

TEXT "Plot size  X:"
TLOC 45 14
ENDTEXT

TEXT
TLOC 55 14
INFO plotSizeX 10
ENDTEXT

TEXT "Y:"
TLOC 67 14
ENDTEXT

TEXT
TLOC 70 14
INFO plotSizeY 10
ENDTEXT

TEXT "Page extents X:"
TLOC 3 16
ENDTEXT

TEXT
TLOC 15 16
INFO xExtents 10
ENDTEXT

TEXT "Y:"
TLOC 26 16
ENDTEXT

TEXT
TLOC 29 16
INFO yExtents 10
ENDTEXT

TEXT "Total pages:"
TLOC 45 16
ENDTEXT

TEXT
TLOC 55 16
INFO totalPages 10
ENDTEXT

TEXT "IPF extents  X:"
TLOC 3 18
ENDTEXT

TEXT
TLOC 15 18
INFO ipfXExtents 10
ENDTEXT

TEXT "Y:"
TLOC 26 18
ENDTEXT

TEXT
TLOC 29 18
INFO ipfYExtents 10
ENDTEXT

TEXT "End caps:"
TLOC 3 25
ENDTEXT

TEXT "Figures && pads:"
TLOC 45 25
ENDTEXT

TEXT "Arc approximation:"
TLOC 3 27
ENDTEXT

TEXT "Lines && text:"
TLOC 45 27
ENDTEXT

TEXT "Width:"
TLOC 3 29
ENDTEXT

TEXT "Filled rectangles:"
TLOC 45 29
ENDTEXT

TEXT "Threshold width:"
TLOC 3 31
ENDTEXT

TEXT "mils"
TLOC 28 31
ENDTEXT

TEXT "Default line weight:"
TLOC 3 33
ENDTEXT

TEXT "X normal width"
TLOC 28 33
ENDTEXT

FIELD centering
FLOC 55 4
ENUMSET 11
POP "CENTERING"
ENDFIELD

FIELD plotterName
FLOC 16 5
ENUMSET 22
POP "PLOTTERNAMES"
ENDFIELD

FIELD rotate
FLOC 45 6
CHECKLIST "Rotate"
ENDFIELD

FIELD paperSize
FLOC 16 7
ENUMSET 22
POP "PAPERSIZES"
ENDFIELD

FIELD mirror
FLOC 45 8
CHECKLIST "Mirror"
ENDFIELD

FIELD stipplesFile
FLOC 16 9
STRFILLIN 22 512
ENDFIELD

FIELD scale
FLOC 55 10
REALFILLIN 9 9
REALMIN 0.000100
REALMAX 999.999900
DECIMAL 4
ENDFIELD

FIELD sizeUnits
FLOC 16 11
ENUMSET 12
POP "SIZEUNITS"
ENDFIELD

FIELD offsetX
FLOC 55 12
REALFILLIN 9 9
DECIMAL 4
ENDFIELD

FIELD offsetY
FLOC 70 12
REALFILLIN 9 9
DECIMAL 4
ENDFIELD

FIELD multiplePlots
FLOC 45 18
CHECKLIST "OK to plot multipage plots"
ENDFIELD

FIELD endCaps
FLOC 22 25
ENUMSET 14
POP "ENDCAPS"
ENDFIELD

FIELD figuresAndPads
FLOC 61 25
ENUMSET 12
POP "FILLTYPES"
ENDFIELD

FIELD arcApprox
FLOC 22 27
ENUMSET 14
POP "ARCAPPROX"
ENDFIELD

FIELD linesAndText
FLOC 61 27
ENUMSET 12
POP "FILLTYPES"
ENDFIELD

FIELD width
FLOC 22 29
ENUMSET 14
POP "WIDTHS"
ENDFIELD

FIELD filledRects
FLOC 61 29
ENUMSET 12
POP "FILLTYPES"
ENDFIELD

FIELD thresholdWidth
FLOC 22 31
INTFILLIN 3 3
ENDFIELD

FIELD defaultLineWeight
FLOC 22 33
INTFILLIN 3 3
MIN 1
ENDFIELD

FIELD done
FLOC 1 37
MENUBUTTON "Done" 9 3
ENDFIELD

FIELD cancel
FLOC 12 37
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD reset
FLOC 23 37
MENUBUTTON "Reset" 9 3
ENDFIELD

FIELD help
FLOC 34 37
MENUBUTTON "Help" 9 3
ENDFIELD

GROUP "Plotter setup:"
GLOC 1 1
GSIZE 41 20
ENDGROUP

GROUP "Orientation setup:"
GLOC 43 1
GSIZE 39 20
ENDGROUP

GROUP "Line/Arc options:"
GLOC 1 22
GSIZE 41 14
ENDGROUP

GROUP "Filled object options:"
GLOC 43 22
GSIZE 39 14
ENDGROUP

ENDTILE

ENDFORM
