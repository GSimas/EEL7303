FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 60 14
HEADER "Split Plane Params"

POPUP <FILLSTYLEPOP>"Solid""Solid","Vertical""Vertical","Horizontal""Horizontal","Diag_Pos""Diag_Pos",\
"Diag_Neg""Diag_Neg","Diag_Both""Diag_Both","Hori_Vert""Hori_Vert".


ACTIVEFIELD "done"

TILE
TEXT "Note: Split plane parameters only support positive planes."
TLOC 3 4
ENDTEXT

TEXT "Style:"
TLOC 3 6
ENDTEXT

TEXT
TLOC 3 9
INFO xSetPrompt 9
ENDTEXT

TEXT
TLOC 15 9
INFO lineWidthPrompt 10
ENDTEXT

TEXT
TLOC 28 9
INFO spacePrompt 7
ENDTEXT

TEXT
TLOC 45 9
INFO anglePrompt 5
ENDTEXT

TEXT
TLOC 3 11
INFO firstPrompt 6
ENDTEXT

TEXT
TLOC 45 11
INFO firstAngle 10
ENDTEXT

TEXT
TLOC 3 15
INFO secondPrompt 7
ENDTEXT

TEXT
TLOC 45 15
INFO secondAngle 10
ENDTEXT

FIELD fillStyle
FLOC 15 6
ENUMSET 10
POP "FILLSTYLEPOP"
ENDFIELD

FIELD firstLineWidth
FLOC 15 11
STRFILLIN 10 10
ENDFIELD

FIELD firstSpacing
FLOC 28 11
STRFILLIN 10 10
ENDFIELD

FIELD secondLineWidth
FLOC 15 15
STRFILLIN 10 10
ENDFIELD

FIELD secondSpacing
FLOC 28 15
STRFILLIN 10 10
ENDFIELD

FIELD done
FLOC 3 22
MENUBUTTON "OK" 9 3
ENDFIELD

FIELD cancel
FLOC 17 22
MENUBUTTON "Cancel" 9 3
ENDFIELD

FIELD help
FLOC 48 22
MENUBUTTON "Help" 8 3
ENDFIELD

GROUP "Positive Plane Shape Fill"
GLOC 0 1
GSIZE 59 19
ENDGROUP

ENDTILE

ENDFORM
