FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 71 40

HEADER "RDL Router"

POPUP <LAYER_DIRECTION>"Orthogonal""orthogonal","Diagonal""diagonal".

ACTIVEFIELD "execute"

TILE

TEXT "Select RDL Layers to Route" 
TLOC 20 1
ENDTEXT 

GRID "setup_routingLayers"
FSIZE 56 9
FLOC 1 4
OPTIONS HLINES VLINES

GHEAD TOP
HEADSIZE 2
OPTIONS 3D
ENDGHEAD
 
ENDGRID



FIELD close
FLOC 60 1
MENUBUTTON "Close" 11 3
ENDFIELD

FIELD execute
FLOC 60 4
MENUBUTTON "Route" 11 4
ENDFIELD

FIELD undo
FLOC 60 8
MENUBUTTON "Undo" 11 3
ENDFIELD

FIELD results
FLOC 60 11
MENUBUTTON "Results" 11 3
ENDFIELD

FIELD help
FLOC 60 16
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE
ENDFORM
