FSIZE 20 9 
ENDFIELD

FIELD CHIP_DOWN_WB
FLOC 34 15
CHECKLIST "" "rg"
FSIZE 2 9
ENDFIELD

FIELD CD_WB_BMP
THUMBNAIL chip-down_wb.bmp
FLOC 36 15
FSIZE 20 9 
ENDFIELD

FIELD CHIP_UP_FC
FLOC 6 6
CHECKLIST "" "rg"
FSIZE 2 9
ENDFIELD

FIELD CU_FC_BMP
THUMBNAIL chip-up_fc.bmp
FLOC 8 6
FSIZE 20 9 
ENDFIELD

FIELD CHIP_UP_WB
FLOC 34 6
CHECKLIST "" "rg"
FSIZE 2 9
ENDFIELD

FIELD CU_WB_BMP
THUMBNAIL chip-up_wb.bmp
FLOC 36 6
FSIZE 20 9 
ENDFIELD

FIELD TV
