FILE_TYPEt=FORM_DEFN VERSION=2
FORM
FIXED
PORT 62 60
HEADER "New Drawing Configuration"


ACTIVEFIELD "done"

TILE

GROUP ""
FLOC 1 0
FSIZE 60 53
ENDGROUP

TEXT "Select the package configuration desired:"
