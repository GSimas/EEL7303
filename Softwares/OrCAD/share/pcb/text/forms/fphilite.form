FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 46 18
HEADER "Display - Highlight/De-highlight"

TILE
TEXT "Select By:"
TLOC 2 2
TGROUP "Selection Area"
ENDTEXT

TEXT
TLOC 2 4
INFO fpHiliteRefdesNetText 14
TGROUP "Selection Area"
ENDTEXT

TEXT
TLOC 2 6
INFO fpHiliteDeviceText 14
TGROUP "Selection Area"
ENDTEXT

TEXT
TLOC 2 8
INFO fpHiliteSortText 5
TGROUP "Selection Area"
ENDTEXT

TEXT
TLOC 3 10
INFO fpHiliteComponentsTitle 30
TGROUP "Selection Area"
ENDTEXT

TEXT
TLOC 3 10
INFO fpHiliteNetsTitle 14
TGROUP "Selection Area"
ENDTEXT

FIELD fpHiliteNet
FLOC 17 2
CHECKLIST "Net" "SelectBy"
FGROUP "Selection Area"
ENDFIELD

FIELD fpHiliteComponent
FLOC 30 2
CHECKLIST "Component" "SelectBy"
FGROUP "Selection Area"
ENDFIELD

FIELD fpHiliteRefdesNetFilter
FLOC 17 4
STRFILLIN 10 31
FGROUP "Selection Area"
ENDFIELD

FIELD fpHiliteDeviceFilter
FLOC 17 6
STRFILLIN 10 31
FGROUP "Selection Area"
ENDFIELD

FIELD fpHiliteRefdesSort
FLOC 17 8
CHECKLIST "Refdes" "SortKey"
FGROUP "Selection Area"
ENDFIELD

FIELD fpHiliteDeviceSort
FLOC 30 8
CHECKLIST "Device" "SortKey"
FGROUP "Selection Area"
ENDFIELD

FIELD fpHiliteOn
FLOC 10 31
CHECKLIST "Highlight" "visibility"
ENDFIELD

FIELD fpHiliteOff
FLOC 24 31
CHECKLIST "De-highlight" "visibility"
ENDFIELD

FIELD fpHiliteComponents
FLOC 3 12
LIST "" 40 7
FGROUP "Selection Area"
ENDFIELD

FIELD fpHiliteNets
FLOC 3 12
LIST "" 40 7
FGROUP "Selection Area"
ENDFIELD

GROUP "Selection Area"
GLOC 1 0
GSIZE 44 30
ENDGROUP

FIELD fpHiliteSelectAll
FLOC 12 26
MENUBUTTON "Select All" 21 3
ENDFIELD

FIELD Done
FLOC 2 34
MENUBUTTON "Done" 10 3
ENDFIELD

FIELD Apply
FLOC 12 34
MENUBUTTON "Apply" 10 3
ENDFIELD

FIELD Cancel
FLOC 23 34
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 34 34
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
