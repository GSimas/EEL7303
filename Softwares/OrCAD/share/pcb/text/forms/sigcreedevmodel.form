FILE_TYPE=FORM_DEFN VERSION=2
FORM fixed_font
FIXED
PORT 70 9
HEADER "Create ESpice Device Model"

POPUP <CircuitTypePOP>"Resistor""Resistor","Capacitor""Capacitor","Inductor""Inductor".


ACTIVEFIELD "Cancel"

TILE
TEXT "ModelName"
TLOC 1 1
ENDTEXT

TEXT "Circuit type"
TLOC 40 1
ENDTEXT

TEXT "Value"
TLOC 40 4
ENDTEXT

TEXT "Single Pins"
TLOC 1 7
ENDTEXT

TEXT "Common Pin"
TLOC 1 10
ENDTEXT

FIELD ModelName
FLOC 15 1
STRFILLIN 20 40
ENDFIELD

FIELD CircuitType
FLOC 54 1
ENUMSET 10
POP "CircuitTypePOP"
ENDFIELD

FIELD Value
FLOC 54 4
STRFILLIN 10 20
ENDFIELD

FIELD SinglePins
FLOC 15 7
STRFILLIN 49 256
ENDFIELD

FIELD CommonPin
FLOC 15 10
STRFILLIN 10 12
ENDFIELD

FIELD OK
FLOC 1 15
MENUBUTTON "OK" 13 3
ENDFIELD

FIELD Cancel
FLOC 15 15
MENUBUTTON "Cancel" 13 3
ENDFIELD

FIELD help
FLOC 54 15
MENUBUTTON "Help" 13 3
ENDFIELD

ENDTILE

ENDFORM
