FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 41 31
HEADER "Assign to Region"

TILE

TEXT "Enter new region name:"
TLOC 2 1
ENDTEXT

FIELD name
FLOC 2 3
STRFILLIN 37 100
ENDFIELD

TEXT "Or pick region to assign to shape(s):"
TLOC 2 6
ENDTEXT

FIELD grouplist
FLOC 2 8
LIST "" 38 9
OPTIONS alphanumsort
ENDFIELD

TEXT "Or"
TLOC 2 26
ENDTEXT

FIELD clear
FLOC 5 26
MENUBUTTON "Clear" 10 3
ENDFIELD

TEXT "present region assignment"
TLOC 16 26
ENDTEXT

FIELD done
FLOC 2 29
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 14 29
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 26 29
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
