FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 84 40
HEADER "Interdigital Filter"

POPUP <actlayers>"subclass""subclass".
POPUP <gndlayers1>"""".
POPUP <gndlayers2>"""".
POPUP <rotate>"0""0.000","45""45.000","90""90.000".
POPUP <secnum>"3""3","5""5","7""7","9""9","11""11","13""13","15""15","17""17".

TILE IFILTER

TABSET "tab"
OPTIONS tabsetDispatch
FLOC 1 1
FSIZE 30 42

TAB "Parameters"
TEXT "Ground 1:"
TLOC 1 1
ENDTEXT

FIELD ground1_subclass
FLOC 10 1
ENUMSET 15
POP "gndlayers1"
ENDFIELD

TEXT "Layer:"
TLOC 1 3
ENDTEXT

FIELD active_subclass
FLOC 10 3
ENUMSET 15
POP "actlayers"
ENDFIELD

TEXT "Ground 2:"
TLOC 1 5
ENDTEXT

FIELD ground2_subclass
FLOC 10 5
ENUMSET 15
POP "gndlayers2"
ENDFIELD

TEXT "Number of sections:"
TLOC 1 7
ENDTEXT

FIELD secnum
FLOC 1 9
ENUMSET 5
POP "secnum"
ENDFIELD

TEXT "Line Width:"
TLOC 1 11
ENDTEXT

FIELD swidth
FLOC 1 13
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel
TLOC 14 13
INFO swidthunits
ENDTEXT

TEXT "Line Length:"
TLOC 1 15
ENDTEXT

FIELD length
FLOC 1 17
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel
TLOC 14 17
INFO lengthunits
ENDTEXT

TEXT "Stub Width:"
TLOC 1 19
ENDTEXT

FIELD swidth1
FLOC 1 21
REALFILLIN 10 10
ENDFIELD

TEXT swidthunitslabel1
TLOC 14 21
INFO swidthunits1
ENDTEXT

TEXT "Stub Length:"
TLOC 1 23
ENDTEXT

FIELD length1
FLOC 1 25
REALFILLIN 10 10
ENDFIELD

TEXT lengthunitslabel1
TLOC 14 25
INFO lengthunits1
ENDTEXT

TEXT "Line Spacing:"
TLOC 1 27
ENDTEXT

FIELD spacing
FLOC 1 29
REALFILLIN 10 10
ENDFIELD

TEXT spacingunitslabel
TLOC 14 29
INFO spacingunits
ENDTEXT

TEXT "Stub Offset:"
TLOC 1 31
ENDTEXT

FIELD offsety
FLOC 1 33
REALFILLIN 10 10
ENDFIELD

TEXT offsetyunitslabel
TLOC 14 33
INFO offsetyunits
ENDTEXT

TEXT "Rotation Lock:"
TLOC 1 35
ENDTEXT

FIELD angle
FLOC 14 35
REALFILLIN 7 7
POP "rotate"
DECIMAL 3
ENDFIELD

FIELD newtrace
FLOC 1 37
CHECKLIST "New trace after insertion"
ENDFIELD

ENDTAB

TAB "Nets"
TEXT "Pin 1 Net:"
TLOC 1 1
ENDTEXT

TEXT
TLOC 10 1
INFO pin1netname 12
ENDTEXT

FIELD pin1netname_list
FLOC 24 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Pin 2 Net:"
TLOC 1 3
ENDTEXT

TEXT
TLOC 10 3
INFO pin2netname 12
ENDTEXT

FIELD pin2netname_list
FLOC 24 3
MENUBUTTON "..." 3 3
ENDFIELD

ENDTAB
ENDTABSET

FIELD onhelp
FLOC 1 44
MENUBUTTON "Help" 14 3
ENDFIELD

FIELD help
FLOC 22 44
MENUBUTTON "Help" 9 3
ENDFIELD

FIELD graphic
THUMBNAIL rfpcbifilter.bmp
OPTIONS STRETCH
FLOC 32 6
FSIZE 48 34
ENDFIELD
ENDTILE

ENDFORM

 