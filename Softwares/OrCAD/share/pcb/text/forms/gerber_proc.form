MIN 3
MAX 6
ENDFIELD

FIELD EOB
FLOC 6 36
FGROUP "End-of-Block char processing"
CHECKLIST "Change EOB character"
ENDFIELD

FIELD current_char
FLOC 25 39
FGROUP "End-of-Block char processing"
STRFILLIN 1 1
ENDFIELD

FIELD new_char
FLOC 25 42
FGROUP "End-of-Block char processing"
STRFILLIN 1 1
ENDFIELD

FIELD done
FLOC 1 46
MENUBUTTON "Done" 10 3
ENDFIELD

FIELD help
FLOC 12 46
MENUBUTTON "Help" 10 3
ENDFIELD

FIELD Execute
FLOC 23 46
MENUBUTTON "Execute" 10 3
ENDFIELD

FIELD cancel
FLOC 34 46
MENUBUTTON "Cancel" 10 3
ENDFIELD

GROUP "General Parameter"
GLOC 5 3
GSIZE 35 12
ENDGROUP

GROUP "Sequence number Processing"
GLOC 5 16
GSIZE 35 15
ENDGROUP

GROUP "End-of-Block char processing"
GLOC 5 32
GSIZE 35 13
ENDGROUP

ENDTILE

ENDFORM
