FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 46 26
HEADER "GERBER FILE PROCESSING"



ACTIVEFIELD "seq_form"

TILE
TEXT "Input File"
TLOC 6 8
TGROUP "General Parameter"
ENDTEXT

TEXT "Output File"
TLOC 6 11
TGROUP "General Parameter"
ENDTEXT

TEXT "Sequence format"
TLOC 10 28
TGROUP "Sequence number Processing"
ENDTEXT

TEXT "Current eob char"
TLOC 8 39
TGROUP "End-of-Block char processing"
ENDTEXT

TEXT "New eob char"
TLOC 8 42
TGROUP "End-of-Block char processing"
ENDTEXT

FIELD input_f
FLOC 18 8
FGROUP "General Parameter"
STRFILLIN 15 30
ENDFIELD

FIELD output_f
FLOC 18 11
FGROUP "General Parameter"
STRFILLIN 15 30
ENDFIELD

FIELD seq_proc
FLOC 6 19
FGROUP "Sequence number Processing"
CHECKLIST "Process sequence numbers"
ENDFIELD

FIELD add_seq
FLOC 10 22
FGROUP "Sequence number Processing"
CHECKLIST "Add sequence numbers" "seq_group"
ENDFIELD

FIELD del_seq
FLOC 10 25
FGROUP "Sequence number Processing"
CHECKLIST "Delete sequence numbers" "seq_group"
ENDFIELD

FIELD seq_form
FLOC 27 28
FGROUP "Sequence number Processing"
