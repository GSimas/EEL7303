FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 62 20
HEADER "IC Import from DEF"

TILE

TEXT "Current LEF Library: "
TLOC 3 1
ENDTEXT

TEXT ""
TLOC 18 1
INFO  curr_library 50
ENDTEXT


FIELD libPath
FLOC 40 1
MENUBUTTON "&Library Manager" 10 3
ENDFIELD

TEXT "DEF file:"
TLOC 3 4
ENDTEXT

FIELD FilePath
FLOC 43 4
MENUBUTTON "Browse..." 10 3
ENDFIELD


FIELD def_filename
FLOC 10 4
STRFILLIN 30 255
ENDFIELD


GROUP "IC Placement"
FLOC 3 7
FSIZE 22 10
ENDGROUP


FIELD center
FLOC 4 10
CHECKLIST "IC center at:" "loc"
ENDFIELD

FIELD lower_left
FLOC 4 12
CHECKLIST "Lower left corner at:" "loc"
ENDFIELD

FIELD origin_def
FLOC 4 14
CHECKLIST "As defined in DEF" "loc"
ENDFIELD

GROUP "Location"
FLOC 33 7
FSIZE 25 10
ENDGROUP

TEXT "X coordinate:"
TLOC 35 10
ENDTEXT

FIELD def_XOrigin
FLOC 46 10
STRFILLIN 8 12
ENDFIELD

TEXT "Y coordinate:"
TLOC 35 13
ENDTEXT

FIELD def_YOrigin
FLOC 46 13
STRFILLIN 8 12
ENDFIELD


FIELD import
FLOC 23 20
MENUBUTTON "&Import" 10 3
ENDFIELD
 
FIELD cancel
FLOC 35 20
MENUBUTTON "&Cancel" 10 3
ENDFIELD

FIELD help
FLOC 47 20
MENUBUTTON "&Help" 10 3
ENDFIELD
 
ENDTILE

ENDFORM
