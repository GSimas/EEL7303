FILE_TYPE=FORM_DEFN VERSION=2 
FORM
FIXED
PORT 50 12
HEADER "RF IFF Import"

MESSAGE warning 2 "%s"
POPUP <importmode>"""".

ACTIVEFIELD "iff_filename"
TILE

TEXT "Layout IFF file"
TLOC 3 1
ENDTEXT

FIELD layout_filename
FLOC 17 1
STRFILLIN 28 512
ENDFIELD 

FIELD layout_browse
FLOC 47 1
MENUBUTTON "..." 3 3
ENDFIELD

FIELD importschematic
FLOC 1 4
CHECKLIST "Schematic IFF file"
ENDFIELD

FIELD schematic_filename
FLOC 17 4
STRFILLIN 28 512
ENDFIELD 

FIELD schematic_browse
FLOC 47 4
MENUBUTTON "..." 3 3
ENDFIELD

FIELD importstackup
FLOC 1 7
CHECKLIST "Import Stackup"
ENDFIELD

FIELD stackup_filename
FLOC 17 7
STRFILLIN 28 512
ENDFIELD 

FIELD stackup_browse
FLOC 47 7
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Import Mode:"
TLOC 1 10
ENDTEXT

FIELD importmode
FLOC 17 10
ENUMSET 12
POP "importmode"
ENDFIELD

FIELD copystackup
FLOC 32 10
CHECKLIST "Copy current stackup"
ENDFIELD

FIELD done
FLOC 1 13
MENUBUTTON "Done" 10 3
ENDFIELD 
 
FIELD cancel
FLOC 13 13
MENUBUTTON "Cancel" 10 3
ENDFIELD 

FIELD edit_lmap
FLOC 26 13
MENUBUTTON "Layer map..." 10 3
ENDFIELD  

FIELD help
FLOC 40 13
MENUBUTTON "Help" 10 3
ENDFIELD 

ENDTILE
ENDFORM


