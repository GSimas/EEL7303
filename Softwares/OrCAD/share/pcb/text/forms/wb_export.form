FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 38 40
HEADER "Wire Bond Export"

POPUP <die_popup> """".

TILE

TEXT "File name:"
TLOC 1 1
ENDTEXT

FIELD filename
FLOC 11 1
STRFILLIN 20 128
ENDFIELD 

FIELD browse
FLOC 33 1
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Parent die:"
TLOC 1 4
ENDTEXT

FIELD die
FLOC 11 4
ENUMSET 19
POP "die_popup"
ENDFIELD

FIELD show_diestacks
FLOC 11 6
CHECKLIST "Show die stacks"
ENDFIELD


GROUP "Export details"
GLOC 1 8
GSIZE 35 10
ENDGROUP

FIELD paths
FLOC 3 10
CHECKLIST "Guide paths"
ENDFIELD

FIELD wires
FLOC 24 10
CHECKLIST "Bond wires"
ENDFIELD

FIELD placement
FLOC 3 12
CHECKLIST "Bond finger placement"
ENDFIELD

FIELD definition
FLOC 3 14
CHECKLIST "Bond finger definition"
ENDFIELD

FIELD rings
FLOC 3 16
CHECKLIST "Ring boundaries"
ENDFIELD

FIELD export
FLOC 1 20
MENUBUTTON "Export" 9 3
ENDFIELD

FIELD close
FLOC 14 20
MENUBUTTON "Close" 9 3
ENDFIELD

FIELD help
FLOC 27 20
MENUBUTTON "Help" 9 3
ENDFIELD

ENDTILE
ENDFORM
