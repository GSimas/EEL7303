FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 71 40

HEADER "Select RDL Routing Parameters"

ACTIVEFIELD "execute"

TILE

TEXT "Via Size" "rdlViaLabel"
TLOC 2 1
ENDTEXT 

FIELD setup_rdlViaSize
FLOC 15 1
REALFILLIN 8 8
ENDFIELD

FIELD setup_rdlViaShapeSquare
FLOC 27 1
CHECKLIST "Square"  "setup_rdlViaShape"
ENDFIELD

FIELD setup_rdlViaShapeRound
FLOC 40 1
CHECKLIST "Round"  "setup_rdlViaShape"
ENDFIELD

TEXT "RDL Routing Constraints"
TLOC 2 5
ENDTEXT 

FIELD setup_rdlUseExistingConstraints
FLOC 27 5
CHECKLIST "Use Existing Values on each layer"  "setup_rdlSpaceTrace"
ENDFIELD

FIELD setup_rdlSetNewConstraints
FLOC 27 7
CHECKLIST "Set New Values for all layers"  "setup_rdlSpaceTrace"
ENDFIELD



GROUP "New RDL Values"
GLOC 1 10
GSIZE 56 7
ENDGROUP

TEXT "Line Size" "rdlNewConstraintLabel1"
TLOC 2 12
ENDTEXT 

FIELD setup_rdlLineSize
FLOC 15 12
REALFILLIN 8 8
ENDFIELD

TEXT "Line-Line Space" "rdlNewConstraintLabel2"
TLOC 27 12
ENDTEXT 

FIELD setup_rdlLineLineSpace
FLOC 40 12
REALFILLIN 8 8
ENDFIELD

TEXT "Line-Pad Space" "rdlNewConstraintLabel3" 
TLOC 2 14
ENDTEXT 

FIELD setup_rdlLinePadSpace
FLOC 15 14
REALFILLIN 8 8
ENDFIELD

TEXT "Pad-Pad Space" "rdlNewConstraintLabel4" 
TLOC 27 14
ENDTEXT 

FIELD setup_rdlPadPadSpace
FLOC 40 14
REALFILLIN 8 8
ENDFIELD


FIELD close
FLOC 60 3
MENUBUTTON "Cancel" 11 3
ENDFIELD

FIELD execute
FLOC 60 6
MENUBUTTON "Next" 11 4
ENDFIELD


FIELD help
FLOC 60 10
MENUBUTTON "Help" 11 3
ENDFIELD

ENDTILE
ENDFORM
