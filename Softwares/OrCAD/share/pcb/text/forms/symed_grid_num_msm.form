FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 39 6
HEADER "Route_Layer"

POPUP <START_POPUP> "Top Left" "Top Left".
POPUP <SCHEME_POPUP> "Customized" "Customized".

TILE

TEXT "Name:"
TLOC 1 1
ENDTEXT
FIELD grid_name
FLOC 8 1
STRFILLIN 19 256
ENDFIELD

TEXT "Priority:"
TLOC 1 3
ENDTEXT
FIELD grid_priority
FLOC 8 3
STRFILLIN 19 20
ENDFIELD

TEXT "Scheme:"
TLOC 1 7
ENDTEXT
FIELD grid_scheme
FLOC 8 7
ENUMSET 18
POP "SCHEME_POPUP"
ENDFIELD

TEXT "First pin:"
TLOC 1 9
ENDTEXT
FIELD grid_start
FLOC 8 9
ENUMSET 18
POP "START_POPUP"
ENDFIELD

TEXT "Prefix:"
TLOC 1 11
ENDTEXT
FIELD grid_prefix
FLOC 8 11
STRFILLIN 19 13
ENDFIELD

TEXT "Start at:"
TLOC 1 13
ENDTEXT
FIELD grid_pin_num_start
FLOC 8 13
STRFILLIN 19 256
ENDFIELD

FIELD grid_pad_letters
FLOC 1 15
CHECKLIST "Pad letters (As)"
ENDFIELD

FIELD grid_pad_numbers
FLOC 1 17
CHECKLIST "Pad numbers (0s)"
ENDFIELD

FIELD grid_letter_first
FLOC 1 19
CHECKLIST "Label with letters before numbers"
ENDFIELD

FIELD grid_jedec
FLOC 1 21
CHECKLIST "Omit letters per JEDEC standard"
ENDFIELD

FIELD grid_noskip_spaces
FLOC 1 23
CHECKLIST "Label unused grid positions"
ENDFIELD

FIELD grid_noskip_stagger
FLOC 1 25
CHECKLIST "Label non-staggered positions"
ENDFIELD

FIELD grid_apply
FLOC 1 28
MENUBUTTON "Apply changes" 20 3
ENDFIELD

ENDTILE

ENDFORM

