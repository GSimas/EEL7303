FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 66 6
HEADER "NC Route"

MESSAGE nerror 0 "A problem occurred - please check the logfile (using Viewlog)."
MESSAGE ndone 0 "NCRoute complete."
MESSAGE ndemo 0 "Cannot Run in Demo Mode."

TILE

TEXT "File name:"
TLOC 3 2
ENDTEXT

FIELD filename
FLOC 16 2
STRFILLIN 18 128
ENDFIELD

FIELD browse
FLOC 37 2
MENUBUTTON "..." 3 3
ENDFIELD

TEXT "Route feedrate:"
TLOC 3 4
ENDTEXT

FIELD feed_rate
FLOC 16 4
INTFILLIN 3 3
MIN 1
ENDFIELD

FIELD separate_files
FLOC 3 7 
CHECKLIST "Separate files for plated/non-plated routing"
ENDFIELD

FIELD execute
FLOC 50 2
MENUBUTTON "Route" 16 3
ENDFIELD

FIELD parameters
FLOC 50 5
MENUBUTTON "NC Parameters..." 16 3
ENDFIELD

FIELD close
FLOC 50 8
MENUBUTTON "Close" 16 3
ENDFIELD

FIELD cancel
FLOC 50 11
MENUBUTTON "Cancel" 16 3
ENDFIELD

FIELD viewlog
FLOC 50 14
MENUBUTTON "View Log" 16 3
ENDFIELD

FIELD help
FLOC 50 17
MENUBUTTON "Help" 16 3
ENDFIELD

ENDTILE
ENDFORM
