FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 56 16
HEADER  "Auto Create I/O Pad for the Connection Point Pin" 

TILE


TEXT "Macro pin:"
TLOC 2 1
ENDTEXT

TEXT ""
TLOC 11 1
INFO pinName 40
ENDTEXT


FIELD padDefined
FLOC 34 1
CHECKLIST "Do not create an I/O pad"
ENDFIELD


GROUP "I/O pad dimensions"
FLOC 2 4
FSIZE 53 6
ENDGROUP


TEXT "Width:"
TLOC 4 7
ENDTEXT

FIELD pad_width
FLOC 11 7
REALFILLIN 13 17
ENDFIELD

TEXT "Height:"
TLOC 31 7
ENDTEXT

FIELD pad_height
FLOC 38 7
REALFILLIN 13 17
ENDFIELD

GROUP "Offset of the I/O pad from the connection point"
FLOC 2 12
FSIZE 53 6
ENDGROUP

TEXT "Offset X:"
TLOC 4 15
ENDTEXT

FIELD x_offset
FLOC 11 15
REALFILLIN 13 17
ENDFIELD

TEXT "Offset Y:"
TLOC 31 15
ENDTEXT

FIELD y_offset
FLOC 38 15 
REALFILLIN 13 17
ENDFIELD

FIELD OK
FLOC 22 21
MENUBUTTON "&OK" 10 3
ENDFIELD


FIELD Cancel
FLOC 33 21
MENUBUTTON "&Cancel" 10 3
ENDFIELD


FIELD Help
FLOC 45 21
MENUBUTTON "&Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
