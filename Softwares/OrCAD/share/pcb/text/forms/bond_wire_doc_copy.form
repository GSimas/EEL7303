FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 80 20
HEADER "Bond Wire Documentation Prep"

POPUP <LAYER_POPUP> """".
POPUP <START_POPUP> """".
POPUP <END_POPUP> """".
POPUP <PROFILE_POPUP> """".
POPUP <OP_POPUP> "Add""ADD", "Delete" "DELETE".

TILE

TEXT "Last updated on:"
TLOC 1 1
ENDTEXT

TEXT "Manufacturing output layer mappings:"
TLOC 1 3
ENDTEXT

TEXT "Last updated on"
TLOC 30 1
OPTIONS bold
INFO last_updated 40
ENDTEXT

TEXT "0 unmapped wires"
TLOC 30 3
OPTIONS bold
INFO unused_count 40
ENDTEXT

GRID map_grid
FLOC 1 6
FSIZE  78 19
OPTIONS VLINES HLINES
GHEAD TOP 
HEADSIZE 2
OPTIONS 3D
ENDGHEAD
GHEAD SIDE 
HEADSIZE 2
OPTIONS 3D NUMBER
POP "OP_POPUP"
ENDGHEAD
ENDGRID

FIELD delete_existing
FLOC 1 25
CHECKLIST "Delete existing lines on mapped layers"
ENDFIELD

FIELD auto_map
FLOC 48 25
MENUBUTTON "Auto generate all in-use combinations" 30 3
ENDFIELD

FIELD generate
FLOC 1 28
MENUBUTTON "Generate" 10 3
ENDFIELD

FIELD close
FLOC 12 28
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD help
FLOC 68 28
MENUBUTTON "Help" 10 3
ENDFIELD

FLEXMODE edgegravity
FLEX map_grid 0 0 1 1

ENDTILE

ENDFORM
